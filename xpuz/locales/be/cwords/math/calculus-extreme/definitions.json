{
    "\u0442\u044d\u0441\u0442 \u0410\u0431\u0435\u043b\u044f": "\u041c\u0435\u0442\u0430\u0434 \u043f\u0440\u0430\u0432\u0435\u0440\u043a\u0456 \u0437\u0431\u0435\u0436\u043d\u0430\u0441\u0446\u0456 \u0431\u044f\u0441\u043a\u043e\u043d\u0446\u0430\u0433\u0430 \u0448\u044d\u0440\u0430\u0433\u0443",
    "\u0430\u0431\u0441\u0430\u043b\u044e\u0442\u043d\u0430\u044f \u043a\u0430\u043d\u0432\u0435\u0440\u0433\u0435\u043d\u0446\u044b\u044f": "\u041a\u0430\u0436\u0443\u0446\u044c, \u0448\u0442\u043e \u0431\u044f\u0441\u043a\u043e\u043d\u0446\u044b \u0448\u044d\u0440\u0430\u0433 \u0437\u0431\u043b\u0456\u0436\u0430\u0435\u0446\u0446\u0430 \u0430\u0431\u0441\u0430\u043b\u044e\u0442\u043d\u0430, \u043a\u0430\u043b\u0456 \u0441\u0443\u043c\u0430 \u0430\u0431\u0441\u0430\u043b\u044e\u0442\u043d\u044b\u0445 \u0437\u043d\u0430\u0447\u044d\u043d\u043d\u044f\u045e \u0441\u043a\u043b\u0430\u0434\u0430\u0435\u043c\u044b\u0445 \u043a\u0430\u043d\u0435\u0447\u043d\u0430\u044f",
    "\u0430\u0431\u0441\u0430\u043b\u044e\u0442\u043d\u044b \u043c\u0430\u043a\u0441\u0456\u043c\u0443\u043c": "\u041d\u0430\u0439\u0431\u043e\u043b\u044c\u0448\u0430\u0435 \u0437\u043d\u0430\u0447\u044d\u043d\u043d\u0435, \u044f\u043a\u043e\u0435 \u0434\u0430\u0441\u044f\u0433\u0430\u0435 \u0444\u0443\u043d\u043a\u0446\u044b\u044f",
    "\u0430\u0431\u0441\u0430\u043b\u044e\u0442\u043d\u044b \u043c\u0456\u043d\u0456\u043c\u0443\u043c": "\u0421\u0430\u043c\u0430\u0435 \u043d\u0456\u0437\u043a\u0430\u0435 \u0437\u043d\u0430\u0447\u044d\u043d\u043d\u0435, \u044f\u043a\u043e\u0435 \u0434\u0430\u0441\u044f\u0433\u0430\u0435 \u0444\u0443\u043d\u043a\u0446\u044b\u044f",
    "\u0430\u0431\u0441\u0430\u043b\u044e\u0442\u043d\u0430\u044f \u0432\u0435\u043b\u0456\u0447\u044b\u043d\u044f": "\u041d\u0435\u0430\u0434\u043c\u043e\u045e\u043d\u0430\u0435 \u0437\u043d\u0430\u0447\u044d\u043d\u043d\u0435 \u0440\u044d\u0447\u0430\u0456\u0441\u043d\u0430\u0433\u0430 \u043b\u0456\u043a\u0443 \u0431\u0435\u0437 \u0443\u043b\u0456\u043a\u0443 \u044f\u0433\u043e \u0437\u043d\u0430\u043a\u0430",
    "\u0447\u0430\u0440\u0433\u0430\u0432\u0430\u043d\u043d\u0435 \u0448\u044d\u0440\u0430\u0433\u0430\u045e": "\u0411\u044f\u0441\u043a\u043e\u043d\u0446\u044b \u0448\u044d\u0440\u0430\u0433, \u0447\u043b\u0435\u043d\u044b \u044f\u043a\u043e\u0433\u0430 \u0447\u0430\u0440\u0433\u0443\u044e\u0446\u0446\u0430 \u043f\u0430\u043c\u0456\u0436 \u0441\u0442\u0430\u043d\u043e\u045e\u0447\u044b\u043c\u0456 \u0456 \u0430\u0434\u043c\u043e\u045e\u043d\u044b\u043c\u0456",
    "\u0442\u044d\u0441\u0442 \u0437 \u0447\u0430\u0440\u0433\u0430\u0432\u0430\u043d\u043d\u0435\u043c \u0441\u0435\u0440\u044b\u0439": "\u041c\u0435\u0442\u0430\u0434, \u044f\u043a\u0456 \u0432\u044b\u043a\u0430\u0440\u044b\u0441\u0442\u043e\u045e\u0432\u0430\u0435\u0446\u0446\u0430 \u0434\u043b\u044f \u0434\u043e\u043a\u0430\u0437\u0443 \u0442\u0430\u0433\u043e, \u0448\u0442\u043e \u043f\u0435\u0440\u0430\u043c\u0435\u043d\u043d\u044b \u0448\u044d\u0440\u0430\u0433 \u0437 \u0447\u043b\u0435\u043d\u0430\u043c\u0456, \u044f\u043a\u0456\u044f \u043f\u0430\u043c\u044f\u043d\u0448\u0430\u044e\u0446\u0446\u0430 \u043f\u0430 \u0430\u0431\u0441\u0430\u043b\u044e\u0442\u043d\u044b\u043c \u0437\u043d\u0430\u0447\u044d\u043d\u043d\u0456, \u0437'\u044f\u045e\u043b\u044f\u0435\u0446\u0446\u0430 \u0437\u0431\u0435\u0436\u043d\u044b\u043c",
    "\u043a\u0430\u043b\u044c\u0446\u0430\u0432\u043e\u0435 \u043f\u0440\u0430\u0441\u0442\u043e\u0440\u0443": "\u0410\u0431'\u0435\u043a\u0442 \u0443 \u0444\u043e\u0440\u043c\u0435 \u043a\u043e\u043b\u044c\u0446\u0430, \u0432\u043e\u0431\u043b\u0430\u0441\u0446\u044c, \u0430\u0431\u043c\u0435\u0436\u0430\u0432\u0430\u043d\u0430\u044f \u0434\u0432\u0443\u043c\u0430 \u043a\u0430\u043d\u0446\u044d\u043d\u0442\u0440\u044b\u0447\u043d\u044b\u043c\u0456 \u0430\u043a\u0440\u0443\u0436\u043d\u0430\u0441\u0446\u044f\u043c\u0456",
    "\u043f\u0435\u0440\u0448\u0430\u0432\u044b\u0442\u0432\u043e\u0440\u043d\u0430\u0435": "\u0414\u044b\u0444\u0435\u0440\u044d\u043d\u0446\u0443\u0435\u043c\u0430\u044f \u0444\u0443\u043d\u043a\u0446\u044b\u044f, \u0432\u044b\u0442\u0432\u043e\u0440\u043d\u0430\u044f \u044f\u043a\u043e\u0439 \u0440\u043e\u045e\u043d\u0430\u044f \u0437\u044b\u0445\u043e\u0434\u043d\u0430\u0439 \u0444\u0443\u043d\u043a\u0446\u044b\u0456",
    "\u0430\u0441\u0456\u043c\u043f\u0442\u043e\u0442\u0430": "\u041b\u0456\u043d\u0456\u044f, \u0442\u0430\u043a\u0430\u044f \u0448\u0442\u043e \u0430\u0434\u043b\u0435\u0433\u043b\u0430\u0441\u0446\u044c \u043f\u0430\u043c\u0456\u0436 \u043a\u0440\u044b\u0432\u043e\u0439 \u0456 \u043b\u0456\u043d\u0456\u044f\u0439 \u043d\u0430\u0431\u043b\u0456\u0436\u0430\u0435\u0446\u0446\u0430 \u0434\u0430 \u043d\u0443\u043b\u044f, \u043a\u0430\u043b\u0456 \u0430\u0434\u043d\u0430 \u0430\u0431\u043e \u0430\u0431\u0435\u0434\u0437\u0432\u0435 \u043a\u0430\u0430\u0440\u0434\u044b\u043d\u0430\u0442\u044b x \u0430\u0431\u043e y \u0456\u043c\u043a\u043d\u0443\u0446\u0446\u0430 \u0434\u0430 \u0431\u044f\u0441\u043a\u043e\u043d\u0446\u0430\u0441\u0446\u0456",
    "\u043f\u043b\u043e\u0448\u0447\u0430 \u043f\u0430\u0434 \u043a\u0440\u044b\u0432\u043e\u0439": "\u0406\u043d\u0442\u044d\u0433\u0440\u0430\u043b, \u044f\u043a\u0456 \u043f\u0440\u0430\u0434\u0441\u0442\u0430\u045e\u043b\u044f\u0435 \u043f\u043b\u043e\u0448\u0447\u0443 \u043f\u0430\u043c\u0456\u0436 \u043a\u0440\u044b\u0432\u043e\u0439 y=f(x), \u0432\u043e\u0441\u0441\u044e x \u0456 \u0432\u0435\u0440\u0442\u044b\u043a\u0430\u043b\u044c\u043d\u044b\u043c\u0456 \u043b\u0456\u043d\u0456\u044f\u043c\u0456 x=a \u0456 x=b, \u0434\u0437\u0435 a \u0456 b \u2014 \u043c\u0435\u0436\u044b \u0456\u043d\u0442\u044d\u0433\u0440\u0430\u0432\u0430\u043d\u043d\u044f",
    "\u043f\u0440\u0430\u0432\u0456\u043b\u0430 \u043b\u0430\u043d\u0446\u0443\u0433\u0430": "\u0412\u044b\u0442\u0432\u043e\u0440\u043d\u0430\u044f \u0441\u0430\u0441\u0442\u0430\u045e\u043d\u043e\u0439 \u0444\u0443\u043d\u043a\u0446\u044b\u0456 - \u0433\u044d\u0442\u0430 \u0432\u044b\u0442\u0432\u043e\u0440\u043d\u0430\u044f \u0437\u043d\u0435\u0448\u043d\u044f\u0439 \u0444\u0443\u043d\u043a\u0446\u044b\u0456, \u044f\u043a\u0430\u044f \u0432\u044b\u043b\u0456\u0447\u0432\u0430\u0435\u0446\u0446\u0430 \u044f\u043a \u0443\u043d\u0443\u0442\u0440\u0430\u043d\u0430\u044f \u0444\u0443\u043d\u043a\u0446\u044b\u044f, \u043f\u0430\u043c\u043d\u043e\u0436\u0430\u043d\u0430\u044f \u043d\u0430 \u0432\u044b\u0442\u0432\u043e\u0440\u043d\u0443\u044e \u045e\u043d\u0443\u0442\u0440\u0430\u043d\u0430\u0439 \u0444\u0443\u043d\u043a\u0446\u044b\u0456",
    "\u0434\u044b\u0444\u0435\u0440\u044d\u043d\u0446\u044b\u044f\u043b\u044c\u043d\u0430\u0435 \u0432\u044b\u043b\u0456\u0447\u044d\u043d\u043d\u0435": "\u0412\u044b\u0432\u0443\u0447\u0430\u0435 \u0445\u0443\u0442\u043a\u0430\u0441\u0446\u044c \u0437\u043c\u0435\u043d\u044b \u0432\u0435\u043b\u0456\u0447\u044b\u043d\u044c. \u0412\u044b\u0442\u0432\u043e\u0440\u043d\u0430\u0435 \u045e\u0440\u0430\u045e\u043d\u0435\u043d\u043d\u0435 \u0432\u044b\u043a\u0430\u0440\u044b\u0441\u0442\u043e\u045e\u0432\u0430\u0435\u0446\u0446\u0430 \u0434\u043b\u044f \u0430\u043f\u0456\u0441\u0430\u043d\u043d\u044f \u0445\u0443\u0442\u043a\u0430\u0441\u0446\u0456 \u0437\u043c\u0435\u043d\u044b \u0444\u0443\u043d\u043a\u0446\u044b\u0456",
    "\u043d\u0435\u0437\u0430\u043b\u0435\u0436\u043d\u0430\u044f \u043f\u0435\u0440\u0430\u043c\u0435\u043d\u043d\u0430\u044f": "\u0412\u0435\u043b\u0456\u0447\u044b\u043d\u044f, \u044f\u043a\u043e\u0439 \u043c\u0430\u043d\u0456\u043f\u0443\u043b\u044e\u044e\u0446\u044c \u0443 \u0444\u0443\u043d\u043a\u0446\u044b\u0456 \u0430\u0431\u043e \u045e\u0440\u0430\u045e\u043d\u0435\u043d\u043d\u0456, \u0447\u0430\u0441\u0442\u0430 \u043f\u0440\u0430\u0434\u0441\u0442\u0430\u045e\u043b\u044f\u044e\u0447\u044b \u043f\u0440\u044b\u0447\u044b\u043d\u0443 \u045e \u043f\u0440\u044b\u0447\u044b\u043d\u043d\u0430-\u0432\u044b\u043d\u0456\u043a\u043e\u0432\u0430\u0439 \u0441\u0443\u0432\u044f\u0437\u0456",
    "\u0437\u0430\u043b\u0435\u0436\u043d\u0430\u044f \u0437\u043c\u0435\u043d\u043d\u0430\u044f": "\u0412\u044b\u043d\u0456\u043a \u0430\u0431\u043e \u0432\u044b\u043d\u0456\u043a \u0443 \u0444\u0443\u043d\u043a\u0446\u044b\u0456 \u0430\u0431\u043e \u045e\u0437\u0430\u0435\u043c\u0430\u0441\u0443\u0432\u044f\u0437\u0456, \u044f\u043a\u0456\u044f \u0437\u043c\u044f\u043d\u044f\u044e\u0446\u0446\u0430 \u045e \u0437\u0430\u043b\u0435\u0436\u043d\u0430\u0441\u0446\u0456 \u0430\u0434 \u0437\u043d\u0430\u0447\u044d\u043d\u043d\u044f\u045e \u0456\u043d\u0448\u044b\u0445 \u0432\u0435\u043b\u0456\u0447\u044b\u043d\u044c",
    "\u0434\u0430\u043c\u0435\u043d": "\u0410\u0434\u043d\u043e\u0441\u0456\u0446\u0446\u0430 \u0434\u0430 \u045e\u0441\u0456\u0445 \u043c\u0430\u0433\u0447\u044b\u043c\u044b\u0445 \u0437\u043d\u0430\u0447\u044d\u043d\u043d\u044f\u045e, \u044f\u043a\u0456\u044f \u043c\u043e\u0436\u043d\u0430 \u0432\u044b\u043a\u0430\u0440\u044b\u0441\u0442\u043e\u045e\u0432\u0430\u0446\u044c \u0443 \u044f\u043a\u0430\u0441\u0446\u0456 \u045e\u0432\u0430\u0445\u043e\u0434\u043d\u044b\u0445 \u0434\u0430\u0434\u0437\u0435\u043d\u044b\u0445 \u0434\u043b\u044f \u0444\u0443\u043d\u043a\u0446\u044b\u0456",
    "\u0434\u044b\u044f\u043f\u0430\u0437\u043e\u043d": "\u041d\u0430\u0431\u043e\u0440 \u0443\u0441\u0456\u0445 \u043c\u0430\u0433\u0447\u044b\u043c\u044b\u0445 \u0432\u044b\u043d\u0456\u043a\u0430\u045e, \u044f\u043a\u0456\u044f \u043c\u043e\u0436\u0430 \u0432\u044b\u0440\u0430\u0431\u0456\u0446\u044c \u0444\u0443\u043d\u043a\u0446\u044b\u044f",
    "\u043c\u044f\u0436\u0430": "\u041a\u0430\u043d\u0446\u044d\u043f\u0446\u044b\u044f, \u044f\u043a\u0430\u044f \u0430\u043f\u0456\u0441\u0432\u0430\u0435, \u0434\u0430 \u044f\u043a\u0456\u0445 \u0432\u044b\u0445\u0430\u0434\u043d\u044b\u0445 \u0437\u043d\u0430\u0447\u044d\u043d\u043d\u044f\u045e \u043d\u0430\u0431\u043b\u0456\u0436\u0430\u0435\u0446\u0446\u0430 \u0444\u0443\u043d\u043a\u0446\u044b\u044f \u043f\u0430 \u043c\u0435\u0440\u044b \u043d\u0430\u0431\u043b\u0456\u0436\u044d\u043d\u043d\u044f \u045e\u0432\u0430\u0445\u043e\u0434\u043d\u044b\u0445 \u0437\u043d\u0430\u0447\u044d\u043d\u043d\u044f\u045e \u0434\u0430 \u043f\u044d\u045e\u043d\u0430\u0433\u0430 \u0437\u043d\u0430\u0447\u044d\u043d\u043d\u044f",
    "\u0432\u044b\u0442\u0432\u043e\u0440\u043d\u044b\u044f": "\u041a\u0430\u043d\u0446\u044d\u043f\u0446\u044b\u044f, \u044f\u043a\u0430\u044f \u0432\u044b\u043c\u044f\u0440\u0430\u0435 \u0456\u043c\u0433\u043d\u0435\u043d\u043d\u0443\u044e \u0445\u0443\u0442\u043a\u0430\u0441\u0446\u044c \u0437\u043c\u0435\u043d\u044b \u0430\u0431\u043e \u043d\u0430\u0445\u0456\u043b \u0434\u0430\u0442\u044b\u0447\u043d\u0430\u0439 \u043b\u0456\u043d\u0456\u0456 \u045e \u043f\u044d\u045e\u043d\u0430\u0439 \u043a\u0440\u043e\u043f\u0446\u044b \u0444\u0443\u043d\u043a\u0446\u044b\u0456",
    "\u0456\u043d\u0442\u044d\u0433\u0440\u0430\u043b\u044c\u043d\u0430\u0435 \u0432\u044b\u043b\u0456\u0447\u044d\u043d\u043d\u0435": "\u041f\u0430\u0448\u044b\u0440\u044d\u043d\u043d\u0435 \u0432\u044b\u043b\u0456\u0447\u044d\u043d\u043d\u044f, \u044f\u043a\u043e\u0435 \u043f\u0440\u0430\u0434\u0443\u0433\u043b\u0435\u0434\u0436\u0432\u0430\u0435 \u0437\u043d\u0430\u0445\u043e\u0434\u0436\u0430\u043d\u043d\u0435 \u043f\u0435\u0440\u0448\u0430\u0432\u044b\u0442\u0432\u043e\u0440\u043d\u0430\u0439 \u0444\u0443\u043d\u043a\u0446\u044b\u0456, \u0432\u044b\u0442\u0432\u043e\u0440\u043d\u0430\u044f \u044f\u043a\u043e\u0439 \u0437\u0430\u0434\u0430\u0434\u0437\u0435\u043d\u0430",
    "\u043b\u0456\u043d\u0435\u0439\u043d\u044b \u0456\u043d\u0442\u044d\u0433\u0440\u0430\u043b": "\u0406\u043d\u0442\u044d\u0433\u0440\u0430\u0432\u0430\u043d\u043d\u0435 \u0444\u0443\u043d\u043a\u0446\u044b\u0456 \u043f\u0430 \u043b\u0456\u043d\u0456\u0456 \u043a\u0440\u044b\u0432\u043e\u0439",
    "\u043f\u0430\u0432\u044f\u0440\u0445\u043e\u045e\u043d\u044b \u0456\u043d\u0442\u044d\u0433\u0440\u0430\u043b": "\u0410\u0431\u0430\u0433\u0443\u043b\u044c\u043d\u0435\u043d\u043d\u0435 \u0440\u043e\u0437\u043d\u044b\u0445 \u0456\u043d\u0442\u044d\u0433\u0440\u0430\u043b\u0430\u045e \u0434\u0430 \u0456\u043d\u0442\u044d\u0433\u0440\u0430\u0432\u0430\u043d\u043d\u044f \u043f\u0430 \u043f\u0430\u0432\u0435\u0440\u0445\u043d\u044f\u0445",
    "\u0448\u043c\u0430\u0442\u043c\u0435\u0440\u043d\u0430\u0433\u0430 \u0432\u044b\u043b\u0456\u0447\u044d\u043d\u043d\u044f": "\u041f\u0430\u0448\u044b\u0440\u044d\u043d\u043d\u0435 \u0432\u044b\u043b\u0456\u0447\u044d\u043d\u043d\u044f \u0430\u0434\u043d\u043e\u0439 \u0437\u043c\u0435\u043d\u043d\u0430\u0439 \u043d\u0430 \u0432\u044b\u043b\u0456\u0447\u044d\u043d\u043d\u0435 \u0437 \u0444\u0443\u043d\u043a\u0446\u044b\u044f\u043c\u0456 \u043d\u0435\u043a\u0430\u043b\u044c\u043a\u0456\u0445 \u0437\u043c\u0435\u043d\u043d\u044b\u0445",
    "\u0432\u0435\u043a\u0442\u0430\u0440\u043d\u0430\u0435 \u043f\u043e\u043b\u0435": "\u041a\u0440\u043e\u043f\u043a\u0430 \u045e \u043f\u0440\u0430\u0441\u0442\u043e\u0440\u044b, \u044f\u043a\u0430\u044f \u043c\u0430\u0435 \u0432\u0435\u043b\u0456\u0447\u044b\u043d\u044e \u0456 \u043a\u0456\u0440\u0443\u043d\u0430\u043a",
    "\u0432\u0435\u043a\u0442\u0430\u0440\u043d\u0430\u0435 \u0432\u044b\u043b\u0456\u0447\u044d\u043d\u043d\u0435": "\u041f\u0430\u0448\u044b\u0440\u044d\u043d\u043d\u0435 \u0432\u044b\u043b\u0456\u0447\u044d\u043d\u043d\u044f, \u044f\u043a\u043e\u0435 \u0437\u0430\u0439\u043c\u0430\u0435\u0446\u0446\u0430 \u0434\u044b\u0444\u0435\u0440\u044d\u043d\u0446\u044b\u044f\u0446\u044b\u044f\u0439 \u0456 \u0456\u043d\u0442\u044d\u0433\u0440\u0430\u0446\u044b\u044f\u0439 \u0432\u0435\u043a\u0442\u0430\u0440\u043d\u0430\u0433\u0430 \u043f\u043e\u043b\u044f \u045e \u0442\u0440\u043e\u0445\u043c\u0435\u0440\u043d\u0430\u0439 \u044d\u045e\u043a\u043b\u0456\u0434\u0430\u0432\u0430\u0439 \u043f\u0440\u0430\u0441\u0442\u043e\u0440\u044b",
    "\u0432\u0435\u043a\u0442\u0430\u0440\u043d\u044b \u043f\u0430\u0442\u044d\u043d\u0446\u044b\u044f\u043b": "\u0412\u0435\u043a\u0442\u0430\u0440\u043d\u0430\u0435 \u043f\u043e\u043b\u0435, \u044f\u043a\u043e\u0435 \u0441\u0442\u0432\u0430\u0440\u0430\u0435 \u0441\u0430\u043b\u0435\u043d\u043e\u0456\u0434\u0430\u043b\u044c\u043d\u0430\u0435 \u043f\u043e\u043b\u0435 \u043f\u0440\u0430\u0437 \u0430\u043f\u0435\u0440\u0430\u0442\u0430\u0440 \u0437\u0430\u0432\u0456\u0442\u043e\u043a"
}