    "\u5668\u5b98": "\u7279\u5b9a\u306e\u6a5f\u80fd\u3092\u5b9f\u884c\u3059\u308b\u305f\u3081\u306b\u9023\u643a\u3059\u308b\u7d44\u7e54\u306e\u30b0\u30eb\u30fc\u30d7",
    "\u81d3\u5668\u7cfb": "\u8907\u96d1\u306a\u6a5f\u80fd\u3092\u5b9f\u884c\u3059\u308b\u305f\u3081\u306b\u9023\u643a\u3059\u308b\u81d3\u5668\u306e\u30b0\u30eb\u30fc\u30d7",
