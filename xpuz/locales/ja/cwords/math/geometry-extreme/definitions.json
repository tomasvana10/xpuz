{
    "\u8907\u96d1\u306a\u5f62\u72b6": "\u8907\u7d20\u6570\u304b\u3089\u751f\u3058\u308b\u3001\u307e\u305f\u306f\u8907\u7d20\u6570\u306b\u3088\u3063\u3066\u8a18\u8ff0\u3055\u308c\u308b\u5e7e\u4f55\u5b66\u7684\u69cb\u9020\u3068\u69cb\u9020\u306e\u7814\u7a76",
    "\u8907\u7d20\u6570": "a + bi \u306e\u5f62\u5f0f\u306e\u6570\u5024 (a \u3068 b \u306f\u5b9f\u6570)",
    "\u5b9f\u90e8": "\u56e0\u6570\u3068\u3057\u3066\u865a\u6570\u5358\u4f4d\u3092\u542b\u307e\u306a\u3044\u8907\u7d20\u6570\u306e\u9805",
    "\u865a\u6570\u90e8": "\u865a\u6570\u5358\u4f4d\u3092\u56e0\u6570\u3068\u3057\u3066\u6301\u3064\u8907\u7d20\u6570\u306e\u90e8\u5206",
    "\u8907\u96d1\u306a\u591a\u69d8\u4f53": "\u66f2\u7dda\u3068\u66f2\u9762\u3092\u4e00\u822c\u5316\u3057\u305f\u8907\u96d1\u306a\u5e7e\u4f55\u5b66\u306e\u7a7a\u9593",
    "\u8907\u7d20\u4ee3\u6570\u591a\u69d8\u4f53": "\u591a\u9805\u65b9\u7a0b\u5f0f\u306e\u89e3\u3067\u3042\u308b\u8907\u96d1\u306a\u5e7e\u4f55\u5b66\u5185\u306e\u7a7a\u9593",
    "\u6b63\u5247\u69cb\u9020": "\u8907\u7d20\u5fae\u5206\u53ef\u80fd\u306a\u8907\u96d1\u306a\u5e7e\u4f55\u5b66\u69cb\u9020",
    "\u6700\u5c0f\u9650\u306e\u30e2\u30c7\u30eb\u30d7\u30ed\u30b0\u30e9\u30e0": "\u8907\u7d20\u591a\u69d8\u4f53\u3068\u8907\u7d20\u4ee3\u6570\u591a\u69d8\u4f53\u3092\u5206\u985e\u3059\u308b\u305f\u3081\u306e\u8907\u7d20\u5e7e\u4f55\u5b66\u30d7\u30ed\u30b0\u30e9\u30e0",
    "\u30e2\u30b8\u30e5\u30e9\u30a4\u7a7a\u9593": "\u3042\u308b\u7a2e\u306e\u5e7e\u4f55\u5b66\u7684\u30aa\u30d6\u30b8\u30a7\u30af\u30c8\u3092\u30d1\u30e9\u30e1\u30fc\u30bf\u5316\u3059\u308b\u8907\u96d1\u306a\u5e7e\u4f55\u5b66\u5185\u306e\u30b9\u30da\u30fc\u30b9",
    "\u30ab\u30e9\u30d3\u4e88\u60f3": "Shing-Tung Yau \u306b\u3088\u3063\u3066\u8a3c\u660e\u3055\u308c\u305f\u3001\u8907\u96d1\u591a\u69d8\u4f53\u4e0a\u306e\u7279\u6b8a\u306a\u30bf\u30a4\u30d7\u306e\u8a08\u91cf\u306e\u5b58\u5728\u306b\u95a2\u3059\u308b\u8907\u96d1\u5e7e\u4f55\u5b66\u306b\u304a\u3051\u308b\u4e88\u60f3",
    "\u30d2\u30c1\u30f3\u30fb\u5c0f\u6797\u901a\u4fe1": "\u5b89\u5b9a\u3057\u305f\u6b63\u5247\u30d9\u30af\u30c8\u30eb\u675f\u3068\u7279\u5b9a\u306e\u5fae\u5206\u65b9\u7a0b\u5f0f\u306e\u89e3\u3068\u306e\u9593\u306e\u8907\u7d20\u5e7e\u4f55\u5b66\u306b\u304a\u3051\u308b\u5bfe\u5fdc",
    "\u30ce\u30ca\u30d9\u30ea\u30a2\u30f3\u30fb\u30db\u30c3\u30b8\u901a\u4fe1": "\u7279\u5b9a\u306e\u30bf\u30a4\u30d7\u306e\u8907\u7d20\u591a\u69d8\u4f53\u3068\u7279\u5b9a\u306e\u30bf\u30a4\u30d7\u306e\u4ee3\u6570\u69cb\u9020\u306e\u9593\u306e\u8907\u7d20\u5e7e\u4f55\u5b66\u306b\u304a\u3051\u308b\u5bfe\u5fdc",
    "\u30b1\u30fc\u30e9\u30fc\u30fb\u30a2\u30a4\u30f3\u30b7\u30e5\u30bf\u30a4\u30f3\u6307\u6a19": "\u7279\u5b9a\u306e\u6761\u4ef6\u3092\u6e80\u305f\u3059\u8907\u96d1\u306a\u591a\u69d8\u4f53\u4e0a\u306e\u7279\u5225\u306a\u30bf\u30a4\u30d7\u306e\u30e1\u30c8\u30ea\u30af\u30b9"
}