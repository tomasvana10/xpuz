{
    "\u0570\u0580\u0561\u0570\u0561\u0576\u0563\u0576\u0565\u0580\u056b \u0581\u056b\u056f\u056c\u0568": "\u0531\u0575\u0576 \u0584\u0561\u0575\u056c\u0565\u0580\u0568, \u0578\u0580\u0578\u0576\u0584 CPU-\u0576 \u0570\u0565\u057f\u0587\u0578\u0582\u0574 \u0567 \u0574\u0565\u056f \u0570\u0580\u0561\u0570\u0561\u0576\u0563 \u056f\u0561\u057f\u0561\u0580\u0565\u056c\u0578\u0582 \u0570\u0561\u0574\u0561\u0580",
    "\u057d\u0578\u0582\u057a\u0565\u0580\u057d\u056f\u0561\u056c\u0561\u0580 \u0573\u0561\u0580\u057f\u0561\u0580\u0561\u057a\u0565\u057f\u0578\u0582\u0569\u0575\u0578\u0582\u0576": "CPU \u0564\u056b\u0566\u0561\u0575\u0576, \u0578\u0580\u0568 \u056f\u0561\u0580\u0578\u0572 \u0567 \u056f\u0561\u057f\u0561\u0580\u0565\u056c \u0574\u056b \u0584\u0561\u0576\u056b \u0570\u0580\u0561\u0570\u0561\u0576\u0563\u0576\u0565\u0580 \u0574\u0565\u056f \u056a\u0561\u0574\u0561\u0581\u0578\u0582\u0575\u0581\u056b \u0581\u056b\u056f\u056c\u056b \u0570\u0561\u0574\u0561\u0580",
    "\u0573\u0575\u0578\u0582\u0572\u056b \u056f\u0561\u0576\u056d\u0561\u057f\u0565\u057d\u0578\u0582\u0574": "\u054f\u0565\u056d\u0576\u056b\u056f\u0561, \u0578\u0580\u057f\u0565\u0572 \u057a\u0580\u0578\u0581\u0565\u057d\u0578\u0580\u0568 \u0583\u0578\u0580\u0571\u0578\u0582\u0574 \u0567 \u056f\u057c\u0561\u0570\u0565\u056c, \u0569\u0565 \u0578\u0580 \u0573\u0561\u0576\u0561\u057a\u0561\u0580\u0570\u0578\u057e \u056f\u0563\u0576\u0561 \u057a\u0561\u0575\u0574\u0561\u0576\u0561\u056f\u0561\u0576\u0568",
    "\u0570\u0575\u0578\u0582\u057d\u056b\u057d\u0561\u0575\u056b\u0576 \u056f\u0561\u0574\u0578\u0582\u0580\u057b": "\u0549\u056b\u0583\u057d\u0565\u0569 (\u0570\u056b\u0576 \u0570\u0561\u0574\u0561\u056f\u0561\u0580\u0563\u0565\u0580), \u0578\u0580\u0568 \u0574\u056b\u0561\u0581\u0576\u0578\u0582\u0574 \u0567 \u057a\u0580\u0578\u0581\u0565\u057d\u0578\u0580\u0568 RAM-\u056b\u0576 \u0587 \u0561\u0580\u0561\u0563\u0568\u0576\u0569\u0561\u0581 \u0561\u057e\u057f\u0578\u0562\u0578\u0582\u057d\u0576\u0565\u0580\u056b\u0576",
    "\u0570\u0561\u0580\u0561\u057e\u0561\u0575\u056b\u0576 \u056f\u0561\u0574\u0578\u0582\u0580\u057b": "\u0549\u056b\u0583\u057d\u0565\u0569 (\u0570\u056b\u0576 \u0570\u0561\u0574\u0561\u056f\u0561\u0580\u0563\u0565\u0580), \u0578\u0580\u0578\u0576\u0584 \u0561\u0577\u056d\u0561\u057f\u0578\u0582\u0574 \u0565\u0576 \u0561\u057e\u0565\u056c\u056b \u0564\u0561\u0576\u0564\u0561\u0572 \u056e\u0561\u0575\u0580\u0561\u0574\u0561\u057d\u0561\u0575\u056b\u0576 \u057d\u0561\u0580\u0584\u0565\u0580\u0578\u057e \u0587 I/O-\u0578\u057e",
    "hyperthreading": "\u054f\u0565\u056d\u0576\u0578\u056c\u0578\u0563\u056b\u0561, \u0578\u0580\u0568 \u0569\u0578\u0582\u0575\u056c \u0567 \u057f\u0561\u056c\u056b\u057d \u0574\u0565\u056f \u057a\u0580\u0578\u0581\u0565\u057d\u0578\u0580\u056b \u0574\u056b\u057b\u0578\u0582\u056f\u0568 \u0570\u0561\u0575\u057f\u0576\u057e\u0565\u056c \u0578\u0580\u057a\u0565\u057d \u0562\u0561\u0566\u0574\u0561\u0569\u056b\u057e \u057f\u0580\u0561\u0574\u0561\u0562\u0561\u0576\u0561\u056f\u0561\u0576 \u0574\u056b\u057b\u0578\u0582\u056f\u0576\u0565\u0580",
    "\u057c\u056b\u057d\u056f": "\u053f\u0580\u0573\u0561\u057f\u057e\u0561\u056e \u0570\u0580\u0561\u0570\u0561\u0576\u0563\u0576\u0565\u0580\u056b \u0570\u0561\u057e\u0561\u0584\u0561\u056e\u0578\u0582 \u0540\u0561\u0574\u0561\u056f\u0561\u0580\u0563\u056b\u0579 - \u057a\u0580\u0578\u0581\u0565\u057d\u0578\u0580\u056b \u0571\u0587\u0561\u057e\u0578\u0580\u0578\u0582\u0574 \u0561\u057e\u0565\u056c\u056b \u057a\u0561\u0580\u0566 \u0570\u0580\u0561\u0570\u0561\u0576\u0563\u0576\u0565\u0580\u0578\u057e",
    "cisc": "Complex Instruction Set Computer - CPU \u0564\u056b\u0566\u0561\u0575\u0576 \u0570\u0566\u0578\u0580, \u0562\u0561\u0580\u0564 \u0570\u0580\u0561\u0570\u0561\u0576\u0563\u0576\u0565\u0580\u0578\u057e",
    "\u057d\u056b\u0574\u0564": "Single Instruction Multiple Data, \u057f\u057e\u0575\u0561\u056c\u0576\u0565\u0580\u056b \u0566\u0578\u0582\u0563\u0561\u0570\u0565\u057c \u0574\u0577\u0561\u056f\u0574\u0561\u0576 \u057f\u0565\u056d\u0576\u056b\u056f\u0561",
    "\u0574\u056b\u056f\u0580\u0578\u0573\u0561\u0580\u057f\u0561\u0580\u0561\u057a\u0565\u057f\u0578\u0582\u0569\u0575\u0578\u0582\u0576": "CPU-\u056b \u0570\u0580\u0561\u0570\u0561\u0576\u0563\u0576\u0565\u0580\u056b \u0570\u0561\u057e\u0561\u0584\u0561\u056e\u0578\u0582\u056b \u056f\u0578\u0576\u056f\u0580\u0565\u057f \u056b\u0580\u0561\u056f\u0561\u0576\u0561\u0581\u0574\u0561\u0576 \u0574\u0561\u0576\u0580\u0561\u0574\u0561\u057d\u0576\u0565\u0580\u0568",
    "\u0570\u0561\u0574\u0561\u056f\u0561\u0580\u0563 \u0579\u056b\u057a\u056b \u057e\u0580\u0561": "\u0544\u056b \u0584\u0561\u0576\u056b \u0562\u0561\u0572\u0561\u0564\u0580\u056b\u0579\u0576\u0565\u0580\u056b (CPU, GPU \u0587 \u0561\u0575\u056c\u0576) \u056b\u0576\u057f\u0565\u0563\u0580\u0578\u0582\u0574 \u0574\u0565\u056f \u0579\u056b\u057a\u056b \u057e\u0580\u0561",
    "\u0576\u0565\u0580\u056f\u0561\u057c\u0578\u0582\u0581\u057e\u0561\u056e \u0570\u0561\u0574\u0561\u056f\u0561\u0580\u0563": "\u0540\u0561\u0574\u0561\u056f\u0561\u0580\u0563\u0579\u0561\u0575\u056b\u0576 \u0570\u0561\u0574\u0561\u056f\u0561\u0580\u0563, \u0578\u0580\u0568 \u0576\u057e\u056b\u0580\u057e\u0561\u056e \u0567 \u056f\u0578\u0576\u056f\u0580\u0565\u057f \u0561\u057c\u0561\u057b\u0561\u0564\u0580\u0561\u0576\u0584\u056b\u0576, \u0570\u0561\u0573\u0561\u056d \u0561\u057e\u0565\u056c\u056b \u0574\u0565\u056e \u057d\u0561\u0580\u0584\u056b \u0576\u0565\u0580\u057d\u0578\u0582\u0574",
    "\u0578\u0580\u0578\u0576\u057e\u0561\u056e\u0568": "\u053e\u0580\u0561\u0563\u0580\u0561\u0577\u0561\u0580\u0568 \u0574\u0577\u057f\u0561\u057a\u0565\u057d \u057a\u0561\u0570\u057e\u0578\u0582\u0574 \u0567 \u0561\u057a\u0561\u0580\u0561\u057f\u0561\u0575\u056b\u0576 \u057d\u0561\u0580\u0584\u0578\u0582\u0574 (\u056b\u0576\u0579\u057a\u0565\u057d BIOS-\u0568)",
    "overclocking": "\u054a\u0580\u0578\u0581\u0565\u057d\u0578\u0580\u056b \u0563\u0578\u0580\u056e\u0561\u0580\u056f\u0578\u0582\u0574\u0568 \u0576\u0561\u056d\u0561\u057f\u0565\u057d\u057e\u0561\u056e\u056b\u0581 \u0561\u057e\u0565\u056c\u056b \u0562\u0561\u0580\u0571\u0580 \u0561\u0580\u0561\u0563\u0578\u0582\u0569\u0575\u0561\u0574\u0562",
    "\u057d\u056d\u0561\u056c\u056b \u0578\u0582\u0572\u0572\u0574\u0561\u0576 \u056f\u0578\u0564\u0568": "RAM-\u0578\u0582\u0574 \u057d\u056d\u0561\u056c\u0576\u0565\u0580\u0568 \u0570\u0561\u0575\u057f\u0576\u0561\u0562\u0565\u0580\u0565\u056c\u0578\u0582 \u0587 \u0578\u0582\u0572\u0572\u0565\u056c\u0578\u0582 \u057f\u0565\u056d\u0576\u056b\u056f\u0561",
    "\u0561\u0576\u057d\u0561\u0580\u0584 \u056f\u0561\u057f\u0561\u0580\u0578\u0582\u0574\u0568": "\u054f\u0565\u056d\u0576\u056b\u056f\u0561, \u0578\u0580\u057f\u0565\u0572 \u057a\u0580\u0578\u0581\u0565\u057d\u0578\u0580\u0568 \u0570\u0580\u0561\u0570\u0561\u0576\u0563\u0576\u0565\u0580 \u0567 \u056f\u0561\u057f\u0561\u0580\u0578\u0582\u0574 \u0561\u057e\u0565\u056c\u056b \u0561\u0580\u0564\u0575\u0578\u0582\u0576\u0561\u057e\u0565\u057f \u056f\u0561\u0580\u0563\u0578\u057e",
    "\u0584\u057e\u0561\u0576\u057f\u0561\u0575\u056b\u0576 \u0570\u0561\u0577\u057e\u0561\u0580\u056f": "\u0540\u0561\u0577\u057e\u0561\u0580\u056f\u0576\u0565\u0580\u056b \u0570\u0561\u0574\u0561\u0580 \u0584\u057e\u0561\u0576\u057f\u0561\u0575\u056b\u0576 \u0574\u0565\u056d\u0561\u0576\u056b\u056f\u0561\u0575\u056b \u057d\u056f\u0566\u0562\u0578\u0582\u0576\u0584\u0576\u0565\u0580\u056b \u056f\u056b\u0580\u0561\u057c\u0578\u0582\u0574",
    "\u0576\u0565\u0575\u0580\u0578\u0574\u0578\u0580\u0586\u0561\u0575\u056b\u0576 \u0570\u0561\u0577\u057e\u0561\u0580\u056f": "\u0548\u0582\u0572\u0565\u0572\u056b\u0581 \u0576\u0565\u0580\u0577\u0576\u0579\u057e\u0561\u056e \u0570\u0561\u0577\u057e\u0578\u0572\u0561\u056f\u0561\u0576 \u0573\u0561\u0580\u057f\u0561\u0580\u0561\u057a\u0565\u057f\u0578\u0582\u0569\u0575\u0578\u0582\u0576\u0576\u0565\u0580, \u0578\u0580\u0578\u0576\u0584 \u0576\u0561\u056d\u0561\u057f\u0565\u057d\u057e\u0561\u056e \u0565\u0576 AI \u0561\u057c\u0561\u057b\u0561\u0564\u0580\u0561\u0576\u0584\u0576\u0565\u0580\u056b \u0570\u0561\u0574\u0561\u0580"
}