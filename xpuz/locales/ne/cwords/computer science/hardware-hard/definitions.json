{
    "\u0928\u093f\u0930\u094d\u0926\u0947\u0936\u0928 \u0938\u0947\u091f": "CPU \u0932\u0947 \u092c\u0941\u091d\u094d\u0928 \u0938\u0915\u094d\u0928\u0947 \u0906\u0926\u0947\u0936\u0939\u0930\u0942\u0915\u094b \u0938\u0919\u094d\u0915\u0932\u0928",
    "\u0926\u0930\u094d\u0924\u093e \u0917\u0930\u094d\u0928\u0941\u0939\u094b\u0938\u094d": "CPU \u092d\u093f\u0924\u094d\u0930 \u0938\u093e\u0928\u094b, \u0938\u0941\u092a\u0930-\u092b\u093e\u0938\u094d\u091f \u092e\u0947\u092e\u094b\u0930\u0940 \u0938\u094d\u0925\u093e\u0928",
    "\u0905\u0902\u0915\u0917\u0923\u093f\u0924 \u0924\u0930\u094d\u0915 \u090f\u0915\u093e\u0907": "CPU \u0915\u094b \u092d\u093e\u0917 \u091c\u0938\u0932\u0947 \u0917\u0923\u0928\u093e \u0917\u0930\u094d\u0926\u091b",
    "\u0928\u093f\u092f\u0928\u094d\u0924\u094d\u0930\u0923 \u0907\u0915\u093e\u0908": "\u0915\u092e\u094d\u092a\u094d\u092f\u0941\u091f\u0930\u0915\u094b \u0915\u092e\u094d\u092a\u094b\u0928\u0947\u0928\u094d\u091f\u0939\u0930\u0942\u0915\u094b \u0915\u093e\u0930\u094d\u092f\u0939\u0930\u0942 \u0938\u092e\u0928\u094d\u0935\u092f \u0917\u0930\u094d\u0926\u091b",
    "\u092e\u0947\u092e\u094b\u0930\u0940 \u092e\u093e\u0924\u094d\u0930 \u092a\u0922\u094d\u0928\u0941\u0939\u094b\u0938\u094d": "\u0915\u092e\u094d\u092a\u094d\u092f\u0942\u091f\u0930 \u0938\u0941\u0930\u0941 \u0917\u0930\u094d\u0928\u0915\u093e \u0932\u093e\u0917\u093f \u0906\u0935\u0936\u094d\u092f\u0915 \u092a\u0930\u094d\u0928\u0947 \u0928\u093f\u0930\u094d\u0926\u0947\u0936\u0928\u0939\u0930\u0942 \u092d\u0923\u094d\u0921\u093e\u0930 \u0917\u0930\u094d\u0926\u091b",
    "\u0915\u094d\u092f\u093e\u0938 \u092e\u0947\u092e\u094b\u0930\u0940": "\u091b\u093f\u091f\u094b \u092a\u0939\u0941\u0901\u091a\u0915\u094b \u0932\u093e\u0917\u093f CPU \u0928\u091c\u093f\u0915\u0915\u094b \u0938\u093e\u0928\u094b, \u091b\u093f\u091f\u094b \u092e\u0947\u092e\u094b\u0930\u0940",
    "\u0918\u0921\u0940\u0915\u094b \u0917\u0924\u093f": "CPU \u0932\u0947 \u0928\u093f\u0930\u094d\u0926\u0947\u0936\u0928\u0939\u0930\u0942 \u0915\u093e\u0930\u094d\u092f\u093e\u0928\u094d\u0935\u092f\u0928 \u0917\u0930\u094d\u0928\u0947 \u0926\u0930",
    "\u0936\u092c\u094d\u0926 \u0906\u0915\u093e\u0930": "CPU \u0932\u0947 \u090f\u0915\u0948 \u092a\u091f\u0915\u092e\u093e \u092a\u094d\u0930\u0936\u094b\u0927\u0928 \u0917\u0930\u094d\u0928 \u0938\u0915\u094d\u0928\u0947 \u092c\u093f\u091f\u0939\u0930\u0942\u0915\u094b \u0938\u0902\u0916\u094d\u092f\u093e",
    "\u092a\u093e\u0907\u092a\u0932\u093e\u0907\u0928": "\u092a\u094d\u0930\u0935\u093f\u0927\u093f \u091c\u0939\u093e\u0901 \u090f\u0915 CPU \u0932\u0947 \u090f\u0915\u0948 \u092a\u091f\u0915 \u0927\u0947\u0930\u0948\u092e\u093e \u0915\u093e\u092e \u0917\u0930\u094d\u0928\u0915\u093e \u0932\u093e\u0917\u093f \u091a\u0930\u0923\u0939\u0930\u0942\u092e\u093e \u0928\u093f\u0930\u094d\u0926\u0947\u0936\u0928\u0939\u0930\u0942 \u0924\u094b\u0921\u094d\u091b",
    "\u092c\u0939\u0941\u092a\u094d\u0930\u0936\u094b\u0927\u0928": "\u090f\u0915\u0948 \u0938\u093e\u0925 \u0915\u093e\u0930\u094d\u092f\u0939\u0930\u0942 \u0917\u0930\u094d\u0928 \u0927\u0947\u0930\u0948 CPU \u0939\u0930\u0942 \u0935\u093e \u0915\u094b\u0930\u0939\u0930\u0942 \u092a\u094d\u0930\u092f\u094b\u0917 \u0917\u0930\u094d\u0926\u0948",
    "\u092c\u093e\u0927\u093e": "\u0918\u091f\u0928\u093e \u0939\u094d\u092f\u093e\u0928\u094d\u0921\u0932 \u0917\u0930\u094d\u0928\u0915\u094b \u0932\u093e\u0917\u093f CPU \u0932\u093e\u0908 \u0905\u0938\u094d\u0925\u093e\u092f\u0940 \u0930\u0942\u092a\u092e\u093e \u092a\u091c \u0917\u0930\u094d\u0928\u0947 \u0938\u0919\u094d\u0915\u0947\u0924",
    "\u092d\u0930\u094d\u091a\u0941\u0905\u0932 \u092e\u0947\u092e\u094b\u0930\u0940": "RAM \u0915\u094b \u0935\u093f\u0938\u094d\u0924\u093e\u0930\u0915\u094b \u0930\u0942\u092a\u092e\u093e \u0939\u093e\u0930\u094d\u0921 \u0921\u093f\u0938\u094d\u0915 \u0938\u094d\u092a\u0947\u0938 \u092a\u094d\u0930\u092f\u094b\u0917 \u0917\u0930\u094d\u0928\u0947 \u092a\u094d\u0930\u0935\u093f\u0927\u093f",
    "bios": "\u0906\u0927\u093e\u0930\u092d\u0942\u0924 \u0907\u0928\u092a\u0941\u091f/\u0906\u0909\u091f\u092a\u0941\u091f \u092a\u094d\u0930\u0923\u093e\u0932\u0940, \u092b\u0930\u094d\u092e\u0935\u0947\u092f\u0930 \u091c\u0938\u0932\u0947 \u0938\u094d\u091f\u093e\u0930\u094d\u091f\u0905\u092a\u092e\u093e \u0939\u093e\u0930\u094d\u0921\u0935\u0947\u092f\u0930 \u0938\u0941\u0930\u0941 \u0917\u0930\u094d\u091b",
    "i/o \u092a\u094b\u0930\u094d\u091f": "\u092a\u0930\u093f\u0927\u0940\u092f \u0909\u092a\u0915\u0930\u0923\u0939\u0930\u0942\u0938\u0901\u0917 \u0938\u091e\u094d\u091a\u093e\u0930\u0915\u094b \u0932\u093e\u0917\u093f \u0915\u092e\u094d\u092a\u094d\u092f\u0941\u091f\u0930\u092e\u093e \u0935\u093f\u0936\u093f\u0937\u094d\u091f \u0920\u0947\u0917\u093e\u0928\u093e",
    "\u0909\u092a\u0915\u0930\u0923 \u091a\u093e\u0932\u0915": "\u0938\u092b\u094d\u091f\u0935\u0947\u092f\u0930 \u091c\u0938\u0932\u0947 \u0905\u092a\u0930\u0947\u091f\u093f\u0919 \u0938\u093f\u0938\u094d\u091f\u092e\u0932\u093e\u0908 \u0909\u092a\u0915\u0930\u0923 \u0928\u093f\u092f\u0928\u094d\u0924\u094d\u0930\u0923 \u0917\u0930\u094d\u0928 \u0905\u0928\u0941\u092e\u0924\u093f \u0926\u093f\u0928\u094d\u091b",
    "endianness": "\u092c\u093e\u0907\u091f\u0939\u0930\u0942 \u092e\u0947\u092e\u094b\u0930\u0940\u092e\u093e \u092d\u0923\u094d\u0921\u093e\u0930\u0923 \u0917\u0930\u093f\u090f\u0915\u093e \u0915\u094d\u0930\u092e",
    "\u0928\u0947\u091f\u0935\u0930\u094d\u0915 \u092a\u094d\u0930\u094b\u091f\u094b\u0915\u0932": "\u092f\u0928\u094d\u0924\u094d\u0930\u0939\u0930\u0942 \u092c\u0940\u091a \u0938\u091e\u094d\u091a\u093e\u0930\u0915\u094b \u0932\u093e\u0917\u093f \u0928\u093f\u092f\u092e \u0930 \u092e\u093e\u092a\u0926\u0923\u094d\u0921\u0939\u0930\u0942\u0915\u094b \u0938\u0947\u091f",
    "\u092a\u0948\u0915\u0947\u091f": "\u0928\u0947\u091f\u0935\u0930\u094d\u0915\u092e\u093e \u092a\u094d\u0930\u0938\u093e\u0930\u093f\u0924 \u0921\u093e\u091f\u093e\u0915\u094b \u0938\u093e\u0928\u094b \u090f\u0915\u093e\u0907",
    "\u0930\u093e\u0909\u091f\u0930": "\u0935\u093f\u092d\u093f\u0928\u094d\u0928 \u0928\u0947\u091f\u0935\u0930\u094d\u0915\u0939\u0930\u0942 \u092c\u0940\u091a \u092a\u094d\u092f\u093e\u0915\u0947\u091f\u0939\u0930\u0942 \u092b\u0930\u094d\u0935\u093e\u0930\u094d\u0921 \u0917\u0930\u094d\u0928\u0947 \u0909\u092a\u0915\u0930\u0923",
    "\u0938\u092c\u0928\u0947\u091f \u092e\u093e\u0938\u094d\u0915": "IP \u0920\u0947\u0917\u093e\u0928\u093e\u0915\u094b \u0928\u0947\u091f\u0935\u0930\u094d\u0915 \u0930 \u0939\u094b\u0938\u094d\u091f \u092d\u093e\u0917\u0939\u0930\u0942 \u0928\u093f\u0930\u094d\u0927\u093e\u0930\u0923 \u0917\u0930\u094d\u0928 \u092a\u094d\u0930\u092f\u094b\u0917 \u0917\u0930\u093f\u0928\u094d\u091b",
    "\u091f\u094b\u092a\u094b\u0932\u094b\u091c\u0940": "\u0928\u0947\u091f\u0935\u0930\u094d\u0915\u092e\u093e \u092f\u0928\u094d\u0924\u094d\u0930\u0939\u0930\u0942 \u0930 \u091c\u0921\u093e\u0928\u0939\u0930\u0942\u0915\u094b \u0935\u094d\u092f\u0935\u0938\u094d\u0925\u093e",
    "\u0917\u094d\u0930\u093e\u0939\u0915 \u0938\u0930\u094d\u092d\u0930 \u092e\u094b\u0921\u0947\u0932": "\u0928\u0947\u091f\u0935\u0930\u094d\u0915 \u0906\u0930\u094d\u0915\u093f\u091f\u0947\u0915\u094d\u091a\u0930 \u091c\u0939\u093e\u0901 \u0917\u094d\u0930\u093e\u0939\u0915\u0939\u0930\u0942\u0932\u0947 \u0938\u0930\u094d\u092d\u0930\u0939\u0930\u0942\u092c\u093e\u091f \u0938\u094d\u0930\u094b\u0924\u0939\u0930\u0942 \u0905\u0928\u0941\u0930\u094b\u0927 \u0917\u0930\u094d\u091b\u0928\u094d",
    "dma": "\u0938\u093f\u0927\u093e \u092e\u0947\u092e\u094b\u0930\u0940 \u092a\u0939\u0941\u0901\u091a, \u092f\u0928\u094d\u0924\u094d\u0930\u0939\u0930\u0942\u0932\u093e\u0908 CPU \u092a\u094d\u0930\u092f\u094b\u0917 \u0928\u0917\u0930\u0940 \u0921\u093e\u091f\u093e \u0938\u094d\u0925\u093e\u0928\u093e\u0928\u094d\u0924\u0930\u0923 \u0917\u0930\u094d\u0928 \u0905\u0928\u0941\u092e\u0924\u093f \u0926\u093f\u0928\u094d\u091b"
}