{
    "karyotype": "\u091c\u0940\u0935\u092e\u093e \u0915\u094d\u0930\u094b\u092e\u094b\u091c\u094b\u092e\u0939\u0930\u0942\u0915\u094b \u092a\u0942\u0930\u094d\u0923 \u0938\u0947\u091f",
    "\u090f\u0932\u0947\u0932": "\u091c\u0940\u0928\u0915\u094b \u0935\u0948\u0915\u0932\u094d\u092a\u093f\u0915 \u0930\u0942\u092a",
    "homologous": "\u0915\u094d\u0930\u094b\u092e\u094b\u091c\u094b\u092e\u0939\u0930\u0942 \u091c\u0941\u0928 \u0909\u0938\u094d\u0924\u0948 \u0926\u0947\u0916\u093f\u0928\u094d\u091b\u0928\u094d \u0930 \u0938\u092e\u093e\u0928 \u0932\u0915\u094d\u0937\u0923\u0939\u0930\u0942\u0915\u094b \u0932\u093e\u0917\u093f \u091c\u0940\u0928\u0939\u0930\u0942 \u092c\u094b\u0915\u094d\u091b\u0928\u094d",
    "meiosis": "\u0915\u094b\u0936\u093f\u0915\u093e \u0935\u093f\u092d\u093e\u091c\u0928 \u092a\u094d\u0930\u0915\u094d\u0930\u093f\u092f\u093e \u091c\u0938\u0932\u0947 \u092e\u0942\u0932 \u0915\u094b\u0936\u093f\u0915\u093e\u0915\u094b \u0930\u0942\u092a\u092e\u093e \u0915\u094d\u0930\u094b\u092e\u094b\u091c\u094b\u092e\u0939\u0930\u0942\u0915\u094b \u0906\u0927\u093e \u0938\u0902\u0916\u094d\u092f\u093e\u0915\u094b \u0938\u093e\u0925 \u0917\u0947\u092e\u0947\u091f\u094d\u0938 (\u0936\u0941\u0915\u094d\u0930\u093e\u0923\u0941 \u0930 \u0905\u0923\u094d\u0921\u093e) \u0909\u0924\u094d\u092a\u093e\u0926\u0928 \u0917\u0930\u094d\u0926\u091b\u0964",
    "\u092e\u093e\u0907\u091f\u094b\u0938\u093f\u0938": "\u0915\u094b\u0936\u093f\u0915\u093e \u0935\u093f\u092d\u093e\u091c\u0928 \u092a\u094d\u0930\u0915\u094d\u0930\u093f\u092f\u093e \u091c\u0938\u0932\u0947 \u092e\u0942\u0932 \u0915\u094b\u0936\u093f\u0915\u093e\u0915\u094b \u0930\u0942\u092a\u092e\u093e \u0915\u094d\u0930\u094b\u092e\u094b\u091c\u094b\u092e\u0939\u0930\u0942\u0915\u094b \u090f\u0909\u091f\u0948 \u0938\u0902\u0916\u094d\u092f\u093e\u092e\u093e \u0928\u092f\u093e\u0901 \u0915\u094b\u0936\u093f\u0915\u093e\u0939\u0930\u0942 \u0909\u0924\u094d\u092a\u093e\u0926\u0928 \u0917\u0930\u094d\u0926\u091b",
    "karyotyping": "\u0905\u0938\u093e\u092e\u093e\u0928\u094d\u092f\u0924\u093e\u0939\u0930\u0942 \u092a\u0939\u093f\u091a\u093e\u0928 \u0917\u0930\u094d\u0928 \u0935\u094d\u092f\u0915\u094d\u0924\u093f\u0915\u094b \u0915\u094d\u0930\u094b\u092e\u094b\u091c\u094b\u092e\u0939\u0930\u0942 \u0935\u093f\u0936\u094d\u0932\u0947\u0937\u0923 \u0917\u0930\u094d\u0926\u0948",
    "\u091c\u0940\u0928\u094b\u091f\u093e\u0907\u092a": "\u090f\u0915 \u091c\u0940\u0935 \u0915\u094b \u0906\u0928\u0941\u0935\u0902\u0936\u093f\u0915 \u092e\u0947\u0915\u0905\u092a",
    "\u092b\u0947\u0928\u094b\u091f\u093e\u0907\u092a": "\u091c\u0940\u0928\u094b\u091f\u093e\u0907\u092a \u0930 \u0935\u093e\u0924\u093e\u0935\u0930\u0923 \u0926\u0941\u0935\u0948 \u0926\u094d\u0935\u093e\u0930\u093e \u0928\u093f\u0930\u094d\u0927\u093e\u0930\u093f\u0924 \u091c\u0940\u0935 \u0915\u094b \u0905\u0935\u0932\u094b\u0915\u0928 \u092f\u094b\u0917\u094d\u092f \u0935\u093f\u0936\u0947\u0937\u0924\u093e\u0939\u0930\u0941",
    "\u092a\u094d\u0930\u092d\u093e\u0935\u0936\u093e\u0932\u0940": "\u090f\u0932\u0947\u0932 \u091c\u0938\u0932\u0947 \u090f\u0909\u091f\u0948 \u0915\u094d\u0930\u094b\u092e\u094b\u091c\u094b\u092e\u092e\u093e \u0909\u092a\u0938\u094d\u0925\u093f\u0924 \u0939\u0941\u0901\u0926\u093e \u0930\u093f\u0938\u0947\u0938\u093f\u092d \u090f\u0932\u093f\u0932\u0915\u094b \u092a\u094d\u0930\u092d\u093e\u0935\u0932\u093e\u0908 \u092e\u093e\u0938\u094d\u0915 \u0917\u0930\u094d\u0926\u091b",
    "\u0930\u093f\u0938\u0947\u0938\u093f\u092d": "\u090f\u0909\u091f\u0948 \u0915\u094d\u0930\u094b\u092e\u094b\u091c\u094b\u092e\u092e\u093e \u0909\u092a\u0938\u094d\u0925\u093f\u0924 \u0939\u0941\u0901\u0926\u093e \u090f\u0915 \u092a\u094d\u0930\u092d\u093e\u0935\u0936\u093e\u0932\u0940 \u090f\u0932\u093f\u0932\u0932\u0947 \u092e\u093e\u0938\u094d\u0915 \u0917\u0930\u093f\u090f\u0915\u094b \u090f\u0932\u0947\u0932",
    "punnett \u0935\u0930\u094d\u0917": "\u0930\u0947\u0916\u093e\u091a\u093f\u0924\u094d\u0930\u0932\u0947 \u0935\u093f\u0936\u0947\u0937 \u0935\u093f\u0936\u0947\u0937\u0924\u093e\u0939\u0930\u0942 \u092a\u094d\u0930\u093e\u092a\u094d\u0924 \u0917\u0930\u094d\u0928\u0947 \u0938\u0928\u094d\u0924\u093e\u0928\u0915\u094b \u0938\u092e\u094d\u092d\u093e\u0935\u0928\u093e\u0915\u094b \u092d\u0935\u093f\u0937\u094d\u092f\u0935\u093e\u0923\u0940 \u0917\u0930\u094d\u0928 \u092a\u094d\u0930\u092f\u094b\u0917 \u0917\u0930\u093f\u0928\u094d\u091b",
    "\u092c\u093e\u092f\u094b\u091f\u0947\u0915\u094d\u0928\u094b\u0932\u094b\u091c\u0940": "\u0928\u092f\u093e\u0901 \u0909\u0924\u094d\u092a\u093e\u0926\u0928 \u0935\u093e \u092a\u094d\u0930\u0915\u094d\u0930\u093f\u092f\u093e\u0939\u0930\u0942 \u092a\u0930\u093f\u092e\u093e\u0930\u094d\u091c\u0928 \u0935\u093e \u0938\u093f\u0930\u094d\u091c\u0928\u093e \u0917\u0930\u094d\u0928 \u091c\u0940\u0935\u093f\u0924 \u091c\u0940\u0935\u0939\u0930\u0942 \u0935\u093e \u0924\u093f\u0928\u0940\u0939\u0930\u0942\u0915\u093e \u0909\u0924\u094d\u092a\u093e\u0926\u0928\u0939\u0930\u0942\u0915\u094b \u092a\u094d\u0930\u092f\u094b\u0917",
    "gmo": "\u091c\u0940\u0935\u0939\u0930\u0942\u0932\u093e\u0908 \u0926\u093f\u0907\u090f\u0915\u094b \u0938\u0902\u0915\u094d\u0937\u093f\u092a\u094d\u0924 \u0936\u092c\u094d\u0926 \u091c\u0938\u0915\u094b \u0906\u0928\u0941\u0935\u0902\u0936\u093f\u0915 \u0936\u094d\u0930\u0943\u0919\u094d\u0916\u0932\u093e \u092a\u094d\u0930\u093e\u0915\u0943\u0924\u093f\u0915 \u0930\u0942\u092a\u092e\u093e \u0928\u0939\u0941\u0928\u0947 \u0924\u0930\u093f\u0915\u093e\u092e\u093e \u092a\u0930\u093f\u0935\u0930\u094d\u0924\u0928 \u0917\u0930\u093f\u090f\u0915\u094b \u091b\u0964",
    "\u090f\u0928\u094d\u091f\u093f\u092c\u093e\u092f\u094b\u091f\u093f\u0915": "\u092a\u0926\u093e\u0930\u094d\u0925 \u091c\u0938\u0932\u0947 \u092c\u094d\u092f\u093e\u0915\u094d\u091f\u0947\u0930\u093f\u092f\u093e\u0915\u094b \u0935\u0943\u0926\u094d\u0927\u093f\u0932\u093e\u0908 \u092e\u093e\u0930\u094d\u091b \u0935\u093e \u0930\u094b\u0915\u094d\u091b",
    "\u092d\u093e\u0907\u0930\u0938": "\u0938\u0902\u0915\u094d\u0930\u093e\u092e\u0915 \u090f\u091c\u0947\u0928\u094d\u091f \u091c\u0938\u0932\u0947 \u0939\u094b\u0938\u094d\u091f \u091c\u0940\u0935\u0915\u094b \u0915\u094b\u0936\u093f\u0915\u093e\u0939\u0930\u0942\u092e\u093e \u092e\u093e\u0924\u094d\u0930 \u092a\u094d\u0930\u0924\u093f\u0915\u0943\u0924\u093f \u092c\u0928\u093e\u0909\u0901\u091b",
    "\u0916\u094b\u092a": "\u091c\u0948\u0935\u093f\u0915 \u0924\u092f\u093e\u0930\u0940 \u091c\u0938\u0932\u0947 \u0935\u093f\u0936\u0947\u0937 \u0930\u094b\u0917\u0915\u094b \u092a\u094d\u0930\u0924\u093f\u0930\u094b\u0927\u093e\u0924\u094d\u092e\u0915 \u0915\u094d\u0937\u092e\u0924\u093e \u092a\u094d\u0930\u0926\u093e\u0928 \u0917\u0930\u094d\u0926\u091b",
    "\u0938\u094d\u0928\u093e\u092f\u0941 \u092a\u094d\u0930\u0923\u093e\u0932\u0940": "\u0935\u093f\u0936\u0947\u0937 \u0915\u094b\u0936\u093f\u0915\u093e\u0939\u0930\u0942\u0915\u094b \u0928\u0947\u091f\u0935\u0930\u094d\u0915 \u091c\u0938\u0932\u0947 \u0938\u092e\u094d\u092a\u0942\u0930\u094d\u0923 \u0936\u0930\u0940\u0930\u092e\u093e \u091c\u093e\u0928\u0915\u093e\u0930\u0940 \u092a\u0920\u093e\u0909\u0901\u091b",
    "endocrine \u092a\u094d\u0930\u0923\u093e\u0932\u0940": "\u0917\u094d\u0930\u0928\u094d\u0925\u0940\u0939\u0930\u0942\u0915\u094b \u092a\u094d\u0930\u0923\u093e\u0932\u0940 \u091c\u0938\u0932\u0947 \u0936\u0930\u0940\u0930\u0915\u093e \u0935\u093f\u092d\u093f\u0928\u094d\u0928 \u0915\u093e\u0930\u094d\u092f\u0939\u0930\u0942 \u0935\u093f\u0928\u093f\u092f\u092e\u093f\u0924 \u0917\u0930\u094d\u0928\u0947 \u0939\u093e\u0930\u094d\u092e\u094b\u0928\u0939\u0930\u0942 \u0909\u0924\u094d\u092a\u093e\u0926\u0928 \u0917\u0930\u094d\u0926\u091b",
    "\u092a\u0930\u093f\u0938\u0902\u091a\u0930\u0923 \u092a\u094d\u0930\u0923\u093e\u0932\u0940": "\u0905\u0902\u0917 \u0930 \u092d\u093e\u0901\u0921\u093e\u0939\u0930\u0942\u0915\u094b \u092a\u094d\u0930\u0923\u093e\u0932\u0940 \u091c\u0938\u0932\u0947 \u0936\u0930\u0940\u0930\u092d\u0930 \u0930\u0917\u0924 \u0922\u0941\u0935\u093e\u0928\u0940 \u0917\u0930\u094d\u091b",
    "\u092a\u094d\u0930\u0924\u093f\u0930\u0915\u094d\u0937\u093e \u092a\u094d\u0930\u0923\u093e\u0932\u0940": "\u0915\u094b\u0936\u093f\u0915\u093e \u0930 \u0905\u0902\u0917\u0939\u0930\u0942\u0915\u094b \u092a\u094d\u0930\u0923\u093e\u0932\u0940 \u091c\u0938\u0932\u0947 \u0936\u0930\u0940\u0930\u0932\u093e\u0908 \u0930\u094b\u0917\u092c\u093e\u091f \u092c\u091a\u093e\u0909\u0901\u091b",
    "\u0930\u094b\u0917\u091c\u0928\u0915": "\u0938\u0942\u0915\u094d\u0937\u094d\u092e\u091c\u0940\u0935 \u0935\u093e \u092d\u093e\u0907\u0930\u0938 \u091c\u0938\u0932\u0947 \u0930\u094b\u0917 \u0928\u093f\u092e\u094d\u0924\u094d\u092f\u093e\u0909\u0901\u091b",
    "\u0935\u093f\u0915\u093e\u0938\u0935\u093e\u0926\u0940 \u0905\u0928\u0941\u0915\u0942\u0932\u0928": "\u0935\u093f\u0936\u0947\u0937\u0924\u093e \u091c\u0938\u0932\u0947 \u092f\u0938\u0915\u094b \u0935\u093e\u0924\u093e\u0935\u0930\u0923\u092e\u093e \u091c\u0940\u0935\u0932\u093e\u0908 \u091c\u0940\u0935\u093f\u0924 \u0935\u093e \u092a\u094d\u0930\u091c\u0928\u0928 \u0932\u093e\u092d \u092a\u094d\u0930\u0926\u093e\u0928 \u0917\u0930\u094d\u0926\u091b",
    "\u0935\u093f\u0936\u093f\u0937\u094d\u091f\u0924\u093e": "\u0935\u093f\u0926\u094d\u092f\u092e\u093e\u0928 \u092a\u094d\u0930\u091c\u093e\u0924\u093f\u092c\u093e\u091f \u0928\u092f\u093e\u0901 \u092a\u094d\u0930\u091c\u093e\u0924\u093f\u0939\u0930\u0942 \u0909\u0924\u094d\u092a\u0928\u094d\u0928 \u0939\u0941\u0928\u0947 \u092a\u094d\u0930\u0915\u094d\u0930\u093f\u092f\u093e"
}