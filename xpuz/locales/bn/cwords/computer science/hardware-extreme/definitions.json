{
    "\u09a8\u09bf\u09b0\u09cd\u09a6\u09c7\u09b6 \u099a\u0995\u09cd\u09b0": "\u09b8\u09bf\u09aa\u09bf\u0987\u0989 \u098f\u0995\u099f\u09bf \u098f\u0995\u0995 \u09a8\u09bf\u09b0\u09cd\u09a6\u09c7\u09b6 \u0995\u09be\u09b0\u09cd\u09af\u0995\u09b0 \u0995\u09b0\u09a4\u09c7 \u09af\u09c7 \u09a7\u09be\u09aa\u0997\u09c1\u09b2\u09bf \u0985\u09a8\u09c1\u09b8\u09b0\u09a3 \u0995\u09b0\u09c7",
    "\u09b8\u09c1\u09aa\u09be\u09b0\u09b8\u09cd\u0995\u09c7\u09b2\u09be\u09b0 \u0986\u09b0\u09cd\u0995\u09bf\u099f\u09c7\u0995\u099a\u09be\u09b0": "CPU \u09a1\u09bf\u099c\u09be\u0987\u09a8 \u09af\u09be \u09aa\u09cd\u09b0\u09a4\u09bf \u0998\u09a1\u09bc\u09bf\u09b0 \u099a\u0995\u09cd\u09b0\u09c7 \u098f\u0995\u09be\u09a7\u09bf\u0995 \u09a8\u09bf\u09b0\u09cd\u09a6\u09c7\u09b6\u09be\u09ac\u09b2\u09c0 \u0995\u09be\u09b0\u09cd\u09af\u0995\u09b0 \u0995\u09b0\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7",
    "\u09b6\u09be\u0996\u09be \u09aa\u09c2\u09b0\u09cd\u09ac\u09be\u09ad\u09be\u09b8": "\u0995\u09cc\u09b6\u09b2 \u09af\u09c7\u0996\u09be\u09a8\u09c7 CPU \u0985\u09a8\u09c1\u09ae\u09be\u09a8 \u0995\u09b0\u09be\u09b0 \u099a\u09c7\u09b7\u09cd\u099f\u09be \u0995\u09b0\u09c7 \u0995\u09cb\u09a8 \u09b6\u09b0\u09cd\u09a4\u09b8\u09be\u09aa\u09c7\u0995\u09cd\u09b7\u09c7 \u09af\u09be\u09ac\u09c7",
    "\u0989\u09a4\u09cd\u09a4\u09b0 \u09b8\u09c7\u09a4\u09c1": "\u099a\u09bf\u09aa\u09b8\u09c7\u099f (\u09aa\u09c1\u09b0\u09be\u09a8\u09cb \u09b8\u09bf\u09b8\u09cd\u099f\u09c7\u09ae) \u09af\u09be CPU \u0995\u09c7 RAM \u098f\u09ac\u0982 \u0989\u099a\u09cd\u099a-\u0997\u09a4\u09bf\u09b0 \u09ac\u09be\u09b8\u09c7\u09b0 \u09b8\u09be\u09a5\u09c7 \u09b8\u0982\u09af\u09c1\u0995\u09cd\u09a4 \u0995\u09b0\u09c7\u099b\u09c7",
    "\u09a6\u0995\u09cd\u09b7\u09bf\u09a3 \u09b8\u09c7\u09a4\u09c1": "\u099a\u09bf\u09aa\u09b8\u09c7\u099f (\u09aa\u09c1\u09b0\u09be\u09a8\u09cb \u09b8\u09bf\u09b8\u09cd\u099f\u09c7\u09ae) \u09af\u09be \u09a7\u09c0\u09b0\u0997\u09a4\u09bf\u09b0 \u09aa\u09c7\u09b0\u09bf\u09ab\u09c7\u09b0\u09be\u09b2 \u098f\u09ac\u0982 I/O \u09aa\u09b0\u09bf\u099a\u09be\u09b2\u09a8\u09be \u0995\u09b0\u09c7",
    "\u09b9\u09be\u0987\u09aa\u09be\u09b0\u09a5\u09cd\u09b0\u09c7\u09a1\u09bf\u0982": "\u09aa\u09cd\u09b0\u09af\u09c1\u0995\u09cd\u09a4\u09bf \u098f\u0995\u099f\u09bf \u098f\u0995\u0995 \u09b8\u09bf\u09aa\u09bf\u0987\u0989 \u0995\u09cb\u09b0\u0995\u09c7 \u098f\u0995\u09be\u09a7\u09bf\u0995 \u09b2\u099c\u09bf\u0995\u09cd\u09af\u09be\u09b2 \u0995\u09cb\u09b0 \u09b9\u09bf\u09b8\u09be\u09ac\u09c7 \u0989\u09aa\u09b8\u09cd\u09a5\u09bf\u09a4 \u09b9\u09a4\u09c7 \u09a6\u09c7\u09af\u09bc",
    "\u099d\u09c1\u0981\u0995\u09bf\u09aa\u09c2\u09b0\u09cd\u09a3": "\u0995\u09ae \u09a8\u09bf\u09b0\u09cd\u09a6\u09c7\u09b6\u09a8\u09be \u09b8\u09c7\u099f \u0995\u09ae\u09cd\u09aa\u09bf\u0989\u099f\u09be\u09b0 - \u09b8\u09b9\u099c \u09a8\u09bf\u09b0\u09cd\u09a6\u09c7\u09b6\u09be\u09ac\u09b2\u09c0 \u09b8\u09b9 CPU \u09a1\u09bf\u099c\u09be\u0987\u09a8",
    "cisc": "\u099c\u099f\u09bf\u09b2 \u09a8\u09bf\u09b0\u09cd\u09a6\u09c7\u09b6\u09a8\u09be \u09b8\u09c7\u099f \u0995\u09ae\u09cd\u09aa\u09bf\u0989\u099f\u09be\u09b0 - \u09b6\u0995\u09cd\u09a4\u09bf\u09b6\u09be\u09b2\u09c0, \u099c\u099f\u09bf\u09b2 \u09a8\u09bf\u09b0\u09cd\u09a6\u09c7\u09b6\u09be\u09ac\u09b2\u09c0 \u09b8\u09b9 CPU \u09a1\u09bf\u099c\u09be\u0987\u09a8",
    "simd": "\u098f\u0995\u0995 \u09a8\u09bf\u09b0\u09cd\u09a6\u09c7\u09b6 \u098f\u0995\u09be\u09a7\u09bf\u0995 \u09a1\u09c7\u099f\u09be, \u09b8\u09ae\u09be\u09a8\u09cd\u09a4\u09b0\u09be\u09b2\u09ad\u09be\u09ac\u09c7 \u09a1\u09c7\u099f\u09be \u09aa\u09cd\u09b0\u0995\u09cd\u09b0\u09bf\u09af\u09bc\u09be\u0995\u09b0\u09a3\u09c7\u09b0 \u0995\u09cc\u09b6\u09b2",
    "\u09ae\u09be\u0987\u0995\u09cd\u09b0\u09cb\u0986\u09b0\u09cd\u0995\u09bf\u099f\u09c7\u0995\u099a\u09be\u09b0": "\u098f\u0995\u099f\u09bf CPU-\u098f\u09b0 \u09a8\u09bf\u09b0\u09cd\u09a6\u09c7\u09b6 \u09b8\u09c7\u099f\u09c7\u09b0 \u09a8\u09bf\u09b0\u09cd\u09a6\u09bf\u09b7\u09cd\u099f \u09ac\u09be\u09b8\u09cd\u09a4\u09ac\u09be\u09af\u09bc\u09a8\u09c7\u09b0 \u09ac\u09bf\u09ac\u09b0\u09a3",
    "\u098f\u0995\u099f\u09bf \u099a\u09bf\u09aa\u09c7 \u09b8\u09bf\u09b8\u09cd\u099f\u09c7\u09ae": "\u098f\u0995\u0995 \u099a\u09bf\u09aa\u09c7 \u098f\u0995\u09be\u09a7\u09bf\u0995 \u0989\u09aa\u09be\u09a6\u09be\u09a8 (CPU, GPU, \u0987\u09a4\u09cd\u09af\u09be\u09a6\u09bf) \u098f\u0995\u09c0\u09ad\u09c2\u09a4 \u0995\u09b0\u09be",
    "\u098f\u09ae\u09ac\u09c7\u09a1\u09c7\u09a1 \u09b8\u09bf\u09b8\u09cd\u099f\u09c7\u09ae": "\u098f\u0995\u099f\u09bf \u09a8\u09bf\u09b0\u09cd\u09a6\u09bf\u09b7\u09cd\u099f \u0995\u09be\u099c\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u09a8\u09bf\u09ac\u09c7\u09a6\u09bf\u09a4 \u098f\u0995\u099f\u09bf \u0995\u09ae\u09cd\u09aa\u09bf\u0989\u099f\u09be\u09b0 \u09b8\u09bf\u09b8\u09cd\u099f\u09c7\u09ae, \u09aa\u09cd\u09b0\u09be\u09af\u09bc\u0987 \u098f\u0995\u099f\u09bf \u09ac\u09a1\u09bc \u09a1\u09bf\u09ad\u09be\u0987\u09b8\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7",
    "\u09ab\u09be\u09b0\u09cd\u09ae\u0993\u09af\u09bc\u09cd\u09af\u09be\u09b0": "\u09b8\u09ab\u09cd\u099f\u0993\u09af\u09bc\u09cd\u09af\u09be\u09b0 \u09b8\u09cd\u09a5\u09be\u09af\u09bc\u09c0\u09ad\u09be\u09ac\u09c7 \u098f\u0995\u099f\u09bf \u09b9\u09be\u09b0\u09cd\u09a1\u0993\u09af\u09bc\u09cd\u09af\u09be\u09b0 \u09a1\u09bf\u09ad\u09be\u0987\u09b8\u09c7 \u09b8\u0982\u09b0\u0995\u09cd\u09b7\u09a3 \u0995\u09b0\u09be \u09b9\u09af\u09bc (\u09af\u09c7\u09ae\u09a8 BIOS)",
    "overclocking": "\u098f\u0995\u099f\u09bf \u09b8\u09bf\u09aa\u09bf\u0987\u0989\u0995\u09c7 \u098f\u09b0 \u0989\u09a6\u09cd\u09a6\u09c7\u09b6\u09cd\u09af\u09ae\u09c2\u09b2\u0995 \u09b8\u09cd\u09aa\u09c7\u09b8\u09bf\u09ab\u09bf\u0995\u09c7\u09b6\u09a8\u09c7\u09b0 \u099a\u09c7\u09af\u09bc\u09c7 \u09ac\u09c7\u09b6\u09bf \u0997\u09a4\u09bf\u09a4\u09c7 \u099a\u09be\u09b2\u09be\u09a8\u09cb",
    "\u09a4\u09cd\u09b0\u09c1\u099f\u09bf \u09b8\u0982\u09b6\u09cb\u09a7\u09a8 \u0995\u09cb\u09a1": "RAM \u098f \u09a4\u09cd\u09b0\u09c1\u099f\u09bf \u09b8\u09a8\u09be\u0995\u09cd\u09a4 \u098f\u09ac\u0982 \u09b8\u0982\u09b6\u09cb\u09a7\u09a8 \u0995\u09b0\u09be\u09b0 \u0995\u09cc\u09b6\u09b2",
    "\u0986\u09a6\u09c7\u09b6 \u09a8\u09bf\u09b0\u09cd\u09ac\u09be\u09b9\u09c7\u09b0 \u09ac\u09be\u0987\u09b0\u09c7": "\u0995\u09cc\u09b6\u09b2 \u09af\u09c7\u0996\u09be\u09a8\u09c7 \u098f\u0995\u099f\u09bf \u09b8\u09bf\u09aa\u09bf\u0987\u0989 \u0986\u09b0\u0993 \u09a6\u0995\u09cd\u09b7 \u0995\u09cd\u09b0\u09ae\u09c7 \u09a8\u09bf\u09b0\u09cd\u09a6\u09c7\u09b6\u09be\u09ac\u09b2\u09c0 \u0995\u09be\u09b0\u09cd\u09af\u0995\u09b0 \u0995\u09b0\u09c7",
    "\u0995\u09cb\u09af\u09bc\u09be\u09a8\u09cd\u099f\u09be\u09ae \u0995\u09ae\u09cd\u09aa\u09bf\u0989\u099f\u09bf\u0982": "\u0997\u09a3\u09a8\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u0995\u09cb\u09af\u09bc\u09be\u09a8\u09cd\u099f\u09be\u09ae \u09ae\u09c7\u0995\u09be\u09a8\u09bf\u0995\u09cd\u09b8\u09c7\u09b0 \u09a8\u09c0\u09a4\u09bf\u0997\u09c1\u09b2\u09bf \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u09be",
    "\u09a8\u09bf\u0989\u09b0\u09cb\u09ae\u09b0\u09cd\u09ab\u09bf\u0995 \u0995\u09ae\u09cd\u09aa\u09bf\u0989\u099f\u09bf\u0982": "\u098f\u0986\u0987 \u0995\u09be\u099c\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u09a1\u09bf\u099c\u09be\u0987\u09a8 \u0995\u09b0\u09be \u09ac\u09cd\u09b0\u09c7\u0987\u09a8-\u0985\u09a8\u09c1\u09aa\u09cd\u09b0\u09be\u09a3\u09bf\u09a4 \u0995\u09ae\u09cd\u09aa\u09bf\u0989\u099f\u09bf\u0982 \u0986\u09b0\u09cd\u0995\u09bf\u099f\u09c7\u0995\u099a\u09be\u09b0"
}