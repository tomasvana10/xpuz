{
    "\u6838\u578b": "\u751f\u7269\u4f53\u4e2d\u5b8c\u6574\u7684\u67d3\u8272\u4f53\u7ec4",
    "\u7b49\u4f4d\u57fa\u56e0": "\u57fa\u56e0\u7684\u66ff\u4ee3\u5f62\u5f0f",
    "\u540c\u6e90\u7684": "\u5916\u89c2\u76f8\u4f3c\u5e76\u643a\u5e26\u76f8\u540c\u6027\u72b6\u57fa\u56e0\u7684\u67d3\u8272\u4f53",
    "\u51cf\u6570\u5206\u88c2": "\u4ea7\u751f\u914d\u5b50\uff08\u7cbe\u5b50\u548c\u5375\u5b50\uff09\u7684\u7ec6\u80de\u5206\u88c2\u8fc7\u7a0b\uff0c\u5176\u67d3\u8272\u4f53\u6570\u91cf\u662f\u4eb2\u4ee3\u7ec6\u80de\u7684\u4e00\u534a",
    "\u6709\u4e1d\u5206\u88c2": "\u7ec6\u80de\u5206\u88c2\u8fc7\u7a0b\uff0c\u4ea7\u751f\u4e0e\u4eb2\u4ee3\u7ec6\u80de\u5177\u6709\u76f8\u540c\u67d3\u8272\u4f53\u6570\u7684\u65b0\u7ec6\u80de",
    "\u6838\u578b\u5206\u6790": "\u5206\u6790\u4e2a\u4f53\u7684\u67d3\u8272\u4f53\u4ee5\u8bc6\u522b\u5f02\u5e38",
    "\u57fa\u56e0\u578b": "\u6709\u673a\u4f53\u7684\u57fa\u56e0\u7ec4\u6210",
    "\u8868\u578b": "\u751f\u7269\u4f53\u7684\u53ef\u89c2\u5bdf\u7279\u5f81\uff0c\u7531\u57fa\u56e0\u578b\u548c\u73af\u5883\u51b3\u5b9a",
    "\u4e3b\u5bfc\u7684": "\u5f53\u5b58\u5728\u4e8e\u540c\u4e00\u67d3\u8272\u4f53\u4e0a\u65f6\u63a9\u76d6\u9690\u6027\u7b49\u4f4d\u57fa\u56e0\u5f71\u54cd\u7684\u7b49\u4f4d\u57fa\u56e0",
    "\u9690\u6027\u7684": "\u5f53\u5b58\u5728\u4e8e\u540c\u4e00\u67d3\u8272\u4f53\u4e0a\u65f6\u88ab\u663e\u6027\u7b49\u4f4d\u57fa\u56e0\u63a9\u76d6\u7684\u7b49\u4f4d\u57fa\u56e0",
    "\u5f6d\u5185\u7279\u5e7f\u573a": "\u7528\u4e8e\u9884\u6d4b\u540e\u4ee3\u9057\u4f20\u7279\u5b9a\u6027\u72b6\u7684\u6982\u7387\u7684\u56fe\u8868",
    "\u751f\u7269\u6280\u672f": "\u4f7f\u7528\u751f\u7269\u4f53\u6216\u5176\u4ea7\u54c1\u6765\u4fee\u6539\u6216\u521b\u9020\u65b0\u4ea7\u54c1\u6216\u6d41\u7a0b",
    "\u8f6c\u57fa\u56e0\u751f\u7269": "\u5176\u57fa\u56e0\u7ec4\u6210\u5df2\u4ee5\u975e\u81ea\u7136\u53d1\u751f\u65b9\u5f0f\u6539\u53d8\u7684\u751f\u7269\u4f53\u7684\u7f29\u5199",
    "\u6297\u751f\u7d20": "\u6740\u6b7b\u6216\u6291\u5236\u7ec6\u83cc\u751f\u957f\u7684\u7269\u8d28",
    "\u75c5\u6bd2": "\u4ec5\u5728\u5bbf\u4e3b\u751f\u7269\u4f53\u7ec6\u80de\u5185\u590d\u5236\u7684\u611f\u67d3\u56e0\u5b50",
    "\u75ab\u82d7": "\u63d0\u4f9b\u5bf9\u7279\u5b9a\u75be\u75c5\u7684\u514d\u75ab\u529b\u7684\u751f\u7269\u5236\u5242",
    "\u795e\u7ecf\u7cfb\u7edf": "\u5728\u5168\u8eab\u4f20\u9012\u4fe1\u606f\u7684\u7279\u6b8a\u7ec6\u80de\u7f51\u7edc",
    "\u5185\u5206\u6ccc\u7cfb\u7edf": "\u4ea7\u751f\u8c03\u8282\u5404\u79cd\u8eab\u4f53\u529f\u80fd\u7684\u6fc0\u7d20\u7684\u817a\u4f53\u7cfb\u7edf",
    "\u5faa\u73af\u7cfb\u7edf": "\u5c06\u8840\u6db2\u8f93\u9001\u81f3\u5168\u8eab\u7684\u5668\u5b98\u548c\u8840\u7ba1\u7cfb\u7edf",
    "\u514d\u75ab\u7cfb\u7edf": "\u4fdd\u62a4\u8eab\u4f53\u514d\u53d7\u75be\u75c5\u4fb5\u5bb3\u7684\u7ec6\u80de\u548c\u5668\u5b98\u7cfb\u7edf",
    "\u75c5\u539f": "\u5f15\u8d77\u75be\u75c5\u7684\u5fae\u751f\u7269\u6216\u75c5\u6bd2",
    "\u8fdb\u5316\u9002\u5e94": "\u8d4b\u4e88\u6709\u673a\u4f53\u5728\u5176\u73af\u5883\u4e2d\u751f\u5b58\u6216\u7e41\u6b96\u4f18\u52bf\u7684\u7279\u5f81",
    "\u7269\u79cd\u5f62\u6210": "\u65b0\u7269\u79cd\u4ece\u73b0\u6709\u7269\u79cd\u4ea7\u751f\u7684\u8fc7\u7a0b"
}