{
    "\u0905\u092c\u0947\u0932\u091a\u0940 \u091a\u093e\u091a\u0923\u0940": "\u0905\u0928\u0902\u0924 \u092e\u093e\u0932\u093f\u0915\u0947\u091a\u094d\u092f\u093e \u0905\u092d\u093f\u0938\u0930\u0923\u093e\u0938\u093e\u0920\u0940 \u091a\u093e\u091a\u0923\u0940\u091a\u0940 \u092a\u0926\u094d\u0927\u0924",
    "\u092a\u0930\u093f\u092a\u0942\u0930\u094d\u0923 \u0905\u092d\u093f\u0938\u0930\u0923": "\u0938\u092e\u0902\u0921\u093e\u0902\u091a\u094d\u092f\u093e \u0928\u093f\u0930\u092a\u0947\u0915\u094d\u0937 \u092e\u0942\u0932\u094d\u092f\u093e\u0902\u091a\u0940 \u092c\u0947\u0930\u0940\u091c \u092e\u0930\u094d\u092f\u093e\u0926\u093f\u0924 \u0905\u0938\u0932\u094d\u092f\u093e\u0938 \u0905\u0928\u0902\u0924 \u0936\u0943\u0902\u0916\u0932\u093e \u092a\u0942\u0930\u094d\u0923\u092a\u0923\u0947 \u0905\u092d\u093f\u0938\u0930\u0923 \u0939\u094b\u0924\u0947 \u0905\u0938\u0947 \u092e\u094d\u0939\u091f\u0932\u0947 \u091c\u093e\u0924\u0947",
    "\u092a\u0930\u093f\u092a\u0942\u0930\u094d\u0923 \u0915\u092e\u093e\u0932": "\u092b\u0902\u0915\u094d\u0936\u0928 \u092a\u094d\u0930\u093e\u092a\u094d\u0924 \u0915\u0947\u0932\u0947\u0932\u0947 \u0938\u0930\u094d\u0935\u094b\u091a\u094d\u091a \u092e\u0942\u0932\u094d\u092f",
    "\u092a\u0930\u093f\u092a\u0942\u0930\u094d\u0923 \u0915\u093f\u092e\u093e\u0928": "\u092b\u0902\u0915\u094d\u0936\u0928 \u092a\u094d\u0930\u093e\u092a\u094d\u0924 \u0915\u0947\u0932\u0947\u0932\u0947 \u0938\u0930\u094d\u0935\u093e\u0924 \u0915\u092e\u0940 \u092e\u0942\u0932\u094d\u092f",
    "\u092a\u0930\u093f\u092a\u0942\u0930\u094d\u0923 \u092e\u0942\u0932\u094d\u092f": "\u0935\u093e\u0938\u094d\u0924\u0935\u093f\u0915 \u0938\u0902\u0916\u094d\u092f\u0947\u091a\u094d\u092f\u093e \u091a\u093f\u0928\u094d\u0939\u093e\u091a\u093e \u0935\u093f\u091a\u093e\u0930 \u0928 \u0915\u0930\u0924\u093e \u0924\u094d\u092f\u093e\u091a\u0947 \u0917\u0948\u0930-\u090b\u0923\u093e\u0924\u094d\u092e\u0915 \u092e\u0942\u0932\u094d\u092f",
    "\u092a\u0930\u094d\u092f\u093e\u092f\u0940 \u092e\u093e\u0932\u093f\u0915\u093e": "\u090f\u0915 \u0905\u0928\u0902\u0924 \u092e\u093e\u0932\u093f\u0915\u093e \u091c\u094d\u092f\u093e\u091a\u094d\u092f\u093e \u0938\u0902\u091c\u094d\u091e\u093e \u0938\u0915\u093e\u0930\u093e\u0924\u094d\u092e\u0915 \u0906\u0923\u093f \u0928\u0915\u093e\u0930\u093e\u0924\u094d\u092e\u0915 \u0926\u0930\u092e\u094d\u092f\u093e\u0928 \u092a\u0930\u094d\u092f\u093e\u092f\u0940 \u0905\u0938\u0924\u093e\u0924",
    "\u092a\u0930\u094d\u092f\u093e\u092f\u0940 \u092e\u093e\u0932\u093f\u0915\u093e \u091a\u093e\u091a\u0923\u0940": "\u0928\u093f\u0930\u092a\u0947\u0915\u094d\u0937 \u092e\u0942\u0932\u094d\u092f \u0915\u092e\u0940 \u0939\u094b\u0923\u093e\u0931\u094d\u092f\u093e \u092a\u0926\u093e\u0902\u0938\u0939 \u092a\u0930\u094d\u092f\u093e\u092f\u0940 \u0936\u0943\u0902\u0916\u0932\u093e \u0939\u0940 \u0905\u092d\u093f\u0938\u0930\u0923 \u092e\u093e\u0932\u093f\u0915\u093e \u0906\u0939\u0947 \u0939\u0947 \u0938\u093f\u0926\u094d\u0927 \u0915\u0930\u0923\u094d\u092f\u093e\u0938\u093e\u0920\u0940 \u0935\u093e\u092a\u0930\u0932\u0940 \u091c\u093e\u0923\u093e\u0930\u0940 \u092a\u0926\u094d\u0927\u0924",
    "\u0935\u0932\u092f": "\u0905\u0902\u0917\u0920\u0940\u091a\u094d\u092f\u093e \u0906\u0915\u093e\u0930\u093e\u091a\u0940 \u0935\u0938\u094d\u0924\u0942, \u0926\u094b\u0928 \u0915\u0947\u0902\u0926\u094d\u0930\u093f\u0924 \u0935\u0930\u094d\u0924\u0941\u0933\u093e\u0902\u0928\u0940 \u0935\u0947\u0922\u0932\u0947\u0932\u093e \u092a\u094d\u0930\u0926\u0947\u0936",
    "\u0905\u0901\u091f\u0940\u0921\u0947\u0930\u093f\u0935\u094d\u0939\u0947\u091f\u093f\u0935\u094d\u0939": "\u090f\u0915 \u092d\u093f\u0928\u094d\u0928 \u0915\u093e\u0930\u094d\u092f \u091c\u094d\u092f\u093e\u091a\u0947 \u0935\u094d\u092f\u0941\u0924\u094d\u092a\u0928\u094d\u0928 \u092e\u0942\u0933 \u0915\u093e\u0930\u094d\u092f\u093e\u0938\u093e\u0930\u0916\u0947 \u0906\u0939\u0947",
    "\u0932\u0915\u094d\u0937\u0923\u0947": "\u090f\u0915 \u0915\u093f\u0902\u0935\u093e \u0926\u094b\u0928\u094d\u0939\u0940 x \u0915\u093f\u0902\u0935\u093e y \u0938\u092e\u0928\u094d\u0935\u092f \u0905\u0928\u0902\u0924\u093e\u0915\u0921\u0947 \u091d\u0941\u0915\u0924 \u0905\u0938\u0932\u094d\u092f\u093e\u0928\u0947 \u0935\u0915\u094d\u0930 \u0906\u0923\u093f \u0930\u0947\u0937\u093e \u092f\u093e\u0902\u091a\u094d\u092f\u093e\u0924\u0940\u0932 \u0905\u0902\u0924\u0930 \u0936\u0942\u0928\u094d\u092f\u093e\u092a\u0930\u094d\u092f\u0902\u0924 \u092a\u094b\u0939\u094b\u091a\u0947\u0932 \u0905\u0936\u0940 \u0930\u0947\u0937\u093e",
    "\u0935\u0915\u094d\u0930 \u0905\u0902\u0924\u0930\u094d\u0917\u0924 \u0915\u094d\u0937\u0947\u0924\u094d\u0930": "\u0935\u0915\u094d\u0930 y=f(x), x-\u0905\u0915\u094d\u0937 \u0906\u0923\u093f \u0909\u092d\u094d\u092f\u093e \u0930\u0947\u0937\u093e x=a \u0906\u0923\u093f x=b \u092e\u0927\u0940\u0932 \u0915\u094d\u0937\u0947\u0924\u094d\u0930\u093e\u091a\u0947 \u092a\u094d\u0930\u0924\u093f\u0928\u093f\u0927\u093f\u0924\u094d\u0935 \u0915\u0930\u0923\u093e\u0930\u093e \u0905\u0935\u093f\u092d\u093e\u091c\u094d\u092f, \u091c\u0947\u0925\u0947 a \u0906\u0923\u093f b \u090f\u0915\u0924\u094d\u0930\u0940\u0915\u0930\u0923\u093e\u091a\u094d\u092f\u093e \u092e\u0930\u094d\u092f\u093e\u0926\u093e \u0906\u0939\u0947\u0924",
    "\u0938\u093e\u0916\u0933\u0940 \u0928\u093f\u092f\u092e": "\u0938\u0902\u092e\u093f\u0936\u094d\u0930 \u092b\u0902\u0915\u094d\u0936\u0928\u091a\u0947 \u0935\u094d\u092f\u0941\u0924\u094d\u092a\u0928\u094d\u0928 \u0939\u0947 \u0906\u0924\u0940\u0932 \u092b\u0902\u0915\u094d\u0936\u0928\u091a\u094d\u092f\u093e \u0935\u094d\u092f\u0941\u0924\u094d\u092a\u0928\u094d\u0928 \u0935\u0947\u0933\u093e \u0906\u0924\u0940\u0932 \u092b\u0902\u0915\u094d\u0936\u0928\u091a\u094d\u092f\u093e \u0935\u0947\u0933\u0940 \u092e\u0942\u0932\u094d\u092f\u092e\u093e\u092a\u0928 \u0915\u0947\u0932\u0947\u0932\u094d\u092f\u093e \u092c\u093e\u0939\u094d\u092f \u0915\u093e\u0930\u094d\u092f\u093e\u091a\u0947 \u0935\u094d\u092f\u0941\u0924\u094d\u092a\u0928\u094d\u0928 \u0905\u0938\u0924\u0947",
    "\u0935\u093f\u092d\u0947\u0926\u0915 \u0915\u0945\u0932\u094d\u0915\u094d\u092f\u0941\u0932\u0938": "\u092a\u0930\u093f\u092e\u093e\u0923\u093e\u0902\u091a\u094d\u092f\u093e \u092c\u0926\u0932\u093e\u091a\u094d\u092f\u093e \u0926\u0930\u093e\u091a\u093e \u0905\u092d\u094d\u092f\u093e\u0938 \u0915\u0930\u0924\u094b. \u0935\u094d\u092f\u0941\u0924\u094d\u092a\u0928\u094d\u0928 \u0938\u092e\u0940\u0915\u0930\u0923 \u092b\u0902\u0915\u094d\u0936\u0928\u091a\u094d\u092f\u093e \u092c\u0926\u0932\u093e\u091a\u094d\u092f\u093e \u0926\u0930\u093e\u091a\u0947 \u0935\u0930\u094d\u0923\u0928 \u0915\u0930\u0923\u094d\u092f\u093e\u0938\u093e\u0920\u0940 \u0935\u093e\u092a\u0930\u0932\u0947 \u091c\u093e\u0924\u0947",
    "\u0938\u094d\u0935\u0924\u0902\u0924\u094d\u0930 \u0905\u0935\u094d\u092f\u0915\u094d\u0924": "\u092b\u0902\u0915\u094d\u0936\u0928 \u0915\u093f\u0902\u0935\u093e \u0938\u092e\u0940\u0915\u0930\u0923\u093e\u092e\u0927\u094d\u092f\u0947 \u092b\u0947\u0930\u092b\u093e\u0930 \u0915\u0947\u0932\u0947\u0932\u0947 \u092a\u094d\u0930\u092e\u093e\u0923, \u0905\u0928\u0947\u0915\u0926\u093e \u0915\u093e\u0930\u0923-\u0906\u0923\u093f-\u092a\u0930\u093f\u0923\u093e\u092e \u0938\u0902\u092c\u0902\u0927\u093e\u0924 \u0915\u093e\u0930\u0923\u093e\u091a\u0947 \u092a\u094d\u0930\u0924\u093f\u0928\u093f\u0927\u093f\u0924\u094d\u0935 \u0915\u0930\u0924\u0947",
    "\u0905\u0935\u0932\u0902\u092c\u0942\u0928 \u0935\u094d\u0939\u0947\u0930\u093f\u090f\u092c\u0932": "\u092a\u0930\u093f\u0923\u093e\u092e \u0915\u093f\u0902\u0935\u093e \u092a\u0930\u093f\u0923\u093e\u092e \u0915\u093f\u0902\u0935\u093e \u0915\u093e\u0930\u094d\u092f \u0915\u093f\u0902\u0935\u093e \u0938\u0902\u092c\u0902\u0927 \u091c\u0947 \u0907\u0924\u0930 \u092a\u094d\u0930\u092e\u093e\u0923\u093e\u0902\u091a\u094d\u092f\u093e \u092e\u0942\u0932\u094d\u092f\u093e\u0902\u0935\u0930 \u0906\u0927\u093e\u0930\u093f\u0924 \u092c\u0926\u0932\u0924\u093e\u0924",
    "\u0921\u094b\u092e\u0947\u0928": "\u092b\u0902\u0915\u094d\u0936\u0928\u0938\u093e\u0920\u0940 \u0907\u0928\u092a\u0941\u091f \u092e\u094d\u0939\u0923\u0942\u0928 \u0935\u093e\u092a\u0930\u0932\u094d\u092f\u093e \u091c\u093e\u090a \u0936\u0915\u0923\u093e\u0931\u094d\u092f\u093e \u0938\u0930\u094d\u0935 \u0938\u0902\u092d\u093e\u0935\u094d\u092f \u092e\u0942\u0932\u094d\u092f\u093e\u0902\u091a\u093e \u0938\u0902\u0926\u0930\u094d\u092d \u0926\u0947\u0924\u0947",
    "\u0936\u094d\u0930\u0947\u0923\u0940": "\u0938\u0930\u094d\u0935 \u0938\u0902\u092d\u093e\u0935\u094d\u092f \u0906\u0909\u091f\u092a\u0941\u091f\u091a\u093e \u0938\u0902\u091a \u091c\u0947 \u092b\u0902\u0915\u094d\u0936\u0928 \u0909\u0924\u094d\u092a\u093e\u0926\u0928 \u0915\u0930\u0942 \u0936\u0915\u0924\u0947",
    "\u092e\u0930\u094d\u092f\u093e\u0926\u093e": "\u092b\u0902\u0915\u094d\u0936\u0928 \u0915\u094b\u0923\u0924\u094d\u092f\u093e \u0906\u0909\u091f\u092a\u0941\u091f \u0935\u094d\u0939\u0945\u0932\u094d\u092f\u0942\u091c\u091a\u094d\u092f\u093e \u091c\u0935\u0933 \u092f\u0947\u0924\u0947 \u092f\u093e\u091a\u0947 \u0935\u0930\u094d\u0923\u0928 \u0915\u0930\u0923\u093e\u0930\u0940 \u0938\u0902\u0915\u0932\u094d\u092a\u0928\u093e \u0907\u0928\u092a\u0941\u091f\u094d\u0938 \u090f\u0915\u093e \u0935\u093f\u0936\u093f\u0937\u094d\u091f \u092e\u0942\u0932\u094d\u092f\u093e\u091a\u094d\u092f\u093e \u091c\u0935\u0933 \u092f\u0947\u0924\u093e\u0924",
    "\u0921\u0947\u0930\u093f\u0935\u094d\u0939\u0947\u091f\u093f\u0935\u094d\u0939\u094d\u091c": "\u092b\u0902\u0915\u094d\u0936\u0928\u0935\u0930\u0940\u0932 \u090f\u0915\u093e \u0935\u093f\u0936\u093f\u0937\u094d\u091f \u092c\u093f\u0902\u0926\u0942\u0935\u0930 \u0924\u093e\u0924\u094d\u0915\u093e\u0933 \u092c\u0926\u0932\u093e\u091a\u093e \u0926\u0930 \u0915\u093f\u0902\u0935\u093e \u0938\u094d\u092a\u0930\u094d\u0936\u0930\u0947\u0937\u0947\u091a\u093e \u0909\u0924\u093e\u0930 \u092e\u094b\u091c\u0923\u093e\u0930\u0940 \u0938\u0902\u0915\u0932\u094d\u092a\u0928\u093e",
    "\u0905\u0935\u093f\u092d\u093e\u091c\u094d\u092f \u0915\u0945\u0932\u094d\u0915\u094d\u092f\u0941\u0932\u0938": "\u0915\u0945\u0932\u094d\u0915\u094d\u092f\u0941\u0932\u0938\u091a\u093e \u0935\u093f\u0938\u094d\u0924\u093e\u0930 \u091c\u094d\u092f\u093e\u092e\u0927\u094d\u092f\u0947 \u0935\u094d\u092f\u0941\u0924\u094d\u092a\u0928\u094d\u0928 \u0926\u093f\u0932\u0947\u0932\u094d\u092f\u093e \u092b\u0902\u0915\u094d\u0936\u0928\u091a\u0947 \u0905\u0901\u091f\u0940\u0921\u0947\u0930\u093f\u0935\u094d\u0939\u0947\u091f\u093f\u0935\u094d\u0939 \u0936\u094b\u0927\u0923\u0947 \u0938\u092e\u093e\u0935\u093f\u0937\u094d\u091f \u0906\u0939\u0947",
    "\u0913\u0933 \u0905\u0935\u093f\u092d\u093e\u091c\u094d\u092f": "\u0935\u0915\u094d\u0930 \u0930\u0947\u0937\u0947\u0938\u0939 \u092b\u0902\u0915\u094d\u0936\u0928\u091a\u0947 \u090f\u0915\u0924\u094d\u0930\u0940\u0915\u0930\u0923",
    "\u092a\u0943\u0937\u094d\u0920\u092d\u093e\u0917 \u0905\u0935\u093f\u092d\u093e\u091c\u094d\u092f": "\u092a\u0943\u0937\u094d\u0920\u092d\u093e\u0917\u093e\u0902\u0935\u0930\u0940\u0932 \u0938\u092e\u093e\u0915\u0932\u0928\u093e\u0938\u093e\u0920\u0940 \u092d\u093f\u0928\u094d\u0928 \u0905\u0935\u093f\u092d\u093e\u091c\u094d\u092f\u093e\u0902\u091a\u0947 \u0938\u093e\u092e\u093e\u0928\u094d\u092f\u0940\u0915\u0930\u0923",
    "\u092e\u0932\u094d\u091f\u0940\u0935\u094d\u0939\u0947\u0930\u093f\u090f\u092c\u0932 \u0915\u0945\u0932\u094d\u0915\u094d\u092f\u0941\u0932\u0938": "\u090f\u0915\u093e \u0935\u094d\u0939\u0947\u0930\u093f\u090f\u092c\u0932\u092e\u0927\u0940\u0932 \u0915\u0945\u0932\u094d\u0915\u094d\u092f\u0941\u0932\u0938\u091a\u093e \u0935\u093f\u0938\u094d\u0924\u093e\u0930 \u0915\u0945\u0932\u094d\u0915\u094d\u092f\u0941\u0932\u0938\u092e\u0927\u094d\u092f\u0947 \u0905\u0928\u0947\u0915 \u0935\u094d\u0939\u0947\u0930\u093f\u090f\u092c\u0932\u094d\u0938\u091a\u094d\u092f\u093e \u092b\u0902\u0915\u094d\u0936\u0928\u094d\u0938\u0938\u0939",
    "\u0935\u0947\u0915\u094d\u091f\u0930 \u092b\u0940\u0932\u094d\u0921": "\u0905\u0902\u0924\u0930\u093e\u0933\u093e\u0924\u0940\u0932 \u090f\u0915 \u092c\u093f\u0902\u0926\u0942 \u091c\u094d\u092f\u093e\u091a\u0940 \u092a\u0930\u093f\u092e\u093e\u0923 \u0906\u0923\u093f \u0926\u093f\u0936\u093e \u0906\u0939\u0947",
    "\u0935\u0947\u0915\u094d\u091f\u0930 \u0915\u0945\u0932\u094d\u0915\u094d\u092f\u0941\u0932\u0938": "\u0915\u0945\u0932\u094d\u0915\u094d\u092f\u0941\u0932\u0938\u091a\u093e \u0935\u093f\u0938\u094d\u0924\u093e\u0930 \u091c\u094b \u0924\u094d\u0930\u093f\u092e\u093f\u0924\u0940\u092f \u092f\u0941\u0915\u094d\u0932\u093f\u0921\u093f\u092f\u0928 \u0938\u094d\u092a\u0947\u0938\u092e\u0927\u0940\u0932 \u0935\u0947\u0915\u094d\u091f\u0930 \u092b\u0940\u0932\u094d\u0921\u091a\u094d\u092f\u093e \u092d\u0947\u0926\u092d\u093e\u0935 \u0906\u0923\u093f \u090f\u0915\u0940\u0915\u0930\u0923\u093e\u0936\u0940 \u0938\u0902\u092c\u0902\u0927\u093f\u0924 \u0906\u0939\u0947",
    "\u0935\u0947\u0915\u094d\u091f\u0930 \u0915\u094d\u0937\u092e\u0924\u093e": "\u0935\u0947\u0915\u094d\u091f\u0930 \u092b\u0940\u0932\u094d\u0921 \u091c\u0947 \u0915\u0930\u094d\u0932 \u0911\u092a\u0930\u0947\u091f\u0930\u0926\u094d\u0935\u093e\u0930\u0947 \u0938\u094b\u0932\u0947\u0928\u094b\u0907\u0921\u0932 \u092b\u0940\u0932\u094d\u0921\u0932\u093e \u091c\u0928\u094d\u092e \u0926\u0947\u0924\u0947"
}