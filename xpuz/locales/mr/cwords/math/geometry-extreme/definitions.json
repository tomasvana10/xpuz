{
    "\u091c\u091f\u093f\u0932 \u092d\u0942\u092e\u093f\u0924\u0940": "\u092d\u094c\u092e\u093f\u0924\u093f\u0915 \u0938\u0902\u0930\u091a\u0928\u093e\u0902\u091a\u093e \u0905\u092d\u094d\u092f\u093e\u0938 \u0906\u0923\u093f \u0938\u0902\u092e\u093f\u0936\u094d\u0930 \u0938\u0902\u0916\u094d\u092f\u093e\u0902\u092e\u0927\u0942\u0928 \u0909\u0926\u094d\u092d\u0935\u0932\u0947\u0932\u094d\u092f\u093e \u0915\u093f\u0902\u0935\u093e \u0935\u0930\u094d\u0923\u0928 \u0915\u0947\u0932\u0947\u0932\u094d\u092f\u093e \u0930\u091a\u0928\u093e",
    "\u091c\u091f\u093f\u0932 \u0938\u0902\u0916\u094d\u092f\u093e": "a + bi \u092f\u093e \u092b\u0949\u0930\u094d\u092e\u091a\u094d\u092f\u093e \u0938\u0902\u0916\u094d\u092f\u093e \u091c\u0947\u0925\u0947 a \u0906\u0923\u093f b \u092f\u093e \u0935\u093e\u0938\u094d\u0924\u0935\u093f\u0915 \u0938\u0902\u0916\u094d\u092f\u093e \u0906\u0939\u0947\u0924",
    "\u0935\u093e\u0938\u094d\u0924\u0935\u093f\u0915 \u092d\u093e\u0917": "\u091c\u091f\u093f\u0932 \u0938\u0902\u0916\u094d\u092f\u0947\u0924\u0940\u0932 \u0938\u0902\u091c\u094d\u091e\u093e \u091c\u094d\u092f\u093e\u092e\u0927\u094d\u092f\u0947 \u0915\u093e\u0932\u094d\u092a\u0928\u093f\u0915 \u090f\u0915\u0915 \u0918\u091f\u0915 \u092e\u094d\u0939\u0923\u0942\u0928 \u0938\u092e\u093e\u0935\u093f\u0937\u094d\u091f \u0928\u093e\u0939\u0940",
    "\u0915\u093e\u0932\u094d\u092a\u0928\u093f\u0915 \u092d\u093e\u0917": "\u091c\u091f\u093f\u0932 \u0938\u0902\u0916\u094d\u092f\u0947\u091a\u093e \u092d\u093e\u0917 \u091c\u094d\u092f\u093e\u092e\u0927\u094d\u092f\u0947 \u0918\u091f\u0915 \u092e\u094d\u0939\u0923\u0942\u0928 \u0915\u093e\u0932\u094d\u092a\u0928\u093f\u0915 \u090f\u0915\u0915 \u0906\u0939\u0947",
    "\u091c\u091f\u093f\u0932 \u0905\u0928\u0947\u0915\u0935\u093f\u0927": "\u0935\u0915\u094d\u0930 \u0906\u0923\u093f \u092a\u0943\u0937\u094d\u0920\u092d\u093e\u0917\u093e\u0902\u091a\u0947 \u0938\u093e\u092e\u093e\u0928\u094d\u092f\u0940\u0915\u0930\u0923 \u0905\u0938\u0932\u0947\u0932\u094d\u092f\u093e \u091c\u091f\u093f\u0932 \u092d\u0942\u092e\u093f\u0924\u0940\u092e\u0927\u0940\u0932 \u091c\u093e\u0917\u093e",
    "\u091c\u091f\u093f\u0932 \u092c\u0940\u091c\u0917\u0923\u093f\u0924\u0940\u092f \u0935\u093e\u0923": "\u091c\u091f\u093f\u0932 \u092d\u0942\u092e\u093f\u0924\u0940\u092e\u0927\u0940\u0932 \u0930\u093f\u0915\u094d\u0924 \u0938\u094d\u0925\u093e\u0928 \u091c\u0947 \u092c\u0939\u0941\u092a\u0926\u0940\u092f \u0938\u092e\u0940\u0915\u0930\u0923\u093e\u0902\u091a\u0947 \u0928\u093f\u0930\u093e\u0915\u0930\u0923 \u0915\u0930\u0924\u093e\u0924",
    "\u0939\u094b\u0932\u094b\u092e\u0949\u0930\u094d\u092b\u093f\u0915 \u092c\u093e\u0902\u0927\u0915\u093e\u092e": "\u091c\u091f\u093f\u0932 \u092d\u0942\u092e\u093f\u0924\u0940\u092e\u0927\u0940\u0932 \u092c\u093e\u0902\u0927\u0915\u093e\u092e\u0947 \u091c\u0940 \u091c\u091f\u093f\u0932-\u092d\u093f\u0928\u094d\u0928 \u0906\u0939\u0947\u0924",
    "\u0915\u093f\u092e\u093e\u0928 \u092e\u0949\u0921\u0947\u0932 \u092a\u094d\u0930\u094b\u0917\u094d\u0930\u093e\u092e": "\u0915\u094d\u0932\u093f\u0937\u094d\u091f \u092e\u0945\u0928\u093f\u092b\u094b\u0932\u094d\u0921\u094d\u0938 \u0906\u0923\u093f \u0915\u094d\u0932\u093f\u0937\u094d\u091f \u092c\u0940\u091c\u0917\u0923\u093f\u0924\u0940\u092f \u091c\u093e\u0924\u0940\u0902\u091a\u094d\u092f\u093e \u0935\u0930\u094d\u0917\u0940\u0915\u0930\u0923\u093e\u0938\u093e\u0920\u0940 \u091c\u091f\u093f\u0932 \u092d\u0942\u092e\u093f\u0924\u0940\u092e\u0927\u0940\u0932 \u090f\u0915 \u0915\u093e\u0930\u094d\u092f\u0915\u094d\u0930\u092e",
    "\u092e\u094b\u0921\u094d\u092f\u0941\u0932\u0940 \u092e\u094b\u0915\u0933\u0940 \u091c\u093e\u0917\u093e": "\u091c\u091f\u093f\u0932 \u092d\u0942\u092e\u093f\u0924\u0940\u092e\u0927\u0940\u0932 \u0930\u093f\u0915\u094d\u0924 \u0938\u094d\u0925\u093e\u0928 \u091c\u0947 \u0915\u093e\u0939\u0940 \u092a\u094d\u0930\u0915\u093e\u0930\u091a\u094d\u092f\u093e \u092d\u094c\u092e\u093f\u0924\u093f\u0915 \u0911\u092c\u094d\u091c\u0947\u0915\u094d\u091f\u091a\u0947 \u092e\u093e\u092a\u0926\u0902\u0921 \u092c\u0928\u0935\u0924\u093e\u0924",
    "\u0915\u0945\u0932\u092c\u0940 \u0905\u0928\u0941\u092e\u093e\u0928": "\u0915\u094d\u0932\u093f\u0937\u094d\u091f \u092e\u0945\u0928\u093f\u092b\u094b\u0932\u094d\u0921\u0935\u0930 \u0935\u093f\u0936\u093f\u0937\u094d\u091f \u092a\u094d\u0930\u0915\u093e\u0930\u091a\u094d\u092f\u093e \u092e\u0947\u091f\u094d\u0930\u093f\u0915\u091a\u094d\u092f\u093e \u0905\u0938\u094d\u0924\u093f\u0924\u094d\u0935\u093e\u092c\u0926\u094d\u0926\u0932 \u0936\u093f\u0902\u0917-\u0924\u0941\u0902\u0917 \u092f\u093e\u090a \u092f\u093e\u0902\u0928\u0940 \u0938\u093f\u0926\u094d\u0927 \u0915\u0947\u0932\u0947\u0932\u0947 \u091c\u091f\u093f\u0932 \u092d\u0942\u092e\u093f\u0924\u0940\u092e\u0927\u0940\u0932 \u0905\u0928\u0941\u092e\u093e\u0928",
    "\u0939\u093f\u091a\u093f\u0928-\u0915\u094b\u092c\u093e\u092f\u093e\u0936\u0940 \u092a\u0924\u094d\u0930\u0935\u094d\u092f\u0935\u0939\u093e\u0930": "\u0938\u094d\u0925\u093f\u0930 \u0939\u094b\u0932\u094b\u092e\u0949\u0930\u094d\u092b\u093f\u0915 \u0935\u0947\u0915\u094d\u091f\u0930 \u092c\u0902\u0921\u0932 \u0906\u0923\u093f \u0935\u093f\u0936\u093f\u0937\u094d\u091f \u0935\u093f\u092d\u0947\u0926\u0915 \u0938\u092e\u0940\u0915\u0930\u0923\u093e\u091a\u0947 \u0928\u093f\u0930\u093e\u0915\u0930\u0923 \u092f\u093e\u0902\u091a\u094d\u092f\u093e\u0924\u0940\u0932 \u091c\u091f\u093f\u0932 \u092d\u0942\u092e\u093f\u0924\u0940\u092e\u0927\u0940\u0932 \u092a\u0924\u094d\u0930\u0935\u094d\u092f\u0935\u0939\u093e\u0930",
    "nonabelian hodge \u092a\u0924\u094d\u0930\u0935\u094d\u092f\u0935\u0939\u093e\u0930": "\u0935\u093f\u0936\u093f\u0937\u094d\u091f \u092a\u094d\u0930\u0915\u093e\u0930\u091a\u094d\u092f\u093e \u091c\u091f\u093f\u0932 \u092e\u0945\u0928\u093f\u092b\u094b\u0932\u094d\u0921\u094d\u0938 \u0906\u0923\u093f \u0935\u093f\u0936\u093f\u0937\u094d\u091f \u092a\u094d\u0930\u0915\u093e\u0930\u091a\u094d\u092f\u093e \u092c\u0940\u091c\u0917\u0923\u093f\u0924\u0940\u092f \u0938\u0902\u0930\u091a\u0928\u093e\u0902\u092e\u0927\u0940\u0932 \u091c\u091f\u093f\u0932 \u092d\u0942\u092e\u093f\u0924\u0940\u092e\u0927\u0940\u0932 \u092a\u0924\u094d\u0930\u0935\u094d\u092f\u0935\u0939\u093e\u0930",
    "\u0915\u0939\u0932\u0930-\u0906\u0908\u0928\u0938\u094d\u091f\u093e\u0908\u0928 \u092e\u0947\u091f\u094d\u0930\u093f\u0915\u094d\u0938": "\u091c\u091f\u093f\u0932 \u092e\u0945\u0928\u093f\u092b\u094b\u0932\u094d\u0921\u0935\u0930 \u0935\u093f\u0936\u093f\u0937\u094d\u091f \u092a\u094d\u0930\u0915\u093e\u0930\u091a\u0947 \u092e\u0947\u091f\u094d\u0930\u093f\u0915\u094d\u0938 \u091c\u0947 \u0935\u093f\u0936\u093f\u0937\u094d\u091f \u0938\u094d\u0925\u093f\u0924\u0940 \u092a\u0942\u0930\u094d\u0923 \u0915\u0930\u0924\u093e\u0924"
}