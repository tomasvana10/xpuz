{
    "\u0938\u0942\u091a\u0928\u093e \u091a\u0915\u094d\u0930": "\u090f\u0915\u091a \u0938\u0942\u091a\u0928\u093e \u0915\u093e\u0930\u094d\u092f\u093e\u0928\u094d\u0935\u093f\u0924 \u0915\u0930\u0923\u094d\u092f\u093e\u0938\u093e\u0920\u0940 CPU \u0916\u093e\u0932\u0940\u0932 \u092a\u093e\u092f\u0931\u094d\u092f\u093e",
    "\u0938\u0941\u092a\u0930\u0938\u094d\u0915\u0947\u0932\u0930 \u0906\u0930\u094d\u0915\u093f\u091f\u0947\u0915\u094d\u091a\u0930": "CPU \u0921\u093f\u091d\u093e\u0907\u0928 \u091c\u0947 \u092a\u094d\u0930\u0924\u094d\u092f\u0947\u0915 \u0918\u0921\u094d\u092f\u093e\u0933 \u091a\u0915\u094d\u0930\u093e\u0924 \u0905\u0928\u0947\u0915 \u0938\u0942\u091a\u0928\u093e \u0915\u093e\u0930\u094d\u092f\u093e\u0928\u094d\u0935\u093f\u0924 \u0915\u0930\u0942 \u0936\u0915\u0924\u0947",
    "\u0936\u093e\u0916\u093e \u0905\u0902\u0926\u093e\u091c": "\u0924\u0902\u0924\u094d\u0930 \u091c\u0947\u0925\u0947 CPU \u0938\u0936\u0930\u094d\u0924 \u0915\u094b\u0923\u0924\u094d\u092f\u093e \u092e\u093e\u0930\u094d\u0917\u093e\u0928\u0947 \u091c\u093e\u0908\u0932 \u092f\u093e\u091a\u093e \u0905\u0902\u0926\u093e\u091c \u0932\u093e\u0935\u0923\u094d\u092f\u093e\u091a\u093e \u092a\u094d\u0930\u092f\u0924\u094d\u0928 \u0915\u0930\u0924\u0947",
    "\u0928\u0949\u0930\u094d\u0925\u092c\u094d\u0930\u093f\u091c": "\u091a\u093f\u092a\u0938\u0947\u091f (\u091c\u0941\u0928\u0940 \u092a\u094d\u0930\u0923\u093e\u0932\u0940) \u091c\u094d\u092f\u093e\u0928\u0947 CPU \u0932\u093e RAM \u0906\u0923\u093f \u0939\u093e\u092f-\u0938\u094d\u092a\u0940\u0921 \u092c\u0938\u0947\u0938\u0936\u0940 \u091c\u094b\u0921\u0932\u0947",
    "\u0938\u093e\u0909\u0925\u092c\u094d\u0930\u093f\u091c": "\u091a\u093f\u092a\u0938\u0947\u091f (\u091c\u0941\u0928\u0940 \u0938\u093f\u0938\u094d\u091f\u0940\u092e) \u091c\u0940 \u0927\u0940\u092e\u0947 \u092a\u0947\u0930\u093f\u092b\u0947\u0930\u0932\u094d\u0938 \u0906\u0923\u093f I/O \u0939\u093e\u0924\u093e\u0933\u0924\u0947",
    "\u0939\u093e\u092f\u092a\u0930\u0925\u094d\u0930\u0947\u0921\u093f\u0902\u0917": "\u090f\u0915\u0932 CPU \u0915\u094b\u0930 \u092e\u0932\u094d\u091f\u093f\u092a\u0932 \u0932\u0949\u091c\u093f\u0915\u0932 \u0915\u094b\u0930 \u092e\u094d\u0939\u0923\u0942\u0928 \u0926\u093f\u0938\u0923\u094d\u092f\u093e\u091a\u0940 \u092a\u0930\u0935\u093e\u0928\u0917\u0940 \u0926\u0947\u0923\u093e\u0930\u0947 \u0924\u0902\u0924\u094d\u0930\u091c\u094d\u091e\u093e\u0928",
    "\u0927\u094b\u0915\u093e\u0926\u093e\u092f\u0915": "\u0915\u092e\u0940 \u0938\u0942\u091a\u0928\u093e \u0938\u0902\u091a \u0938\u0902\u0917\u0923\u0915 - \u0938\u094b\u092a\u094d\u092f\u093e \u0938\u0942\u091a\u0928\u093e\u0902\u0938\u0939 CPU \u0921\u093f\u091d\u093e\u0907\u0928",
    "cisc": "\u0915\u0949\u092e\u094d\u092a\u094d\u0932\u0947\u0915\u094d\u0938 \u0907\u0902\u0938\u094d\u091f\u094d\u0930\u0915\u094d\u0936\u0928 \u0938\u0947\u091f \u0915\u0949\u092e\u094d\u092a\u094d\u092f\u0941\u091f\u0930 - \u0936\u0915\u094d\u0924\u093f\u0936\u093e\u0932\u0940, \u091c\u091f\u093f\u0932 \u0938\u0942\u091a\u0928\u093e\u0902\u0938\u0939 CPU \u0921\u093f\u091d\u093e\u0907\u0928",
    "simd": "\u0938\u093f\u0902\u0917\u0932 \u0907\u0902\u0938\u094d\u091f\u094d\u0930\u0915\u094d\u0936\u0928 \u092e\u0932\u094d\u091f\u093f\u092a\u0932 \u0921\u0947\u091f\u093e, \u0938\u092e\u093e\u0902\u0924\u0930 \u0921\u0947\u091f\u093e\u0935\u0930 \u092a\u094d\u0930\u0915\u094d\u0930\u093f\u092f\u093e \u0915\u0930\u0923\u094d\u092f\u093e\u091a\u0947 \u0924\u0902\u0924\u094d\u0930",
    "\u092e\u093e\u092f\u0915\u094d\u0930\u094b\u0906\u0930\u094d\u0915\u093f\u091f\u0947\u0915\u094d\u091a\u0930": "CPU \u091a\u094d\u092f\u093e \u0938\u0942\u091a\u0928\u093e \u0938\u0947\u091f\u091a\u0947 \u0935\u093f\u0936\u093f\u0937\u094d\u091f \u0905\u0902\u092e\u0932\u092c\u091c\u093e\u0935\u0923\u0940 \u0924\u092a\u0936\u0940\u0932",
    "\u090f\u0915 \u091a\u093f\u092a \u0935\u0930 \u092a\u094d\u0930\u0923\u093e\u0932\u0940": "\u090f\u0915\u093e\u091a \u091a\u093f\u092a\u0935\u0930 \u0905\u0928\u0947\u0915 \u0918\u091f\u0915 (CPU, GPU, \u0907.) \u090f\u0915\u0924\u094d\u0930 \u0915\u0930\u0923\u0947",
    "\u090f\u092e\u094d\u092c\u0947\u0921\u0947\u0921 \u092a\u094d\u0930\u0923\u093e\u0932\u0940": "\u090f\u0915\u093e \u0935\u093f\u0936\u093f\u0937\u094d\u091f \u0915\u093e\u0930\u094d\u092f\u093e\u0938\u093e\u0920\u0940 \u0938\u092e\u0930\u094d\u092a\u093f\u0924 \u0938\u0902\u0917\u0923\u0915 \u092a\u094d\u0930\u0923\u093e\u0932\u0940, \u0905\u0928\u0947\u0915\u0926\u093e \u092e\u094b\u0920\u094d\u092f\u093e \u0909\u092a\u0915\u0930\u0923\u093e\u092e\u0927\u094d\u092f\u0947",
    "\u092b\u0930\u094d\u092e\u0935\u0947\u0905\u0930": "\u0939\u093e\u0930\u094d\u0921\u0935\u0947\u0905\u0930 \u0909\u092a\u0915\u0930\u0923\u093e\u0935\u0930 (\u091c\u0938\u0947 \u0915\u0940 BIOS) \u0915\u093e\u092f\u092e\u0938\u094d\u0935\u0930\u0942\u092a\u0940 \u0938\u093e\u0920\u0935\u0932\u0947\u0932\u0947 \u0938\u0949\u092b\u094d\u091f\u0935\u0947\u0905\u0930",
    "\u0913\u0935\u094d\u0939\u0930\u0915\u094d\u0932\u0949\u0915\u093f\u0902\u0917": "CPU \u0932\u093e \u0924\u094d\u092f\u093e\u091a\u094d\u092f\u093e \u0905\u092a\u0947\u0915\u094d\u0937\u093f\u0924 \u0935\u0948\u0936\u093f\u0937\u094d\u091f\u094d\u092f\u093e\u0902\u092a\u0947\u0915\u094d\u0937\u093e \u091c\u093e\u0938\u094d\u0924 \u0935\u0947\u0917\u093e\u0928\u0947 \u091a\u093e\u0932\u0935\u0923\u0947",
    "\u0924\u094d\u0930\u0941\u091f\u0940 \u0938\u0941\u0927\u093e\u0930\u0923\u0947 \u0915\u094b\u0921": "RAM \u092e\u0927\u0940\u0932 \u0924\u094d\u0930\u0941\u091f\u0940 \u0936\u094b\u0927\u0923\u094d\u092f\u093e\u091a\u0947 \u0906\u0923\u093f \u0938\u0941\u0927\u093e\u0930\u0923\u094d\u092f\u093e\u091a\u0947 \u0924\u0902\u0924\u094d\u0930",
    "\u0911\u0930\u094d\u0921\u0930\u092c\u093e\u0939\u0947\u0930\u091a\u0940 \u0905\u0902\u092e\u0932\u092c\u091c\u093e\u0935\u0923\u0940": "\u0924\u0902\u0924\u094d\u0930 \u091c\u0947\u0925\u0947 CPU \u0905\u0927\u093f\u0915 \u0915\u093e\u0930\u094d\u092f\u0915\u094d\u0937\u092e \u0915\u094d\u0930\u092e\u093e\u0928\u0947 \u0938\u0942\u091a\u0928\u093e \u0915\u093e\u0930\u094d\u092f\u093e\u0928\u094d\u0935\u093f\u0924 \u0915\u0930\u0924\u0947",
    "\u0915\u094d\u0935\u093e\u0902\u091f\u092e \u0938\u0902\u0917\u0923\u0928": "\u0917\u0923\u0928\u0947\u0938\u093e\u0920\u0940 \u0915\u094d\u0935\u093e\u0902\u091f\u092e \u092e\u0947\u0915\u0945\u0928\u093f\u0915\u094d\u0938\u091a\u0940 \u0924\u0924\u094d\u0924\u094d\u0935\u0947 \u0935\u093e\u092a\u0930\u0923\u0947",
    "\u0928\u094d\u092f\u0942\u0930\u094b\u092e\u0949\u0930\u094d\u092b\u093f\u0915 \u0938\u0902\u0917\u0923\u0928": "AI \u0915\u093e\u0930\u094d\u092f\u093e\u0902\u0938\u093e\u0920\u0940 \u0921\u093f\u091d\u093e\u0907\u0928 \u0915\u0947\u0932\u0947\u0932\u0947 \u092e\u0947\u0902\u0926\u0942-\u092a\u094d\u0930\u0947\u0930\u093f\u0924 \u0938\u0902\u0917\u0923\u0928 \u0906\u0930\u094d\u0915\u093f\u091f\u0947\u0915\u094d\u091a\u0930"
}