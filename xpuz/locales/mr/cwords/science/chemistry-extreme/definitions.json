{
    "\u092a\u094d\u0932\u093e\u091d\u094d\u092e\u093e": "\u091a\u093e\u0930\u094d\u091c \u0915\u0947\u0932\u0947\u0932\u0947 \u0915\u0923 (\u0906\u092f\u0928 \u0906\u0923\u093f \u0907\u0932\u0947\u0915\u094d\u091f\u094d\u0930\u0949\u0928) \u0905\u0938\u0932\u0947\u0932\u094d\u092f\u093e \u092a\u0926\u093e\u0930\u094d\u0925\u093e\u091a\u0940 \u0938\u094d\u0925\u093f\u0924\u0940",
    "\u0906\u092f\u0928\u093f\u0915": "\u0935\u093f\u0930\u0941\u0926\u094d\u0927 \u091a\u093e\u0930\u094d\u091c \u0915\u0947\u0932\u0947\u0932\u094d\u092f\u093e \u0906\u092f\u0928\u093e\u0902\u092e\u0927\u0940\u0932 \u0906\u0915\u0930\u094d\u0937\u0923\u093e\u092e\u0941\u0933\u0947 \u092c\u0902\u0927 \u0924\u092f\u093e\u0930 \u0939\u094b\u0924\u093e\u0924",
    "\u0938\u0939\u0938\u0902\u092f\u094b\u091c\u0915": "\u0905\u0923\u0942\u0902\u092e\u0927\u0940\u0932 \u0907\u0932\u0947\u0915\u094d\u091f\u094d\u0930\u0949\u0928\u094d\u0938\u091a\u094d\u092f\u093e \u0938\u093e\u092e\u093e\u092f\u093f\u0915\u0930\u0923\u093e\u0928\u0947 \u092c\u0902\u0927 \u0924\u092f\u093e\u0930 \u0939\u094b\u0924\u093e\u0924",
    "\u0927\u093e\u0924\u0942": "\u092a\u0949\u091d\u093f\u091f\u093f\u0935\u094d\u0939 \u092e\u0947\u091f\u0932 \u0915\u0945\u0936\u0928\u094d\u0938 \u0906\u0923\u093f \u0921\u093f\u0932\u094b\u0915\u0932\u093e\u0907\u091c\u094d\u0921 \u0907\u0932\u0947\u0915\u094d\u091f\u094d\u0930\u0949\u0928 \u092f\u093e\u0902\u091a\u094d\u092f\u093e\u0924\u0940\u0932 \u0906\u0915\u0930\u094d\u0937\u0923\u093e\u092e\u0941\u0933\u0947 \u092c\u0902\u0927 \u0924\u092f\u093e\u0930 \u0939\u094b\u0924\u093e\u0924",
    "\u0938\u0902\u0924\u0941\u0932\u093f\u0924 \u0938\u092e\u0940\u0915\u0930\u0923": "\u0926\u094b\u0928\u094d\u0939\u0940 \u092c\u093e\u091c\u0942\u0902\u091a\u094d\u092f\u093e \u092a\u094d\u0930\u0924\u094d\u092f\u0947\u0915 \u0918\u091f\u0915\u093e\u091a\u094d\u092f\u093e \u0905\u0923\u0942\u0902\u091a\u094d\u092f\u093e \u0938\u092e\u093e\u0928 \u0938\u0902\u0916\u094d\u092f\u0947\u0938\u0939 \u0930\u093e\u0938\u093e\u092f\u0928\u093f\u0915 \u0938\u092e\u0940\u0915\u0930\u0923",
    "\u0924\u0940\u0933": "\u092a\u0926\u093e\u0930\u094d\u0925\u093e\u091a\u0947 \u092a\u094d\u0930\u092e\u093e\u0923 \u092e\u094b\u091c\u0923\u094d\u092f\u093e\u0938\u093e\u0920\u0940 \u0935\u093e\u092a\u0930\u0932\u0947 \u091c\u093e\u0923\u093e\u0930\u0947 \u090f\u0915\u0915 (Avogadro \u091a\u094d\u092f\u093e \u0915\u0923\u093e\u0902\u091a\u0940 \u0938\u0902\u0916\u094d\u092f\u093e)",
    "\u0935\u0938\u094d\u0924\u0941\u092e\u093e\u0928 \u0938\u0902\u0916\u094d\u092f\u093e": "\u0905\u0923\u0942\u091a\u094d\u092f\u093e \u0915\u0947\u0902\u0926\u094d\u0930\u0915\u093e\u0924\u0940\u0932 \u092a\u094d\u0930\u094b\u091f\u0949\u0928 \u0906\u0923\u093f \u0928\u094d\u092f\u0942\u091f\u094d\u0930\u0949\u0928\u091a\u0940 \u092c\u0947\u0930\u0940\u091c",
    "\u0905\u0923\u0941\u0915\u094d\u0930\u092e\u093e\u0902\u0915": "\u0905\u0923\u0942\u091a\u094d\u092f\u093e \u0915\u0947\u0902\u0926\u094d\u0930\u0915\u093e\u0924\u0940\u0932 \u092a\u094d\u0930\u094b\u091f\u0949\u0928\u091a\u0940 \u0938\u0902\u0916\u094d\u092f\u093e (\u0918\u091f\u0915\u093e\u0902\u091a\u0940 \u0913\u0933\u0916 \u0920\u0930\u0935\u0924\u0947)",
    "\u0938\u092e\u0938\u094d\u0925\u093e\u0928\u093f\u0915": "\u0928\u094d\u092f\u0942\u091f\u094d\u0930\u0949\u0928\u091a\u094d\u092f\u093e \u092d\u093f\u0928\u094d\u0928 \u0938\u0902\u0916\u094d\u092f\u0947\u0938\u0939 \u0938\u092e\u093e\u0928 \u0918\u091f\u0915\u093e\u091a\u0947 \u0905\u0923\u0942",
    "\u0911\u0915\u094d\u0938\u093f\u0921\u0947\u0936\u0928": "\u0905\u0923\u0942 \u0915\u093f\u0902\u0935\u093e \u0930\u0947\u0923\u0942\u092e\u0927\u0942\u0928 \u0907\u0932\u0947\u0915\u094d\u091f\u094d\u0930\u0949\u0928\u091a\u0947 \u0928\u0941\u0915\u0938\u093e\u0928",
    "\u0915\u092a\u093e\u0924": "\u0905\u0923\u0942 \u0915\u093f\u0902\u0935\u093e \u0930\u0947\u0923\u0942\u0926\u094d\u0935\u093e\u0930\u0947 \u0907\u0932\u0947\u0915\u094d\u091f\u094d\u0930\u0949\u0928 \u092e\u093f\u0933\u0935\u0923\u0947",
    "\u0930\u0947\u0921\u0949\u0915\u094d\u0938": "\u0930\u093e\u0938\u093e\u092f\u0928\u093f\u0915 \u0905\u092d\u093f\u0915\u094d\u0930\u093f\u092f\u093e \u091c\u094d\u092f\u093e\u092e\u0927\u094d\u092f\u0947 \u0911\u0915\u094d\u0938\u093f\u0921\u0947\u0936\u0928 \u0906\u0923\u093f \u0918\u091f \u092f\u093e \u0926\u094b\u0928\u094d\u0939\u0940 \u092a\u094d\u0930\u0915\u094d\u0930\u093f\u092f\u093e\u0902\u091a\u093e \u0938\u092e\u093e\u0935\u0947\u0936 \u0939\u094b\u0924\u094b",
    "\u0938\u0915\u094d\u0930\u093f\u092f\u0924\u093e \u090a\u0930\u094d\u091c\u093e": "\u0930\u093e\u0938\u093e\u092f\u0928\u093f\u0915 \u0905\u092d\u093f\u0915\u094d\u0930\u093f\u092f\u093e \u0918\u0921\u0923\u094d\u092f\u093e\u0938\u093e\u0920\u0940 \u0906\u0935\u0936\u094d\u092f\u0915 \u0915\u093f\u092e\u093e\u0928 \u090a\u0930\u094d\u091c\u093e",
    "\u0909\u0924\u094d\u092a\u094d\u0930\u0947\u0930\u0915": "\u092a\u0926\u093e\u0930\u094d\u0925 \u091c\u0947 \u0938\u0947\u0935\u0928 \u0928 \u0915\u0930\u0924\u093e \u0930\u093e\u0938\u093e\u092f\u0928\u093f\u0915 \u0905\u092d\u093f\u0915\u094d\u0930\u093f\u092f\u093e\u091a\u093e \u0926\u0930 \u0935\u093e\u0922\u0935\u0924\u0947",
    "\u0925\u0930\u094d\u092e\u094b\u0921\u093e\u092f\u0928\u093e\u092e\u093f\u0915\u094d\u0938": "\u0930\u0938\u093e\u092f\u0928\u0936\u093e\u0938\u094d\u0924\u094d\u0930\u093e\u091a\u0940 \u0936\u093e\u0916\u093e \u0909\u0937\u094d\u0923\u0924\u093e, \u0915\u093e\u0930\u094d\u092f, \u0924\u093e\u092a\u092e\u093e\u0928 \u0906\u0923\u093f \u0909\u0930\u094d\u091c\u0947\u0936\u0940 \u0938\u0902\u092c\u0902\u0927\u093f\u0924 \u0906\u0939\u0947",
    "\u090f\u0902\u0921\u094b\u0925\u0930\u094d\u092e\u093f\u0915": "\u0938\u092d\u094b\u0935\u0924\u093e\u0932\u091a\u0940 \u0909\u0937\u094d\u0923\u0924\u093e \u0936\u094b\u0937\u0942\u0928 \u0918\u0947\u0923\u093e\u0930\u0940 \u0930\u093e\u0938\u093e\u092f\u0928\u093f\u0915 \u092a\u094d\u0930\u0924\u093f\u0915\u094d\u0930\u093f\u092f\u093e",
    "\u090f\u0915\u094d\u091d\u094b\u0925\u0930\u094d\u092e\u093f\u0915": "\u0930\u093e\u0938\u093e\u092f\u0928\u093f\u0915 \u0905\u092d\u093f\u0915\u094d\u0930\u093f\u092f\u093e \u091c\u094d\u092f\u093e\u092e\u0941\u0933\u0947 \u0906\u091c\u0942\u092c\u093e\u091c\u0942\u0932\u093e \u0909\u0937\u094d\u0923\u0924\u093e \u0928\u093f\u0930\u094d\u092e\u093e\u0923 \u0939\u094b\u0924\u0947"
}