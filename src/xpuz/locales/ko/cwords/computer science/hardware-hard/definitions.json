{
    "\uba85\ub839\uc5b4 \uc138\ud2b8": "CPU\uac00 \uc774\ud574\ud560 \uc218 \uc788\ub294 \uba85\ub839 \ubaa8\uc74c",
    "\ub4f1\ub85d\ud558\ub2e4": "CPU \ub0b4\uc758 \uc791\uace0 \ucd08\uace0\uc18d \uba54\ubaa8\ub9ac \uc704\uce58",
    "\uc0b0\uc220 \ub17c\ub9ac \uc7a5\uce58": "\uacc4\uc0b0\uc744 \uc218\ud589\ud558\ub294 CPU \ubd80\ubd84",
    "\uc81c\uc5b4 \uc7a5\uce58": "\ucef4\ud4e8\ud130 \uad6c\uc131\uc694\uc18c\uc758 \ub3d9\uc791\uc744 \uc870\uc815\ud569\ub2c8\ub2e4.",
    "\uc77d\uae30 \uc804\uc6a9 \uba54\ubaa8\ub9ac": "\ucef4\ud4e8\ud130\ub97c \uc2dc\uc791\ud558\ub294 \ub370 \ud544\uc218\uc801\uc778 \uc9c0\uce68\uc744 \uc800\uc7a5\ud569\ub2c8\ub2e4.",
    "\uce90\uc2dc \uba54\ubaa8\ub9ac": "\ube60\ub978 \uc561\uc138\uc2a4\ub97c \uc704\ud574 CPU\uc5d0 \uac00\uae4c\uc6b4 \ub354 \uc791\uace0 \ube60\ub978 \uba54\ubaa8\ub9ac",
    "\uc2dc\uacc4 \uc18d\ub3c4": "CPU\uac00 \uba85\ub839\uc744 \uc2e4\ud589\ud558\ub294 \uc18d\ub3c4",
    "\ub2e8\uc5b4 \ud06c\uae30": "CPU\uac00 \ud55c\ubc88\uc5d0 \ucc98\ub9ac\ud560 \uc218 \uc788\ub294 \ube44\ud2b8 \uc218",
    "\uad00\ub85c": "CPU\uac00 \uba85\ub839\uc744 \uc5ec\ub7ec \ub2e8\uacc4\ub85c \ub098\ub204\uc5b4 \ud55c \ubc88\uc5d0 \uc5ec\ub7ec \uc791\uc5c5\uc744 \uc218\ud589\ud558\ub294 \uae30\uc220",
    "\ub2e4\uc911 \ucc98\ub9ac": "\uc5ec\ub7ec CPU \ub610\ub294 \ucf54\uc5b4\ub97c \uc0ac\uc6a9\ud558\uc5ec \ub3d9\uc2dc\uc5d0 \uc791\uc5c5 \uc218\ud589",
    "\ubc29\ud574\ud558\ub2e4": "\uc774\ubca4\ud2b8 \ucc98\ub9ac\ub97c \uc704\ud574 CPU\ub97c \uc77c\uc2dc\uc801\uc73c\ub85c \uc815\uc9c0\uc2dc\ud0a4\ub294 \uc2e0\ud638",
    "\uac00\uc0c1 \uba54\ubaa8\ub9ac": "\ud558\ub4dc\ub514\uc2a4\ud06c \uacf5\uac04\uc744 RAM\uc758 \ud655\uc7a5\uc73c\ub85c \ud65c\uc6a9\ud558\ub294 \uae30\uc220",
    "\ubc14\uc774\uc624\uc2a4": "\uae30\ubcf8 \uc785\ucd9c\ub825 \uc2dc\uc2a4\ud15c, \uc2dc\uc791 \uc2dc \ud558\ub4dc\uc6e8\uc5b4\ub97c \ucd08\uae30\ud654\ud558\ub294 \ud38c\uc6e8\uc5b4",
    "I/O \ud3ec\ud2b8": "\uc8fc\ubcc0 \uc7a5\uce58\uc640 \ud1b5\uc2e0\ud558\uae30 \uc704\ud55c \ucef4\ud4e8\ud130\uc758 \ud2b9\uc815 \uc8fc\uc18c",
    "\uc7a5\uce58 \ub4dc\ub77c\uc774\ubc84": "\uc6b4\uc601 \uccb4\uc81c\uac00 \uc7a5\uce58\ub97c \uc81c\uc5b4\ud560 \uc218 \uc788\ub3c4\ub85d \ud558\ub294 \uc18c\ud504\ud2b8\uc6e8\uc5b4",
    "\uc5d4\ub514\uc548": "\ubc14\uc774\ud2b8\uac00 \uba54\ubaa8\ub9ac\uc5d0 \uc800\uc7a5\ub418\ub294 \uc21c\uc11c",
    "\ub124\ud2b8\uc6cc\ud06c \ud504\ub85c\ud1a0\ucf5c": "\uc7a5\uce58 \uac04 \ud1b5\uc2e0\uc744 \uc704\ud55c \uc77c\ub828\uc758 \uaddc\uce59 \ubc0f \ud45c\uc900",
    "\ud328\ud0b7": "\ub124\ud2b8\uc6cc\ud06c\ub97c \ud1b5\ud574 \uc804\uc1a1\ub418\ub294 \uc791\uc740 \ub370\uc774\ud130 \ub2e8\uc704",
    "\ub77c\uc6b0\ud130": "\uc11c\ub85c \ub2e4\ub978 \ub124\ud2b8\uc6cc\ud06c \uac04\uc5d0 \ud328\ud0b7\uc744 \uc804\ub2ec\ud558\ub294 \uc7a5\uce58",
    "\uc11c\ube0c\ub137 \ub9c8\uc2a4\ud06c": "IP \uc8fc\uc18c\uc758 \ub124\ud2b8\uc6cc\ud06c \ubc0f \ud638\uc2a4\ud2b8 \ubd80\ubd84\uc744 \uacb0\uc815\ud558\ub294 \ub370 \uc0ac\uc6a9\ub429\ub2c8\ub2e4.",
    "\ud1a0\ud3f4\ub85c\uc9c0": "\ub124\ud2b8\uc6cc\ud06c\uc758 \uc7a5\uce58 \ubc0f \uc5f0\uacb0 \ubc30\uc5f4",
    "\ud074\ub77c\uc774\uc5b8\ud2b8 \uc11c\ubc84 \ubaa8\ub378": "\ud074\ub77c\uc774\uc5b8\ud2b8\uac00 \uc11c\ubc84\ub85c\ubd80\ud130 \ub9ac\uc18c\uc2a4\ub97c \uc694\uccad\ud558\ub294 \ub124\ud2b8\uc6cc\ud06c \uc544\ud0a4\ud14d\ucc98",
    "dma": "\uc9c1\uc811 \uba54\ubaa8\ub9ac \uc561\uc138\uc2a4\ub97c \ud1b5\ud574 \uc7a5\uce58\ub294 CPU\ub97c \uc0ac\uc6a9\ud558\uc9c0 \uc54a\uace0 \ub370\uc774\ud130\ub97c \uc804\uc1a1\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4."
}