{
    "\uc544\ubca8\uc758 \uc2dc\ud5d8": "\ubb34\ud55c \uae09\uc218\uc758 \uc218\ub834\uc744 \ud14c\uc2a4\ud2b8\ud558\ub294 \ubc29\ubc95",
    "\uc808\ub300\uc218\ub834": "\ud53c\ud569\uc758 \uc808\ub300\uac12\uc758 \ud569\uc774 \uc720\ud55c\ud558\uba74 \ubb34\ud55c \uae09\uc218\ub294 \uc808\ub300\uc801\uc73c\ub85c \uc218\ub834\ud55c\ub2e4\uace0 \ud569\ub2c8\ub2e4.",
    "\uc808\ub300 \ucd5c\ub300": "\ud568\uc218\uac00 \ub3c4\ub2ec\ud558\ub294 \uac00\uc7a5 \ub192\uc740 \uac12",
    "\uc808\ub300 \ucd5c\uc18c": "\ud568\uc218\uac00 \ub3c4\ub2ec\ud558\ub294 \uac00\uc7a5 \ub0ae\uc740 \uac12",
    "\uc808\ub300\uac12": "\ubd80\ud638\uc5d0 \uad00\uacc4\uc5c6\uc774 \uc2e4\uc218\uc758 \uc74c\uc218\uac00 \uc544\ub2cc \uac12",
    "\uad50\ub300 \uc2dc\ub9ac\uc988": "\uc591\uc218\uc640 \uc74c\uc218\uac00 \ubc88\uac08\uc544 \ub098\ud0c0\ub098\ub294 \ubb34\ud55c \uae09\uc218",
    "\uad50\ub300 \uc2dc\ub9ac\uc988 \ud14c\uc2a4\ud2b8": "\uc808\ub300\uac12\uc774 \uac10\uc18c\ud558\ub294 \ud56d\uc744 \uac16\ub294 \uad50\ub300 \uacc4\uc5f4\uc774 \uc218\ub834 \uacc4\uc5f4\uc784\uc744 \uc99d\uba85\ud558\ub294 \ub370 \uc0ac\uc6a9\ub418\ub294 \ubc29\ubc95",
    "\uace0\ub9ac": "\ub450 \uac1c\uc758 \ub3d9\uc2ec\uc6d0\uc73c\ub85c \ub458\ub7ec\uc2f8\uc778 \uc601\uc5ed\uc778 \uace0\ub9ac \ubaa8\uc591\uc758 \ubb3c\uccb4",
    "\uc5ed\ub3c4\ud568\uc218": "\ub3c4\ud568\uc218\uac00 \uc6d0\ub798 \ud568\uc218\uc640 \ub3d9\uc77c\ud55c \ubbf8\ubd84 \uac00\ub2a5 \ud568\uc218",
    "\uc810\uadfc\uc120": "x \ub610\ub294 y \uc88c\ud45c \uc911 \ud558\ub098 \ub610\ub294 \ub458 \ubaa8\ub450\uac00 \ubb34\ud55c\ub300\uc5d0 \uac00\uae4c\uc6cc\uc9c0\ub294 \uacbd\ud5a5\uc774 \uc788\uc73c\ubbc0\ub85c \uace1\uc120\uacfc \uc120 \uc0ac\uc774\uc758 \uac70\ub9ac\uac00 0\uc5d0 \uac00\uae4c\uc6cc\uc9c0\ub294 \uc120\uc785\ub2c8\ub2e4.",
    "\uace1\uc120 \uc544\ub798\uc758 \uba74\uc801": "\uace1\uc120 y=f(x), x\ucd95, \uc218\uc9c1\uc120 x=a\uc640 x=b \uc0ac\uc774\uc758 \uba74\uc801\uc744 \ub098\ud0c0\ub0b4\ub294 \uc801\ubd84\uc785\ub2c8\ub2e4. \uc5ec\uae30\uc11c a\uc640 b\ub294 \uc801\ubd84\uc758 \ud55c\uacc4\uc785\ub2c8\ub2e4.",
    "\uc5f0\uc1c4 \ubc95\uce59": "\ubcf5\ud569 \ud568\uc218\uc758 \ub3c4\ud568\uc218\ub294 \ub0b4\ubd80 \ud568\uc218\uc758 \ub3c4\ud568\uc218\uc640 \ub0b4\ubd80 \ud568\uc218\uc758 \ub3c4\ud568\uc218\ub97c \uacf1\ud558\uc5ec \uacc4\uc0b0\ub41c \uc678\ubd80 \ud568\uc218\uc758 \ub3c4\ud568\uc218\uc785\ub2c8\ub2e4.",
    "\ubbf8\ubd84\ud559": "\uc218\ub7c9\uc758 \ubcc0\ud654\uc728\uc744 \uc5f0\uad6c\ud569\ub2c8\ub2e4. \ubbf8\ubd84 \ubc29\uc815\uc2dd\uc740 \ud568\uc218\uc758 \ubcc0\ud654\uc728\uc744 \uc124\uba85\ud558\ub294 \ub370 \uc0ac\uc6a9\ub429\ub2c8\ub2e4.",
    "\ub3c5\ub9bd \ubcc0\uc218": "\ud568\uc218\ub098 \ubc29\uc815\uc2dd\uc5d0\uc11c \uc870\uc791\ub418\ub294 \uc591\uc73c\ub85c, \uc885\uc885 \uc6d0\uc778\uacfc \uacb0\uacfc \uad00\uacc4\uc758 \uc6d0\uc778\uc744 \ub098\ud0c0\ub0c5\ub2c8\ub2e4.",
    "\uc885\uc18d\ubcc0\uc218": "\ub2e4\ub978 \uc591\uc758 \uac12\uc5d0 \ub530\ub77c \ubcc0\ud654\ud558\ub294 \ud568\uc218\ub098 \uad00\uacc4\uc758 \uacb0\uacfc \ub610\ub294 \uacb0\uacfc",
    "\ub3c4\uba54\uc778": "\ud568\uc218\uc5d0 \ub300\ud55c \uc785\ub825\uc73c\ub85c \uc0ac\uc6a9\ud560 \uc218 \uc788\ub294 \ubaa8\ub4e0 \uac00\ub2a5\ud55c \uac12\uc744 \ub098\ud0c0\ub0c5\ub2c8\ub2e4.",
    "\ubc94\uc704": "\ud568\uc218\uac00 \uc0dd\uc131\ud560 \uc218 \uc788\ub294 \ubaa8\ub4e0 \uac00\ub2a5\ud55c \ucd9c\ub825\uc758 \uc9d1\ud569",
    "\ud55c\uacc4": "\uc785\ub825\uc774 \ud2b9\uc815 \uac12\uc5d0 \uac00\uae4c\uc6cc\uc9c8\uc218\ub85d \ud568\uc218\uac00 \uc5b4\ub5a4 \ucd9c\ub825 \uac12\uc5d0 \uac00\uae4c\uc6cc\uc9c0\ub294\uc9c0\ub97c \uc124\uba85\ud558\ub294 \uac1c\ub150",
    "\ud30c\uc0dd\uc0c1\ud488": "\ud568\uc218\uc758 \ud2b9\uc815\uc810\uc5d0\uc11c\uc758 \uc21c\uac04\uc801\uc778 \ubcc0\ud654\uc728\uc774\ub098 \uc811\uc120\uc758 \uae30\uc6b8\uae30\ub97c \uce21\uc815\ud558\ub294 \uac1c\ub150",
    "\uc801\ubd84\ubc95": "\ub3c4\ud568\uc218\uac00 \uc8fc\uc5b4\uc9c4 \ud568\uc218\uc758 \uc5ed\ub3c4\ud568\uc218\ub97c \ucc3e\ub294 \uac83\uacfc \uad00\ub828\ub41c \ubbf8\uc801\ubd84\ud559\uc758 \ud655\uc7a5",
    "\uc120\uc801\ubd84": "\uace1\uc120\uc744 \ub530\ub77c \ud568\uc218\ub97c \uc801\ubd84",
    "\ud45c\uba74 \uc801\ubd84": "\ud45c\uba74\uc5d0 \ub300\ud55c \uc801\ubd84\uc5d0 \ub300\ud55c \ub2e4\uc591\ud55c \uc801\ubd84\uc758 \uc77c\ubc18\ud654",
    "\ub2e4\ubcc0\uc218 \ubbf8\uc801\ubd84\ud559": "\ud558\ub098\uc758 \ubcc0\uc218\uc5d0 \ub300\ud55c \ubbf8\uc801\ubd84\uc744 \uc5ec\ub7ec \ubcc0\uc218\uc758 \ud568\uc218\ub97c \uc0ac\uc6a9\ud558\ub294 \ubbf8\uc801\ubd84\uc73c\ub85c \ud655\uc7a5",
    "\ubca1\ud130 \ud544\ub4dc": "\ud06c\uae30\uc640 \ubc29\ud5a5\uc744 \uac00\uc9c0\uace0 \uc788\ub294 \uacf5\uac04\uc758 \ud55c \uc810",
    "\ubca1\ud130 \ubbf8\uc801\ubd84\ud559": "3\ucc28\uc6d0 \uc720\ud074\ub9ac\ub4dc \uacf5\uac04\uc5d0\uc11c \ubca1\ud130\uc7a5\uc758 \ubbf8\ubd84\uacfc \uc801\ubd84\uc744 \ub2e4\ub8e8\ub294 \ubbf8\uc801\ubd84\ud559\uc758 \ud655\uc7a5",
    "\ubca1\ud130 \uc804\uc704": "\uceec \uc5f0\uc0b0\uc790\ub97c \ud1b5\ud574 \uc194\ub808\ub178\uc774\ub4dc \ud544\ub4dc\ub97c \uc0dd\uc131\ud558\ub294 \ubca1\ud130 \ud544\ub4dc"
}