{
    "\u0d8b\u0db4\u0daf\u0dd9\u0dc3\u0dca \u0da0\u0d9a\u0dca\u0dbb\u0dba": "\u0dad\u0db1\u0dd2 \u0d8b\u0db4\u0daf\u0dd9\u0dc3\u0dca \u0d9a\u0dca\u200d\u0dbb\u0dd2\u0dba\u0dcf\u0dad\u0dca\u0db8\u0d9a \u0d9a\u0dd2\u0dbb\u0dd3\u0db8\u0da7 CPU \u0d91\u0d9a \u0d85\u0db1\u0dd4\u0d9c\u0db8\u0db1\u0dba \u0d9a\u0dbb\u0db1 \u0db4\u0dd2\u0dba\u0dc0\u0dbb",
    "\u0d85\u0db0\u0dd2 \u0db4\u0dbb\u0dd2\u0db8\u0dcf\u0dab \u0d9c\u0dd8\u0dc4 \u0db1\u0dd2\u0dbb\u0dca\u0db8\u0dcf\u0dab \u0dc1\u0dd2\u0dbd\u0dca\u0db4\u0dba": "\u0d94\u0dbb\u0dbd\u0ddd\u0dc3\u0dd4 \u0da0\u0d9a\u0dca\u200d\u0dbb\u0dba\u0d9a\u0da7 \u0db6\u0dc4\u0dd4\u0dc0\u0dd2\u0db0 \u0d8b\u0db4\u0daf\u0dd9\u0dc3\u0dca \u0d9a\u0dca\u200d\u0dbb\u0dd2\u0dba\u0dcf\u0dad\u0dca\u0db8\u0d9a \u0d9a\u0dc5 \u0dc4\u0dd0\u0d9a\u0dd2 CPU \u0db1\u0dd2\u0dbb\u0dca\u0db8\u0dcf\u0dab\u0dba",
    "\u0dc1\u0dcf\u0d9b\u0dcf \u0d85\u0db1\u0dcf\u0dc0\u0dd0\u0d9a\u0dd2\u0dba": "CPU \u0d91\u0d9a \u0d9a\u0ddc\u0db1\u0dca\u0daf\u0dda\u0dc3\u0dd2\u0dba\u0d9a\u0dca \u0dba\u0db1\u0dca\u0db1\u0dda \u0d9a\u0dd4\u0db8\u0db1 \u0db4\u0dd0\u0dad\u0dca\u0dad\u0da7\u0daf\u0dd0\u0dba\u0dd2 \u0d85\u0db1\u0dd4\u0db8\u0dcf\u0db1 \u0d9a\u0dd2\u0dbb\u0dd3\u0db8\u0da7 \u0d8b\u0dad\u0dca\u0dc3\u0dcf\u0dc4 \u0d9a\u0dbb\u0db1 \u0dad\u0dcf\u0d9a\u0dca\u200d\u0dc2\u0dab\u0dba",
    "\u0d8b\u0dad\u0dd4\u0dbb\u0dd4 \u0db4\u0dcf\u0dbd\u0db8": "CPU RAM \u0dc3\u0dc4 \u0d85\u0db0\u0dd2\u0dc0\u0dda\u0d9c\u0dd3 \u0db6\u0dc3\u0dca \u0dc0\u0dbd\u0da7 \u0dc3\u0db8\u0dca\u0db6\u0db1\u0dca\u0db0 \u0d9a\u0dbb\u0db1 \u0da0\u0dd2\u0db4\u0dca\u0dc3\u0dd9\u0da7\u0dca (\u0db4\u0dd0\u0dbb\u0dab\u0dd2 \u0db4\u0daf\u0dca\u0db0\u0dad\u0dd2).",
    "\u0daf\u0d9a\u0dd4\u0dab\u0dd4 \u0db4\u0dcf\u0dbd\u0db8": "\u0db8\u0db1\u0dca\u0daf\u0d9c\u0dcf\u0db8\u0dd3 \u0db4\u0dbb\u0dca\u0dba\u0db1\u0dca\u0dad \u0dc3\u0dc4 I/O \u0dc4\u0dc3\u0dd4\u0dbb\u0dd4\u0dc0\u0db1 \u0da0\u0dd2\u0db4\u0dca\u0dc3\u0dd9\u0da7\u0dca (\u0db4\u0dd0\u0dbb\u0dab\u0dd2 \u0db4\u0daf\u0dca\u0db0\u0dad\u0dd2).",
    "\u0d85\u0db0\u0dd2 \u0db1\u0dd6\u0dbd\u0dca \u0daf\u0dd0\u0db8\u0dd3\u0db8": "\u0dad\u0db1\u0dd2 CPU \u0dc4\u0dbb\u0dba\u0d9a\u0dca \u0db6\u0dc4\u0dd4 \u0dad\u0dcf\u0dbb\u0dca\u0d9a\u0dd2\u0d9a \u0dc4\u0dbb\u0dba\u0d9a\u0dca \u0dbd\u0dd9\u0dc3 \u0daf\u0dd2\u0dc3\u0dca\u0dc0\u0dd3\u0db8\u0da7 \u0d89\u0da9 \u0dc3\u0dbd\u0dc3\u0db1 \u0dad\u0dcf\u0d9a\u0dca\u0dc2\u0dab\u0dba",
    "\u0d85\u0dc0\u0daf\u0dcf\u0db1\u0db8\u0dca": "\u0d85\u0da9\u0dd4 \u0d9a\u0dbb\u0db1 \u0dbd\u0daf \u0d8b\u0db4\u0daf\u0dd9\u0dc3\u0dca \u0d9a\u0da7\u0dca\u0da7\u0dbd \u0db4\u0dbb\u0dd2\u0d9c\u0dab\u0d9a\u0dba - \u0dc3\u0dbb\u0dbd \u0d8b\u0db4\u0daf\u0dd9\u0dc3\u0dca \u0dc3\u0dc4\u0dd2\u0dad CPU \u0db1\u0dd2\u0dbb\u0dca\u0db8\u0dcf\u0dab\u0dba",
    "cisc": "\u0dc3\u0d82\u0d9a\u0dd3\u0dbb\u0dca\u0dab \u0d8b\u0db4\u0daf\u0dd9\u0dc3\u0dca \u0d9a\u0da7\u0dca\u0da7\u0dbd \u0db4\u0dbb\u0dd2\u0d9c\u0dab\u0d9a\u0dba - \u0db6\u0dbd\u0dc0\u0dad\u0dca, \u0dc3\u0d82\u0d9a\u0dd3\u0dbb\u0dca\u0dab \u0d8b\u0db4\u0daf\u0dd9\u0dc3\u0dca \u0dc3\u0dc4\u0dd2\u0dad CPU \u0db1\u0dd2\u0dbb\u0dca\u0db8\u0dcf\u0dab\u0dba",
    "simd": "\u0dad\u0db1\u0dd2 \u0d8b\u0db4\u0daf\u0dd9\u0dc3\u0dca \u0db6\u0dc4\u0dd4 \u0daf\u0dad\u0dca\u0dad, \u0dc3\u0db8\u0dcf\u0db1\u0dca\u0dad\u0dbb\u0dc0 \u0daf\u0dad\u0dca\u0dad \u0dc3\u0dd0\u0d9a\u0dc3\u0dd3\u0db8\u0dda \u0dad\u0dcf\u0d9a\u0dca\u0dc2\u0dab\u0dba",
    "\u0d9a\u0dca\u0dc2\u0dd4\u0daf\u0dca\u0dbb \u0d9c\u0dd8\u0dc4 \u0db1\u0dd2\u0dbb\u0dca\u0db8\u0dcf\u0dab \u0dc1\u0dd2\u0dbd\u0dca\u0db4\u0dba": "CPU \u0dc4\u0dd2 \u0d8b\u0db4\u0daf\u0dd9\u0dc3\u0dca \u0db8\u0dcf\u0dbd\u0dcf\u0dc0\u0d9a \u0db1\u0dd2\u0dc1\u0dca\u0da0\u0dd2\u0dad \u0d9a\u0dca\u200d\u0dbb\u0dd2\u0dba\u0dcf\u0dad\u0dca\u0db8\u0d9a \u0d9a\u0dd2\u0dbb\u0dd3\u0db8\u0dda \u0dc0\u0dd2\u0dc3\u0dca\u0dad\u0dbb",
    "\u0da0\u0dd2\u0db4\u0dba\u0d9a\u0dca \u0db8\u0dad \u0db4\u0daf\u0dca\u0db0\u0dad\u0dd2\u0dba": "\u0dad\u0db1\u0dd2 \u0da0\u0dd2\u0db4\u0dba\u0d9a\u0dca \u0db8\u0dad \u0db6\u0dc4\u0dd4 \u0dc3\u0d82\u0dbb\u0da0\u0d9a (CPU, GPU, \u0d86\u0daf\u0dd2\u0dba) \u0d92\u0d9a\u0dcf\u0db6\u0daf\u0dca\u0db0 \u0d9a\u0dd2\u0dbb\u0dd3\u0db8",
    "\u0d9a\u0dcf\u0dc0\u0dd0\u0daf\u0dca\u0daf\u0dd6 \u0db4\u0daf\u0dca\u0db0\u0dad\u0dd2\u0dba": "\u0dc0\u0dd2\u0dc1\u0dda\u0dc2\u0dd2\u0dad \u0d9a\u0dcf\u0dbb\u0dca\u0dba\u0dba\u0d9a\u0dca \u0dc3\u0db3\u0dc4\u0dcf \u0d9a\u0dd0\u0db4 \u0dc0\u0dd6 \u0db4\u0dbb\u0dd2\u0d9c\u0dab\u0d9a \u0db4\u0daf\u0dca\u0db0\u0dad\u0dd2\u0dba\u0d9a\u0dca, \u0db6\u0ddc\u0dc4\u0ddd \u0dc0\u0dd2\u0da7 \u0dc0\u0dd2\u0dc1\u0dcf\u0dbd \u0d8b\u0db4\u0dcf\u0d82\u0d9c\u0dba\u0d9a\u0dca \u0dad\u0dd4\u0dc5",
    "\u0dc3\u0dca\u0dae\u0dd2\u0dbb\u0dcf\u0d82\u0d9c": "\u0daf\u0dd8\u0daa\u0dcf\u0d82\u0d9c \u0d8b\u0db4\u0dcf\u0d82\u0d9c\u0dba\u0d9a\u0dca \u0db8\u0dad \u0dc3\u0dca\u0dae\u0dd2\u0dbb\u0dc0\u0db8 \u0d9c\u0db6\u0da9\u0dcf \u0d9a\u0dbb \u0d87\u0dad\u0dd2 \u0db8\u0dd8\u0daf\u0dd4\u0d9a\u0dcf\u0d82\u0d9c (BIOS \u0dc0\u0dd0\u0db1\u0dd2)",
    "overclocking": "CPU \u0d91\u0d9a\u0d9a\u0dca \u0d91\u0dc4\u0dd2 \u0d85\u0db4\u0dda\u0d9a\u0dca\u0dc2\u0dd2\u0dad \u0db4\u0dd2\u0dbb\u0dd2\u0dc0\u0dd2\u0dad\u0dbb\u0dba\u0db1\u0dca\u0da7 \u0dc0\u0da9\u0dcf \u0dc0\u0dd0\u0da9\u0dd2 \u0dc0\u0dda\u0d9c\u0dba\u0d9a\u0dd2\u0db1\u0dca \u0db0\u0dcf\u0dc0\u0db1\u0dba \u0d9a\u0dd2\u0dbb\u0dd3\u0db8",
    "\u0daf\u0ddd\u0dc2 \u0db1\u0dd2\u0dc0\u0dd0\u0dbb\u0daf\u0dd2 \u0d9a\u0dd2\u0dbb\u0dd3\u0db8\u0dda \u0d9a\u0dda\u0dad\u0dba": "RAM \u0dc4\u0dd2 \u0daf\u0ddd\u0dc2 \u0dc4\u0db3\u0dd4\u0db1\u0dcf\u0d9c\u0dd9\u0db1 \u0db1\u0dd2\u0dc0\u0dd0\u0dbb\u0daf\u0dd2 \u0d9a\u0dd2\u0dbb\u0dd3\u0db8\u0dda \u0dad\u0dcf\u0d9a\u0dca\u0dc2\u0dab\u0dba",
    "\u0db4\u0dd2\u0dc5\u0dd2\u0dc0\u0dd9\u0dbd\u0dd2\u0db1\u0dca \u0d9a\u0dca\u200d\u0dbb\u0dd2\u0dba\u0dcf\u0dad\u0dca\u0db8\u0d9a \u0db1\u0ddc\u0dc0\u0dd3\u0db8": "CPU \u0d91\u0d9a\u0d9a\u0dca \u0dc0\u0da9\u0dcf\u0dad\u0dca \u0d9a\u0dcf\u0dbb\u0dca\u0dba\u0d9a\u0dca\u0dc2\u0db8 \u0d85\u0db1\u0dd4\u0db4\u0dd2\u0dc5\u0dd2\u0dc0\u0dd9\u0dbd\u0d9a\u0da7 \u0d8b\u0db4\u0daf\u0dd9\u0dc3\u0dca \u0d9a\u0dca\u200d\u0dbb\u0dd2\u0dba\u0dcf\u0dad\u0dca\u0db8\u0d9a \u0d9a\u0dbb\u0db1 \u0dad\u0dcf\u0d9a\u0dca\u200d\u0dc2\u0dab\u0dba",
    "\u0d9a\u0dca\u0dc0\u0ddc\u0db1\u0dca\u0da7\u0db8\u0dca \u0db4\u0dbb\u0dd2\u0d9c\u0dab\u0db1\u0dba": "\u0d9c\u0dab\u0db1\u0dba \u0d9a\u0dd2\u0dbb\u0dd3\u0db8 \u0dc3\u0db3\u0dc4\u0dcf \u0d9a\u0dca\u0dc0\u0ddc\u0db1\u0dca\u0da7\u0db8\u0dca \u0dba\u0dcf\u0db1\u0dca\u0dad\u0dca\u200d\u0dbb \u0dc0\u0dd2\u0daf\u0dca\u200d\u0dba\u0dcf\u0dc0\u0dda \u0db8\u0dd6\u0dbd\u0db0\u0dbb\u0dca\u0db8 \u0d8b\u0db4\u0dba\u0ddd\u0d9c\u0dd3 \u0d9a\u0dbb \u0d9c\u0dd0\u0db1\u0dd3\u0db8",
    "neuromorphic \u0db4\u0dbb\u0dd2\u0d9c\u0dab\u0d9a\u0d9a\u0dbb\u0dab\u0dba": "AI \u0d9a\u0dcf\u0dbb\u0dca\u0dba\u0dba\u0db1\u0dca \u0dc3\u0db3\u0dc4\u0dcf \u0db1\u0dd2\u0dbb\u0dca\u0db8\u0dcf\u0dab\u0dba \u0d9a\u0dbb \u0d87\u0dad\u0dd2 \u0db8\u0ddc\u0dc5\u0dba \u0d86\u0dc1\u0dca\u200d\u0dbb\u0dd2\u0dad \u0db4\u0dbb\u0dd2\u0d9c\u0dab\u0d9a \u0d9c\u0dd8\u0dc4 \u0db1\u0dd2\u0dbb\u0dca\u0db8\u0dcf\u0dab \u0dc1\u0dd2\u0dbd\u0dca\u0db4\u0dba"
}