{
    "\u30a2\u30d9\u30eb\u306e\u8a66\u7df4": "\u7121\u9650\u7d1a\u6570\u306e\u53ce\u675f\u3092\u30c6\u30b9\u30c8\u3059\u308b\u65b9\u6cd5",
    "\u7d76\u5bfe\u53ce\u675f": "\u7121\u9650\u7d1a\u6570\u306f\u3001\u88ab\u52a0\u6570\u306e\u7d76\u5bfe\u5024\u306e\u5408\u8a08\u304c\u6709\u9650\u3067\u3042\u308c\u3070\u7d76\u5bfe\u306b\u53ce\u675f\u3059\u308b\u3068\u3044\u3044\u307e\u3059\u3002",
    "\u7d76\u5bfe\u6700\u5927\u5024": "\u95a2\u6570\u304c\u9054\u6210\u3059\u308b\u6700\u9ad8\u5024",
    "\u7d76\u5bfe\u6700\u5c0f\u5024": "\u95a2\u6570\u304c\u9054\u6210\u3059\u308b\u6700\u5c0f\u5024",
    "\u7d76\u5bfe\u5024": "\u7b26\u53f7\u306b\u95a2\u4fc2\u306a\u304f\u5b9f\u6570\u306e\u975e\u8ca0\u306e\u5024",
    "\u4ea4\u4e92\u30b7\u30ea\u30fc\u30ba": "\u9805\u304c\u6b63\u3068\u8ca0\u3092\u4ea4\u4e92\u306b\u7e70\u308a\u8fd4\u3059\u7121\u9650\u7d1a\u6570",
    "\u4ea4\u4e92\u30b7\u30ea\u30fc\u30ba\u30c6\u30b9\u30c8": "\u7d76\u5bfe\u5024\u304c\u6e1b\u5c11\u3059\u308b\u9805\u3092\u542b\u3080\u4ea4\u4e92\u7cfb\u5217\u304c\u53ce\u675f\u7cfb\u5217\u3067\u3042\u308b\u3053\u3068\u3092\u8a3c\u660e\u3059\u308b\u305f\u3081\u306b\u4f7f\u7528\u3055\u308c\u308b\u65b9\u6cd5",
    "\u74b0\u72b6": "\u30ea\u30f3\u30b0\u72b6\u306e\u7269\u4f53\u30012 \u3064\u306e\u540c\u5fc3\u5186\u3067\u56f2\u307e\u308c\u305f\u9818\u57df",
    "\u9006\u8a98\u5c0e\u4f53": "\u5143\u306e\u95a2\u6570\u3068\u5c0e\u95a2\u6570\u304c\u7b49\u3057\u3044\u5fae\u5206\u53ef\u80fd\u306a\u95a2\u6570",
    "\u6f38\u8fd1\u7dda": "x \u5ea7\u6a19\u307e\u305f\u306f y \u5ea7\u6a19\u306e\u4e00\u65b9\u307e\u305f\u306f\u4e21\u65b9\u304c\u7121\u9650\u5927\u306b\u8fd1\u3065\u304f\u306b\u3064\u308c\u3066\u3001\u66f2\u7dda\u3068\u76f4\u7dda\u306e\u9593\u306e\u8ddd\u96e2\u304c\u30bc\u30ed\u306b\u8fd1\u3065\u304f\u3088\u3046\u306a\u76f4\u7dda",
    "\u66f2\u7dda\u306e\u4e0b\u306e\u9818\u57df": "\u66f2\u7dda y=f(x)\u3001x \u8ef8\u3001\u304a\u3088\u3073\u5782\u76f4\u7dda x=a \u304a\u3088\u3073 x=b \u306e\u9593\u306e\u9762\u7a4d\u3092\u8868\u3059\u7a4d\u5206\u3002\u3053\u3053\u3067\u3001a \u3068 b \u306f\u7a4d\u5206\u306e\u9650\u754c\u3067\u3059\u3002",
    "\u9023\u9396\u6cd5\u5247": "\u8907\u5408\u95a2\u6570\u306e\u5c0e\u95a2\u6570\u306f\u3001\u5185\u90e8\u95a2\u6570\u3067\u8a55\u4fa1\u3055\u308c\u305f\u5916\u90e8\u95a2\u6570\u306e\u5c0e\u95a2\u6570\u306b\u5185\u90e8\u95a2\u6570\u306e\u5c0e\u95a2\u6570\u3092\u4e57\u7b97\u3057\u305f\u3082\u306e\u3067\u3059\u3002",
    "\u5fae\u5206\u7a4d\u5206": "\u91cf\u306e\u5909\u5316\u7387\u3092\u7814\u7a76\u3057\u307e\u3059\u3002\u5fae\u5206\u65b9\u7a0b\u5f0f\u306f\u95a2\u6570\u306e\u5909\u5316\u7387\u3092\u8a18\u8ff0\u3059\u308b\u305f\u3081\u306b\u4f7f\u7528\u3055\u308c\u307e\u3059\u3002",
    "\u72ec\u7acb\u5909\u6570": "\u95a2\u6570\u307e\u305f\u306f\u65b9\u7a0b\u5f0f\u3067\u64cd\u4f5c\u3055\u308c\u308b\u91cf\u3002\u591a\u304f\u306e\u5834\u5408\u3001\u539f\u56e0\u3068\u7d50\u679c\u306e\u95a2\u4fc2\u3067\u539f\u56e0\u3092\u8868\u3057\u307e\u3059\u3002",
    "\u5f93\u5c5e\u5909\u6570": "\u4ed6\u306e\u91cf\u306e\u5024\u306b\u57fa\u3065\u3044\u3066\u5909\u5316\u3059\u308b\u95a2\u6570\u307e\u305f\u306f\u95a2\u4fc2\u306b\u304a\u3051\u308b\u7d50\u679c\u307e\u305f\u306f\u7d50\u679c",
    "\u30c9\u30e1\u30a4\u30f3": "\u95a2\u6570\u3078\u306e\u5165\u529b\u3068\u3057\u3066\u4f7f\u7528\u3067\u304d\u308b\u3059\u3079\u3066\u306e\u5024\u3092\u6307\u3057\u307e\u3059\u3002",
    "\u7bc4\u56f2": "\u95a2\u6570\u304c\u751f\u6210\u3067\u304d\u308b\u3059\u3079\u3066\u306e\u51fa\u529b\u306e\u30bb\u30c3\u30c8",
    "\u9650\u754c": "\u5165\u529b\u304c\u7279\u5b9a\u306e\u5024\u306b\u8fd1\u3065\u304f\u306b\u3064\u308c\u3066\u3001\u95a2\u6570\u304c\u3069\u306e\u3088\u3046\u306a\u51fa\u529b\u5024\u306b\u8fd1\u3065\u304f\u304b\u3092\u8aac\u660e\u3059\u308b\u6982\u5ff5",
    "\u30c7\u30ea\u30d0\u30c6\u30a3\u30d6": "\u95a2\u6570\u4e0a\u306e\u7279\u5b9a\u306e\u70b9\u306b\u304a\u3051\u308b\u77ac\u9593\u7684\u306a\u5909\u5316\u7387\u307e\u305f\u306f\u63a5\u7dda\u306e\u50be\u304d\u3092\u6e2c\u5b9a\u3059\u308b\u6982\u5ff5",
    "\u7a4d\u5206\u6cd5": "\u5c0e\u95a2\u6570\u304c\u4e0e\u3048\u3089\u308c\u308b\u95a2\u6570\u306e\u9006\u5c0e\u95a2\u6570\u3092\u898b\u3064\u3051\u308b\u3053\u3068\u3092\u542b\u3080\u5fae\u7a4d\u5206\u306e\u62e1\u5f35",
    "\u7dda\u7a4d\u5206": "\u66f2\u7dda\u306b\u6cbf\u3063\u305f\u95a2\u6570\u306e\u7a4d\u5206",
    "\u9762\u7a4d\u5206": "\u3055\u307e\u3056\u307e\u306a\u7a4d\u5206\u306e\u8868\u9762\u4e0a\u306e\u7a4d\u5206\u3078\u306e\u4e00\u822c\u5316",
    "\u591a\u5909\u6570\u5fae\u7a4d\u5206": "1 \u5909\u6570\u306e\u5fae\u7a4d\u5206\u3092\u8907\u6570\u306e\u5909\u6570\u306e\u95a2\u6570\u3092\u4f7f\u7528\u3057\u305f\u5fae\u7a4d\u5206\u306b\u62e1\u5f35\u3057\u305f\u3082\u306e",
    "\u30d9\u30af\u30c8\u30eb\u30d5\u30a3\u30fc\u30eb\u30c9": "\u5927\u304d\u3055\u3068\u65b9\u5411\u3092\u6301\u3064\u7a7a\u9593\u5185\u306e\u70b9",
    "\u30d9\u30af\u30c8\u30eb\u5fae\u7a4d\u5206": "3 \u6b21\u5143\u30e6\u30fc\u30af\u30ea\u30c3\u30c9\u7a7a\u9593\u306b\u304a\u3051\u308b\u30d9\u30af\u30c8\u30eb\u5834\u306e\u5fae\u5206\u3068\u7a4d\u5206\u3092\u6271\u3046\u5fae\u7a4d\u5206\u306e\u62e1\u5f35",
    "\u30d9\u30af\u30c8\u30eb\u30dd\u30c6\u30f3\u30b7\u30e3\u30eb": "\u30ab\u30fc\u30eb\u6f14\u7b97\u5b50\u3092\u901a\u3058\u3066\u30bd\u30ec\u30ce\u30a4\u30c9\u5834\u3092\u751f\u3058\u3055\u305b\u308b\u30d9\u30af\u30c8\u30eb\u5834"
}