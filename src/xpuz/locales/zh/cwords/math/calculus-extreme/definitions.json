{
    "\u963f\u8d1d\u5c14\u7684\u6d4b\u8bd5": "\u65e0\u7a77\u7ea7\u6570\u6536\u655b\u6027\u7684\u68c0\u9a8c\u65b9\u6cd5",
    "\u7edd\u5bf9\u6536\u655b": "\u5982\u679c\u88ab\u52a0\u6570\u7684\u7edd\u5bf9\u503c\u4e4b\u548c\u662f\u6709\u9650\u7684\uff0c\u5219\u79f0\u65e0\u9650\u7ea7\u6570\u7edd\u5bf9\u6536\u655b",
    "\u7edd\u5bf9\u6700\u5927\u503c": "\u51fd\u6570\u83b7\u5f97\u7684\u6700\u9ad8\u503c",
    "\u7edd\u5bf9\u6700\u5c0f\u503c": "\u51fd\u6570\u8fbe\u5230\u7684\u6700\u4f4e\u503c",
    "\u7edd\u5bf9\u503c": "\u5b9e\u6570\u7684\u975e\u8d1f\u503c\uff0c\u4e0d\u8003\u8651\u5176\u7b26\u53f7",
    "\u4ea4\u66ff\u7cfb\u5217": "\u5176\u9879\u5728\u6b63\u8d1f\u4e4b\u95f4\u4ea4\u66ff\u7684\u65e0\u9650\u7ea7\u6570",
    "\u4ea4\u66ff\u4e32\u8054\u8bd5\u9a8c": "\u7528\u4e8e\u8bc1\u660e\u9879\u7684\u7edd\u5bf9\u503c\u51cf\u5c0f\u7684\u4ea4\u66ff\u7ea7\u6570\u662f\u6536\u655b\u7ea7\u6570\u7684\u65b9\u6cd5",
    "\u73af": "\u73af\u5f62\u7269\u4f53\uff0c\u7531\u4e24\u4e2a\u540c\u5fc3\u5706\u56f4\u6210\u7684\u533a\u57df",
    "\u53cd\u5bfc\u6570": "\u53ef\u5fae\u51fd\u6570\uff0c\u5176\u5bfc\u6570\u7b49\u4e8e\u539f\u51fd\u6570",
    "\u6e10\u8fd1\u7ebf": "\u5f53 x \u6216 y \u5750\u6807\u4e4b\u4e00\u6216\u4e24\u8005\u8d8b\u4e8e\u65e0\u7a77\u5927\u65f6\uff0c\u66f2\u7ebf\u4e0e\u76f4\u7ebf\u4e4b\u95f4\u7684\u8ddd\u79bb\u63a5\u8fd1\u4e8e\u96f6\u7684\u7ebf",
    "\u66f2\u7ebf\u4e0b\u9762\u79ef": "\u8868\u793a\u66f2\u7ebf y=f(x)\u3001x \u8f74\u548c\u5782\u76f4\u7ebf x=a \u548c x=b \u4e4b\u95f4\u9762\u79ef\u7684\u79ef\u5206\uff0c\u5176\u4e2d a \u548c b \u662f\u79ef\u5206\u6781\u9650",
    "\u94fe\u5f0f\u6cd5\u5219": "\u590d\u5408\u51fd\u6570\u7684\u5bfc\u6570\u662f\u5728\u5185\u90e8\u51fd\u6570\u5904\u8ba1\u7b97\u7684\u5916\u90e8\u51fd\u6570\u7684\u5bfc\u6570\u4e58\u4ee5\u5185\u90e8\u51fd\u6570\u7684\u5bfc\u6570",
    "\u5fae\u5206\u5b66": "\u7814\u7a76\u6570\u91cf\u7684\u53d8\u5316\u7387\u3002\u5bfc\u6570\u65b9\u7a0b\u7528\u4e8e\u63cf\u8ff0\u51fd\u6570\u7684\u53d8\u5316\u7387",
    "\u81ea\u53d8\u91cf": "\u5728\u51fd\u6570\u6216\u65b9\u7a0b\u4e2d\u64cd\u4f5c\u7684\u91cf\uff0c\u901a\u5e38\u8868\u793a\u56e0\u679c\u5173\u7cfb\u4e2d\u7684\u539f\u56e0",
    "\u56e0\u53d8\u91cf": "\u6839\u636e\u5176\u4ed6\u91cf\u7684\u503c\u800c\u53d8\u5316\u7684\u51fd\u6570\u6216\u5173\u7cfb\u7684\u7ed3\u679c\u6216\u7ed3\u679c",
    "\u9886\u57df": "\u6307\u53ef\u7528\u4f5c\u51fd\u6570\u8f93\u5165\u7684\u6240\u6709\u53ef\u80fd\u503c",
    "\u8303\u56f4": "\u51fd\u6570\u53ef\u4ee5\u4ea7\u751f\u7684\u6240\u6709\u53ef\u80fd\u8f93\u51fa\u7684\u96c6\u5408",
    "\u9650\u5236": "\u63cf\u8ff0\u5f53\u8f93\u5165\u63a5\u8fd1\u67d0\u4e2a\u503c\u65f6\u51fd\u6570\u7684\u8f93\u51fa\u503c\u66f4\u63a5\u8fd1\u7684\u6982\u5ff5",
    "\u884d\u751f\u54c1": "\u6d4b\u91cf\u51fd\u6570\u4e0a\u67d0\u4e00\u70b9\u7684\u77ac\u65f6\u53d8\u5316\u7387\u6216\u5207\u7ebf\u659c\u7387\u7684\u6982\u5ff5",
    "\u79ef\u5206\u5b66": "\u5fae\u79ef\u5206\u7684\u6269\u5c55\uff0c\u6d89\u53ca\u6c42\u5bfc\u6570\u7ed9\u5b9a\u7684\u51fd\u6570\u7684\u53cd\u5bfc\u6570",
    "\u7ebf\u79ef\u5206": "\u51fd\u6570\u6cbf\u66f2\u7ebf\u7684\u79ef\u5206",
    "\u8868\u9762\u79ef\u5206": "\u4e0d\u540c\u79ef\u5206\u5230\u66f2\u9762\u79ef\u5206\u7684\u63a8\u5e7f",
    "\u591a\u53d8\u91cf\u5fae\u79ef\u5206": "\u4e00\u53d8\u91cf\u5fae\u79ef\u5206\u6269\u5c55\u5230\u591a\u53d8\u91cf\u51fd\u6570\u5fae\u79ef\u5206",
    "\u77e2\u91cf\u573a": "\u7a7a\u95f4\u4e2d\u5177\u6709\u5927\u5c0f\u548c\u65b9\u5411\u7684\u70b9",
    "\u5411\u91cf\u5fae\u79ef\u5206": "\u5fae\u79ef\u5206\u7684\u6269\u5c55\uff0c\u5904\u7406\u4e09\u7ef4\u6b27\u51e0\u91cc\u5fb7\u7a7a\u95f4\u4e2d\u5411\u91cf\u573a\u7684\u5fae\u5206\u548c\u79ef\u5206",
    "\u77e2\u91cf\u52bf": "\u901a\u8fc7\u65cb\u5ea6\u7b97\u5b50\u4ea7\u751f\u87ba\u7ebf\u7ba1\u573a\u7684\u77e2\u91cf\u573a"
}