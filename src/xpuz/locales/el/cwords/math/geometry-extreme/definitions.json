{
    "\u03c3\u03cd\u03bd\u03b8\u03b5\u03c4\u03b7 \u03b3\u03b5\u03c9\u03bc\u03b5\u03c4\u03c1\u03af\u03b1": "\u0397 \u03bc\u03b5\u03bb\u03ad\u03c4\u03b7 \u03c4\u03c9\u03bd \u03b3\u03b5\u03c9\u03bc\u03b5\u03c4\u03c1\u03b9\u03ba\u03ce\u03bd \u03b4\u03bf\u03bc\u03ce\u03bd \u03ba\u03b1\u03b9 \u03ba\u03b1\u03c4\u03b1\u03c3\u03ba\u03b5\u03c5\u03ce\u03bd \u03c0\u03bf\u03c5 \u03c0\u03c1\u03bf\u03ba\u03cd\u03c0\u03c4\u03bf\u03c5\u03bd \u03b1\u03c0\u03cc \u03ae \u03c0\u03b5\u03c1\u03b9\u03b3\u03c1\u03ac\u03c6\u03bf\u03bd\u03c4\u03b1\u03b9 \u03b1\u03c0\u03cc \u03c4\u03bf\u03c5\u03c2 \u03bc\u03b9\u03b3\u03b1\u03b4\u03b9\u03ba\u03bf\u03cd\u03c2 \u03b1\u03c1\u03b9\u03b8\u03bc\u03bf\u03cd\u03c2",
    "\u03bc\u03b9\u03b3\u03b1\u03b4\u03b9\u03ba\u03bf\u03af \u03b1\u03c1\u03b9\u03b8\u03bc\u03bf\u03af": "\u0391\u03c1\u03b9\u03b8\u03bc\u03bf\u03af \u03c4\u03b7\u03c2 \u03bc\u03bf\u03c1\u03c6\u03ae\u03c2 a + bi \u03cc\u03c0\u03bf\u03c5 \u03bf\u03b9 a \u03ba\u03b1\u03b9 b \u03b5\u03af\u03bd\u03b1\u03b9 \u03c0\u03c1\u03b1\u03b3\u03bc\u03b1\u03c4\u03b9\u03ba\u03bf\u03af \u03b1\u03c1\u03b9\u03b8\u03bc\u03bf\u03af",
    "\u03c0\u03c1\u03b1\u03b3\u03bc\u03b1\u03c4\u03b9\u03ba\u03cc \u03bc\u03ad\u03c1\u03bf\u03c2": "\u039f \u03cc\u03c1\u03bf\u03c2 \u03c3\u03b5 \u03bc\u03b9\u03b3\u03b1\u03b4\u03b9\u03ba\u03cc \u03b1\u03c1\u03b9\u03b8\u03bc\u03cc \u03c0\u03bf\u03c5 \u03b4\u03b5\u03bd \u03c0\u03b5\u03c1\u03b9\u03ad\u03c7\u03b5\u03b9 \u03c4\u03b7 \u03c6\u03b1\u03bd\u03c4\u03b1\u03c3\u03c4\u03b9\u03ba\u03ae \u03bc\u03bf\u03bd\u03ac\u03b4\u03b1 \u03c9\u03c2 \u03c0\u03b1\u03c1\u03ac\u03b3\u03bf\u03bd\u03c4\u03b1",
    "\u03c6\u03b1\u03bd\u03c4\u03b1\u03c3\u03c4\u03b9\u03ba\u03cc \u03bc\u03ad\u03c1\u03bf\u03c2": "\u03a4\u03bf \u03bc\u03ad\u03c1\u03bf\u03c2 \u03b5\u03bd\u03cc\u03c2 \u03bc\u03b9\u03b3\u03b1\u03b4\u03b9\u03ba\u03bf\u03cd \u03b1\u03c1\u03b9\u03b8\u03bc\u03bf\u03cd \u03c0\u03bf\u03c5 \u03ad\u03c7\u03b5\u03b9 \u03c9\u03c2 \u03c0\u03b1\u03c1\u03ac\u03b3\u03bf\u03bd\u03c4\u03b1 \u03c4\u03b7 \u03c6\u03b1\u03bd\u03c4\u03b1\u03c3\u03c4\u03b9\u03ba\u03ae \u03bc\u03bf\u03bd\u03ac\u03b4\u03b1",
    "\u03c3\u03cd\u03bd\u03b8\u03b5\u03c4\u03b5\u03c2 \u03c0\u03bf\u03bb\u03bb\u03b1\u03c0\u03bb\u03ad\u03c2": "\u03a7\u03ce\u03c1\u03bf\u03b9 \u03c3\u03b5 \u03c3\u03cd\u03bd\u03b8\u03b5\u03c4\u03b7 \u03b3\u03b5\u03c9\u03bc\u03b5\u03c4\u03c1\u03af\u03b1 \u03c0\u03bf\u03c5 \u03b5\u03af\u03bd\u03b1\u03b9 \u03b3\u03b5\u03bd\u03b9\u03ba\u03b5\u03cd\u03c3\u03b5\u03b9\u03c2 \u03ba\u03b1\u03bc\u03c0\u03c5\u03bb\u03ce\u03bd \u03ba\u03b1\u03b9 \u03b5\u03c0\u03b9\u03c6\u03b1\u03bd\u03b5\u03b9\u03ce\u03bd",
    "\u03c3\u03cd\u03bd\u03b8\u03b5\u03c4\u03b5\u03c2 \u03b1\u03bb\u03b3\u03b5\u03b2\u03c1\u03b9\u03ba\u03ad\u03c2 \u03c0\u03bf\u03b9\u03ba\u03b9\u03bb\u03af\u03b5\u03c2": "\u03a7\u03ce\u03c1\u03bf\u03b9 \u03c3\u03cd\u03bd\u03b8\u03b5\u03c4\u03b7\u03c2 \u03b3\u03b5\u03c9\u03bc\u03b5\u03c4\u03c1\u03af\u03b1\u03c2 \u03c0\u03bf\u03c5 \u03b5\u03af\u03bd\u03b1\u03b9 \u03bb\u03cd\u03c3\u03b5\u03b9\u03c2 \u03c0\u03bf\u03bb\u03c5\u03c9\u03bd\u03c5\u03bc\u03b9\u03ba\u03ce\u03bd \u03b5\u03be\u03b9\u03c3\u03ce\u03c3\u03b5\u03c9\u03bd",
    "\u03bf\u03bb\u03bf\u03bc\u03bf\u03c1\u03c6\u03b9\u03ba\u03ad\u03c2 \u03ba\u03b1\u03c4\u03b1\u03c3\u03ba\u03b5\u03c5\u03ad\u03c2": "\u039a\u03b1\u03c4\u03b1\u03c3\u03ba\u03b5\u03c5\u03ad\u03c2 \u03c3\u03b5 \u03c3\u03cd\u03bd\u03b8\u03b5\u03c4\u03b7 \u03b3\u03b5\u03c9\u03bc\u03b5\u03c4\u03c1\u03af\u03b1 \u03c0\u03bf\u03c5 \u03b5\u03af\u03bd\u03b1\u03b9 \u03bc\u03b9\u03b3\u03b1\u03b4\u03b9\u03ba\u03ad\u03c2-\u03b4\u03b9\u03b1\u03c6\u03bf\u03c1\u03bf\u03c0\u03bf\u03b9\u03ae\u03c3\u03b9\u03bc\u03b5\u03c2",
    "\u03c0\u03c1\u03cc\u03b3\u03c1\u03b1\u03bc\u03bc\u03b1 \u03b5\u03bb\u03ac\u03c7\u03b9\u03c3\u03c4\u03bf\u03c5 \u03bc\u03bf\u03bd\u03c4\u03ad\u03bb\u03bf\u03c5": "\u0388\u03bd\u03b1 \u03c0\u03c1\u03cc\u03b3\u03c1\u03b1\u03bc\u03bc\u03b1 \u03c3\u03cd\u03bd\u03b8\u03b5\u03c4\u03b7\u03c2 \u03b3\u03b5\u03c9\u03bc\u03b5\u03c4\u03c1\u03af\u03b1\u03c2 \u03b3\u03b9\u03b1 \u03c4\u03b7\u03bd \u03c4\u03b1\u03be\u03b9\u03bd\u03cc\u03bc\u03b7\u03c3\u03b7 \u03c3\u03cd\u03bd\u03b8\u03b5\u03c4\u03c9\u03bd \u03c0\u03bf\u03bb\u03bb\u03b1\u03c0\u03bb\u03ce\u03bd \u03ba\u03b1\u03b9 \u03c3\u03cd\u03bd\u03b8\u03b5\u03c4\u03c9\u03bd \u03b1\u03bb\u03b3\u03b5\u03b2\u03c1\u03b9\u03ba\u03ce\u03bd \u03c0\u03bf\u03b9\u03ba\u03b9\u03bb\u03b9\u03ce\u03bd",
    "moduli \u03c7\u03ce\u03c1\u03bf\u03c5\u03c2": "\u03a7\u03ce\u03c1\u03bf\u03b9 \u03c3\u03b5 \u03c3\u03cd\u03bd\u03b8\u03b5\u03c4\u03b7 \u03b3\u03b5\u03c9\u03bc\u03b5\u03c4\u03c1\u03af\u03b1 \u03c0\u03bf\u03c5 \u03c0\u03b1\u03c1\u03b1\u03bc\u03b5\u03c4\u03c1\u03bf\u03c0\u03bf\u03b9\u03bf\u03cd\u03bd \u03ba\u03ac\u03c0\u03bf\u03b9\u03bf \u03c4\u03cd\u03c0\u03bf \u03b3\u03b5\u03c9\u03bc\u03b5\u03c4\u03c1\u03b9\u03ba\u03bf\u03cd \u03b1\u03bd\u03c4\u03b9\u03ba\u03b5\u03b9\u03bc\u03ad\u03bd\u03bf\u03c5",
    "calabi \u03b5\u03b9\u03ba\u03b1\u03c3\u03af\u03b1": "\u039c\u03b9\u03b1 \u03b5\u03b9\u03ba\u03b1\u03c3\u03af\u03b1 \u03c3\u03c4\u03b7 \u03c3\u03cd\u03bd\u03b8\u03b5\u03c4\u03b7 \u03b3\u03b5\u03c9\u03bc\u03b5\u03c4\u03c1\u03af\u03b1 \u03c0\u03bf\u03c5 \u03b1\u03c0\u03bf\u03b4\u03b5\u03af\u03c7\u03b8\u03b7\u03ba\u03b5 \u03b1\u03c0\u03cc \u03c4\u03bf\u03bd Shing-Tung Yau \u03c3\u03c7\u03b5\u03c4\u03b9\u03ba\u03ac \u03bc\u03b5 \u03c4\u03b7\u03bd \u03cd\u03c0\u03b1\u03c1\u03be\u03b7 \u03b5\u03bd\u03cc\u03c2 \u03b5\u03b9\u03b4\u03b9\u03ba\u03bf\u03cd \u03c4\u03cd\u03c0\u03bf\u03c5 \u03bc\u03ad\u03c4\u03c1\u03b7\u03c3\u03b7\u03c2 \u03c3\u03b5 \u03bc\u03b9\u03b1 \u03c3\u03cd\u03bd\u03b8\u03b5\u03c4\u03b7 \u03c0\u03bf\u03bb\u03bb\u03b1\u03c0\u03bb\u03cc\u03c4\u03b7\u03c4\u03b1",
    "\u03b1\u03bb\u03bb\u03b7\u03bb\u03bf\u03b3\u03c1\u03b1\u03c6\u03af\u03b1 hitchin-kobayashi": "\u039c\u03b9\u03b1 \u03b1\u03bd\u03c4\u03b9\u03c3\u03c4\u03bf\u03b9\u03c7\u03af\u03b1 \u03c3\u03b5 \u03c3\u03cd\u03bd\u03b8\u03b5\u03c4\u03b7 \u03b3\u03b5\u03c9\u03bc\u03b5\u03c4\u03c1\u03af\u03b1 \u03bc\u03b5\u03c4\u03b1\u03be\u03cd \u03c3\u03c4\u03b1\u03b8\u03b5\u03c1\u03ce\u03bd \u03bf\u03bb\u03bf\u03bc\u03bf\u03c1\u03c6\u03b9\u03ba\u03ce\u03bd \u03b4\u03b9\u03b1\u03bd\u03c5\u03c3\u03bc\u03b1\u03c4\u03b9\u03ba\u03ce\u03bd \u03b4\u03b5\u03c3\u03bc\u03af\u03b4\u03c9\u03bd \u03ba\u03b1\u03b9 \u03bb\u03cd\u03c3\u03b5\u03c9\u03bd \u03c3\u03b5 \u03bc\u03b9\u03b1 \u03bf\u03c1\u03b9\u03c3\u03bc\u03ad\u03bd\u03b7 \u03b4\u03b9\u03b1\u03c6\u03bf\u03c1\u03b9\u03ba\u03ae \u03b5\u03be\u03af\u03c3\u03c9\u03c3\u03b7",
    "nonabelian hodge \u03b1\u03bb\u03bb\u03b7\u03bb\u03bf\u03b3\u03c1\u03b1\u03c6\u03af\u03b1": "\u039c\u03b9\u03b1 \u03b1\u03bd\u03c4\u03b9\u03c3\u03c4\u03bf\u03b9\u03c7\u03af\u03b1 \u03c3\u03c4\u03b7 \u03c3\u03cd\u03bd\u03b8\u03b5\u03c4\u03b7 \u03b3\u03b5\u03c9\u03bc\u03b5\u03c4\u03c1\u03af\u03b1 \u03bc\u03b5\u03c4\u03b1\u03be\u03cd \u03bf\u03c1\u03b9\u03c3\u03bc\u03ad\u03bd\u03c9\u03bd \u03c4\u03cd\u03c0\u03c9\u03bd \u03bc\u03b9\u03b3\u03b1\u03b4\u03b9\u03ba\u03ce\u03bd \u03c0\u03bf\u03bb\u03bb\u03b1\u03c0\u03bb\u03ce\u03bd \u03ba\u03b1\u03b9 \u03bf\u03c1\u03b9\u03c3\u03bc\u03ad\u03bd\u03c9\u03bd \u03c4\u03cd\u03c0\u03c9\u03bd \u03b1\u03bb\u03b3\u03b5\u03b2\u03c1\u03b9\u03ba\u03ce\u03bd \u03b4\u03bf\u03bc\u03ce\u03bd",
    "\u03bc\u03b5\u03c4\u03c1\u03ae\u03c3\u03b5\u03b9\u03c2 kahler-einstein": "\u0395\u03b9\u03b4\u03b9\u03ba\u03bf\u03af \u03c4\u03cd\u03c0\u03bf\u03b9 \u03bc\u03b5\u03c4\u03c1\u03ae\u03c3\u03b5\u03c9\u03bd \u03c3\u03b5 \u03bc\u03b9\u03b1 \u03c3\u03cd\u03bd\u03b8\u03b5\u03c4\u03b7 \u03c0\u03bf\u03bb\u03bb\u03b1\u03c0\u03bb\u03cc\u03c4\u03b7\u03c4\u03b1 \u03c0\u03bf\u03c5 \u03b9\u03ba\u03b1\u03bd\u03bf\u03c0\u03bf\u03b9\u03bf\u03cd\u03bd \u03bc\u03b9\u03b1 \u03c3\u03c5\u03b3\u03ba\u03b5\u03ba\u03c1\u03b9\u03bc\u03ad\u03bd\u03b7 \u03c3\u03c5\u03bd\u03b8\u03ae\u03ba\u03b7"
}