{
    "\u0438\u043c\u0442\u0438\u04b3\u043e\u043d\u0438 \u04b2\u043e\u0431\u0438\u043b": "\u0423\u0441\u0443\u043b\u0438 \u0441\u0430\u043d\u04b7\u0438\u0448\u0438 \u043a\u043e\u043d\u0432\u0435\u0440\u0433\u0435\u043d\u0441\u0438\u044f\u0438 \u0441\u0438\u043b\u0441\u0438\u043b\u0430\u0438 \u0431\u0435\u043e\u0445\u0438\u0440",
    "\u043a\u043e\u043d\u0432\u0435\u0440\u0433\u0435\u043d\u0441\u0438\u044f\u0438 \u043c\u0443\u0442\u043b\u0430\u049b": "\u0421\u0438\u043b\u0441\u0438\u043b\u0430\u0438 \u0431\u0435\u043e\u0445\u0438\u0440 \u0433\u0443\u0444\u0442\u0430 \u043c\u0435\u0448\u0430\u0432\u0430\u0434, \u043a\u0438 \u0430\u0433\u0430\u0440 \u04b7\u0430\u043c\u044a\u0438 \u049b\u0438\u043c\u0430\u0442\u04b3\u043e\u0438 \u043c\u0443\u0442\u043b\u0430\u049b\u0438 \u04b7\u0430\u043c\u044a\u0448\u0443\u0434\u0430\u04b3\u043e \u043d\u0438\u04b3\u043e\u04e3 \u0431\u043e\u0448\u0430\u0434, \u043a\u043e\u043c\u0438\u043b\u0430\u043d \u0431\u0430 \u04b3\u0430\u043c \u043d\u0430\u0437\u0434\u0438\u043a \u043c\u0435\u0448\u0430\u0432\u0430\u0434.",
    "\u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u0438\u0438 \u043c\u0443\u0442\u043b\u0430\u049b": "\u0410\u0440\u0437\u0438\u0448\u0438 \u0431\u0430\u043b\u0430\u043d\u0434\u0442\u0430\u0440\u0438\u043d\u0435, \u043a\u0438 \u0444\u0443\u043d\u043a\u0441\u0438\u044f \u0431\u0430 \u0434\u0430\u0441\u0442 \u043c\u0435\u043e\u0440\u0430\u0434",
    "\u04b3\u0430\u0434\u0434\u0438 \u0430\u049b\u0430\u043b\u0438 \u043c\u0443\u0442\u043b\u0430\u049b": "\u0410\u0440\u0437\u0438\u0448\u0438 \u043f\u0430\u0441\u0442\u0442\u0430\u0440\u0438\u043d\u0435, \u043a\u0438 \u0444\u0443\u043d\u043a\u0441\u0438\u044f \u0431\u0430 \u0434\u0430\u0441\u0442 \u043c\u0435\u043e\u0440\u0430\u0434",
    "\u0430\u0440\u0437\u0438\u0448\u0438 \u043c\u0443\u0442\u043b\u0430\u049b": "\u049a\u0438\u043c\u0430\u0442\u0438 \u0493\u0430\u0439\u0440\u0438\u043c\u0430\u043d\u0444\u0438\u0438 \u0430\u0434\u0430\u0434\u0438 \u0432\u043e\u049b\u0435\u04e3 \u043d\u043e\u0432\u043e\u0431\u0430\u0441\u0442\u0430 \u0430\u0437 \u0430\u043b\u043e\u043c\u0430\u0442\u0438 \u043e\u043d",
    "\u0441\u0438\u043b\u0441\u0438\u043b\u0430\u0438 \u0430\u043b\u0442\u0435\u0440\u043d\u0430\u0442\u0438\u0432\u04e3": "\u0421\u0438\u043b\u0441\u0438\u043b\u0430\u0438 \u0431\u0435\u043e\u0445\u0438\u0440, \u043a\u0438 \u0438\u0441\u0442\u0438\u043b\u043e\u04b3\u043e\u0442\u0430\u0448 \u0431\u0430\u0439\u043d\u0438 \u043c\u0443\u0441\u0431\u0430\u0442 \u0432\u0430 \u043c\u0430\u043d\u0444\u04e3 \u0438\u0432\u0430\u0437 \u043c\u0435\u0448\u0430\u0432\u0430\u043d\u0434",
    "\u0441\u0430\u043d\u04b7\u0438\u0448\u0438 \u0441\u0438\u043b\u0441\u0438\u043b\u0430\u0438 \u0430\u043b\u0442\u0435\u0440\u043d\u0430\u0442\u0438\u0432\u04e3": "\u0423\u0441\u0443\u043b\u0438 \u0431\u0430\u0440\u043e\u0438 \u0438\u0441\u0431\u043e\u0442\u0438 \u043e\u043d, \u043a\u0438 \u0441\u0438\u043b\u0441\u0438\u043b\u0430\u0438 \u0430\u043b\u0442\u0435\u0440\u043d\u0430\u0442\u0438\u0432\u04e3 \u0431\u043e \u0438\u0441\u0442\u0438\u043b\u043e\u04b3\u04b3\u043e\u0435, \u043a\u0438 \u0430\u0440\u0437\u0438\u0448\u0438 \u043c\u0443\u0442\u043b\u0430\u049b \u043a\u0430\u043c \u043c\u0435\u0448\u0430\u0432\u0430\u0434, \u044f\u043a \u049b\u0430\u0442\u043e\u0440 \u043a\u043e\u043d\u0432\u0435\u0440\u0433\u0435\u043d\u0442\u0438\u0441\u0442",
    "\u04b3\u0430\u043b\u049b\u0430": "\u041e\u0431\u044a\u0435\u043a\u0442\u0438 \u04b3\u0430\u043b\u049b\u0430\u0448\u0430\u043a\u043b, \u043c\u0438\u043d\u0442\u0430\u049b\u0430\u0435, \u043a\u0438 \u0431\u043e \u0434\u0443 \u0434\u043e\u0438\u0440\u0430\u04b3\u043e\u0438 \u043c\u0443\u0442\u0430\u043c\u0430\u0440\u043a\u0430\u0437 \u043c\u0430\u04b3\u0434\u0443\u0434 \u0430\u0441\u0442",
    "\u0437\u0438\u0434\u0434\u0438 \u04b3\u043e\u0441\u0438\u043b\u0448\u0443\u0434\u0430": "\u0424\u0443\u043d\u043a\u0441\u0438\u044f\u0438 \u0434\u0438\u0444\u0444\u0435\u0440\u0435\u043d\u0441\u0438\u0430\u043b\u0438\u0435, \u043a\u0438 \u04b3\u043e\u0441\u0438\u043b\u0430\u0438 \u043e\u043d \u0431\u0430 \u0444\u0443\u043d\u043a\u0441\u0438\u044f\u0438 \u0430\u0441\u043b\u04e3 \u0431\u0430\u0440\u043e\u0431\u0430\u0440 \u0430\u0441\u0442",
    "\u0430\u0441\u0438\u043c\u043f\u0442\u043e\u0442": "\u0425\u0430\u0442\u0435, \u043a\u0438 \u043c\u0430\u0441\u043e\u0444\u0430\u0438 \u0431\u0430\u0439\u043d\u0438 \u043a\u0430\u04b7 \u0432\u0430 \u0445\u0430\u0442 \u0431\u0430 \u0441\u0438\u0444\u0440 \u043d\u0430\u0437\u0434\u0438\u043a \u043c\u0435\u0448\u0430\u0432\u0430\u0434, \u0437\u0435\u0440\u043e \u044f\u043a \u0451 \u04b3\u0430\u0440\u0434\u0443 \u043a\u043e\u043e\u0440\u0434\u0438\u043d\u0430\u0442\u0430\u04b3\u043e\u0438 x \u0451 y \u0431\u0430 \u0431\u0435\u043e\u0445\u0438\u0440 \u043c\u0430\u0439\u043b \u0434\u043e\u0440\u0430\u043d\u0434",
    "\u043c\u0430\u0439\u0434\u043e\u043d\u0438 \u0437\u0435\u0440\u0438 \u043a\u0430\u04b7": "\u0418\u043d\u0442\u0435\u0433\u0440\u0430\u043b, \u043a\u0438 \u043c\u0430\u0439\u0434\u043e\u043d\u0438 \u0431\u0430\u0439\u043d\u0438 \u043a\u0430\u0459\u0438 y=f(x), \u043c\u0435\u04b3\u0432\u0430\u0440\u0438 x \u0432\u0430 \u0445\u0430\u0442\u04b3\u043e\u0438 \u0430\u043c\u0443\u0434\u0438\u0438 x=a \u0432\u0430 x=b \u043c\u0435\u0431\u043e\u0448\u0430\u0434, \u043a\u0438 \u0434\u0430\u0440 \u043e\u043d a \u0432\u0430 b \u04b3\u0443\u0434\u0443\u0434\u0438 \u0438\u043d\u0442\u0435\u0433\u0440\u0430\u043b\u04e3 \u043c\u0435\u0431\u043e\u0448\u0430\u043d\u0434",
    "\u049b\u043e\u0438\u0434\u0430\u0438 \u0437\u0430\u043d\u04b7\u0438\u0440": "\u04b2\u043e\u0441\u0438\u043b\u0438 \u0444\u0443\u043d\u043a\u0441\u0438\u044f\u0438 \u0442\u0430\u0440\u043a\u0438\u0431\u04e3 \u04b3\u043e\u0441\u0438\u043b\u0438 \u0444\u0443\u043d\u043a\u0441\u0438\u044f\u0438 \u0431\u0435\u0440\u0443\u043d\u0438\u0435 \u043c\u0435\u0431\u043e\u0448\u0430\u0434, \u043a\u0438 \u0434\u0430\u0440 \u0432\u0430\u049b\u0442\u0438 \u0444\u0443\u043d\u043a\u0441\u0438\u044f\u0438 \u0434\u043e\u0445\u0438\u043b\u04e3 \u0431\u0430 \u04b3\u043e\u0441\u0438\u043b\u0438 \u0444\u0443\u043d\u043a\u0441\u0438\u044f\u0438 \u0434\u043e\u0445\u0438\u043b\u04e3 \u0431\u0430\u04b3\u043e \u0434\u043e\u0434\u0430 \u043c\u0435\u0448\u0430\u0432\u0430\u0434",
    "\u04b3\u0438\u0441\u043e\u0431\u04b3\u043e\u0438 \u0434\u0438\u0444\u0444\u0435\u0440\u0435\u043d\u0441\u0438\u0430\u043b\u04e3": "\u0421\u0443\u0440\u044a\u0430\u0442\u0438 \u0442\u0430\u0493\u0438\u0440\u0451\u0431\u0438\u0438 \u043c\u0438\u049b\u0434\u043e\u0440\u04b3\u043e\u0440\u043e \u043c\u0435\u043e\u043c\u04ef\u0437\u0430\u0434. \u041c\u0443\u043e\u0434\u0438\u043b\u0430\u0438 \u04b3\u043e\u0441\u0438\u043b\u0430\u0432\u04e3 \u0431\u0430\u0440\u043e\u0438 \u0442\u0430\u0432\u0441\u0438\u0444\u0438 \u0441\u0443\u0440\u044a\u0430\u0442\u0438 \u0442\u0430\u0493\u0438\u0440\u0451\u0431\u0438\u0438 \u0444\u0443\u043d\u043a\u0441\u0438\u044f \u0438\u0441\u0442\u0438\u0444\u043e\u0434\u0430 \u043c\u0435\u0448\u0430\u0432\u0430\u0434",
    "\u0442\u0430\u0493\u0438\u0440\u0451\u0431\u0430\u043d\u0434\u0430\u0438 \u043c\u0443\u0441\u0442\u0430\u049b\u0438\u043b": "\u041c\u0438\u049b\u0434\u043e\u0440\u0435, \u043a\u0438 \u0434\u0430\u0440 \u0444\u0443\u043d\u043a\u0441\u0438\u044f \u0451 \u043c\u0443\u043e\u0434\u0438\u043b\u0430 \u0438\u0434\u043e\u0440\u0430 \u043a\u0430\u0440\u0434\u0430 \u043c\u0435\u0448\u0430\u0432\u0430\u0434, \u0430\u043a\u0441\u0430\u0440 \u0432\u0430\u049b\u0442 \u0441\u0430\u0431\u0430\u0431\u0440\u043e \u0434\u0430\u0440 \u0440\u043e\u0431\u0438\u0442\u0430\u0438 \u0441\u0430\u0431\u0430\u0431\u0443 \u043d\u0430\u0442\u0438\u04b7\u0430 \u0438\u0444\u043e\u0434\u0430 \u043c\u0435\u043a\u0443\u043d\u0430\u0434",
    "\u0442\u0430\u0493\u0438\u0440\u0451\u0431\u0430\u043d\u0434\u0430\u0438 \u0432\u043e\u0431\u0430\u0441\u0442\u0430": "\u041d\u0430\u0442\u0438\u04b7\u0430 \u0451 \u043d\u0430\u0442\u0438\u04b7\u0430\u0435, \u043a\u0438 \u0444\u0443\u043d\u043a\u0441\u0438\u044f \u0451 \u043c\u0443\u043d\u043e\u0441\u0438\u0431\u0430\u0442\u0435, \u043a\u0438 \u0434\u0430\u0440 \u0430\u0441\u043e\u0441\u0438 \u0430\u0440\u0437\u0438\u0448\u04b3\u043e\u0438 \u0434\u0438\u0433\u0430\u0440 \u043c\u0438\u049b\u0434\u043e\u0440\u04b3\u043e \u0442\u0430\u0493\u0439\u0438\u0440 \u043c\u0435\u0451\u0431\u0430\u0434",
    "\u0434\u043e\u043c\u0435\u043d": "\u0411\u0430 \u04b3\u0430\u043c\u0430\u0438 \u0430\u0440\u0437\u0438\u0448\u04b3\u043e\u0438 \u0438\u043c\u043a\u043e\u043d\u043f\u0430\u0437\u0438\u0440, \u043a\u0438 \u043c\u0435\u0442\u0430\u0432\u043e\u043d\u0430\u043d\u0434 \u04b3\u0430\u043c\u0447\u0443\u043d \u0432\u0443\u0440\u0443\u0434 \u0431\u0430 \u0444\u0443\u043d\u043a\u0441\u0438\u044f \u0438\u0441\u0442\u0438\u0444\u043e\u0434\u0430 \u0448\u0430\u0432\u0430\u043d\u0434, \u0438\u0448\u043e\u0440\u0430 \u043c\u0435\u043a\u0443\u043d\u0430\u0434",
    "\u0434\u0438\u0430\u043f\u0430\u0437\u043e\u043d": "\u041c\u0430\u04b7\u043c\u04ef\u0438 \u04b3\u0430\u043c\u0430\u0438 \u043d\u0430\u0442\u0438\u04b7\u0430\u04b3\u043e\u0438 \u0438\u043c\u043a\u043e\u043d\u043f\u0430\u0437\u0438\u0440\u0435, \u043a\u0438 \u0444\u0443\u043d\u043a\u0441\u0438\u044f \u043c\u0435\u0442\u0430\u0432\u043e\u043d\u0430\u0434 \u0442\u0430\u0432\u043b\u0438\u0434 \u043a\u0443\u043d\u0430\u0434",
    "\u043c\u0430\u04b3\u0434\u0443\u0434\u0438\u044f\u0442": "\u041a\u043e\u043d\u0441\u0435\u043f\u0441\u0438\u044f\u0435, \u043a\u0438 \u0442\u0430\u0432\u0441\u0438\u0444 \u043c\u0435\u043a\u0443\u043d\u0430\u0434, \u043a\u0438 \u0444\u0443\u043d\u043a\u0441\u0438\u044f \u0431\u0430 \u043a\u0430\u0434\u043e\u043c \u0430\u0440\u0437\u0438\u0448\u04b3\u043e\u0438 \u0431\u0430\u0440\u043e\u043c\u0430\u0434, \u0432\u0430\u049b\u0442\u0435 \u043a\u0438 \u0432\u043e\u0440\u0438\u0434\u043e\u0442 \u0431\u0430 \u0430\u0440\u0437\u0438\u0448\u0438 \u043c\u0443\u0430\u0439\u044f\u043d \u043d\u0430\u0437\u0434\u0438\u043a \u043c\u0435\u0448\u0430\u0432\u0430\u0434",
    "\u04b3\u043e\u0441\u0438\u043b\u0430\u04b3\u043e\u0438": "\u041c\u0430\u0444\u04b3\u0443\u043c\u0435, \u043a\u0438 \u0441\u0443\u0440\u044a\u0430\u0442\u0438 \u0442\u0430\u0493\u0438\u0440\u0451\u0431\u0438\u0438 \u0444\u0430\u0432\u0440\u04e3 \u0451 \u043d\u0438\u0448\u0435\u0431\u0438\u0438 \u0445\u0430\u0442\u0438 \u0442\u0430\u043d\u0433\u0435\u043d\u0441\u0440\u043e \u0434\u0430\u0440 \u043d\u0443\u049b\u0442\u0430\u0438 \u043c\u0443\u0430\u0439\u044f\u043d\u0438 \u0444\u0443\u043d\u043a\u0441\u0438\u044f \u0447\u0435\u043d \u043c\u0435\u043a\u0443\u043d\u0430\u0434",
    "\u04b3\u0438\u0441\u043e\u0431\u0438 \u0438\u043d\u0442\u0435\u0433\u0440\u0430\u043b\u04e3": "\u0422\u0430\u0432\u0441\u0435\u0430\u0438 \u04b3\u0438\u0441\u043e\u0431, \u043a\u0438 \u0434\u0430\u0440\u0451\u0444\u0442\u0438 \u0430\u043d\u0442\u0438\u0434\u0435\u0440\u0438\u0432\u0430\u0442\u0438\u0432\u0438 \u0444\u0443\u043d\u043a\u0441\u0438\u044f\u0435\u0440\u043e \u0434\u0430\u0440 \u0431\u0430\u0440 \u043c\u0435\u0433\u0438\u0440\u0430\u0434, \u043a\u0438 \u04b3\u043e\u0441\u0438\u043b\u0430\u0438 \u043e\u043d \u0434\u043e\u0434\u0430 \u0448\u0443\u0434\u0430\u0430\u0441\u0442",
    "\u0438\u043d\u0442\u0435\u0433\u0440\u0430\u043b\u0438 \u0445\u0430\u0442": "\u0418\u043d\u0442\u0435\u0433\u0440\u0430\u0442\u0441\u0438\u044f\u0438 \u0444\u0443\u043d\u043a\u0441\u0438\u044f \u0434\u0430\u0440 \u0445\u0430\u0442\u0438 \u043a\u0430\u04b7",
    "\u0438\u043d\u0442\u0435\u0433\u0440\u0430\u043b\u0438 \u0440\u04ef\u0438\u0437\u0430\u043c\u0438\u043d\u04e3": "\u04b6\u0430\u043c\u044a\u043e\u0432\u0430\u0440\u0438\u0438 \u0438\u043d\u0442\u0435\u0433\u0440\u0430\u043b\u04b3\u043e\u0438 \u0433\u0443\u043d\u043e\u0433\u0443\u043d \u0431\u0430 \u0438\u043d\u0442\u0435\u0433\u0440\u0430\u0442\u0441\u0438\u044f\u04b3\u043e \u0434\u0430\u0440 \u0440\u04ef\u0438 \u0441\u0430\u0442\u04b3\u04b3\u043e",
    "\u04b3\u0438\u0441\u043e\u0431\u04b3\u043e\u0438 \u0431\u0438\u0441\u0451\u0440\u0442\u0430\u0493\u0439\u0438\u0440\u0451\u0431\u0430\u043d\u0434\u0430": "\u0422\u0430\u0432\u0441\u0435\u0430\u0438 \u04b3\u0438\u0441\u043e\u0431 \u0434\u0430\u0440 \u044f\u043a \u0442\u0430\u0493\u0438\u0440\u0451\u0431\u0430\u043d\u0434\u0430 \u0431\u0430 \u04b3\u0438\u0441\u043e\u0431 \u0431\u043e \u0444\u0443\u043d\u043a\u0441\u0438\u044f\u04b3\u043e\u0438 \u044f\u043a\u0447\u0430\u043d\u0434 \u0442\u0430\u0493\u0438\u0440\u0451\u0431\u0430\u043d\u0434\u0430",
    "\u043c\u0430\u0439\u0434\u043e\u043d\u0438 \u0432\u0435\u043a\u0442\u043e\u0440\u04e3": "\u041d\u0443\u049b\u0442\u0430\u0435 \u0434\u0430\u0440 \u0444\u0430\u0437\u043e, \u043a\u0438 \u0431\u0443\u0437\u0443\u0440\u0433\u04e3 \u0432\u0430 \u0441\u0430\u043c\u0442 \u0434\u043e\u0440\u0430\u0434",
    "\u04b3\u0438\u0441\u043e\u0431\u04b3\u043e\u0438 \u0432\u0435\u043a\u0442\u043e\u0440\u04e3": "\u0422\u0430\u0432\u0441\u0435\u0430\u0438 \u04b3\u0438\u0441\u043e\u0431\u04b3\u043e, \u043a\u0438 \u0431\u043e \u0444\u0430\u0440\u049b\u0438\u044f\u0442 \u0432\u0430 \u0438\u043d\u0442\u0435\u0433\u0440\u0430\u0442\u0441\u0438\u044f\u0438 \u043c\u0430\u0439\u0434\u043e\u043d\u0438 \u0432\u0435\u043a\u0442\u043e\u0440\u04e3 \u0434\u0430\u0440 \u0444\u0430\u0437\u043e\u0438 \u0441\u0435\u0447\u0435\u043d\u0430\u043a\u0430\u0438 \u0415\u0432\u043a\u043b\u0438\u0434\u04e3 \u0441\u0430\u0440\u0443 \u043a\u043e\u0440 \u0434\u043e\u0440\u0430\u0434.",
    "\u043f\u043e\u0442\u0435\u043d\u0441\u0438\u0430\u043b\u0438 \u0432\u0435\u043a\u0442\u043e\u0440\u04e3": "\u041c\u0430\u0439\u0434\u043e\u043d\u0438 \u0432\u0435\u043a\u0442\u043e\u0440\u0438\u0435, \u043a\u0438 \u0442\u0430\u0432\u0430\u0441\u0441\u0443\u0442\u0438 \u043e\u043f\u0435\u0440\u0430\u0442\u043e\u0440\u0438 curl \u043c\u0430\u0439\u0434\u043e\u043d\u0438 \u0441\u043e\u043b\u0435\u043d\u043e\u0438\u0434\u0430\u043b\u0438\u0440\u043e \u0431\u0430 \u0432\u0443\u04b7\u0443\u0434 \u043c\u0435\u043e\u0440\u0430\u0434"
}