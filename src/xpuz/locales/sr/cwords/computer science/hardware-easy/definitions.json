{
    "\u041f\u0440\u043e\u0446\u0435\u0441\u043e\u0440\u0438": "\u041c\u043e\u0437\u0430\u043a \u0440\u0430\u0447\u0443\u043d\u0430\u0440\u0430, \u0438\u0437\u0432\u0440\u0448\u0430\u0432\u0430 \u0438\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0438\u0458\u0435",
    "\u0420\u0410\u041c \u043c\u0435\u043c\u043e\u0440\u0438\u0458\u0430": "\u041d\u0435\u0441\u0442\u0430\u043b\u043d\u0430 \u043c\u0435\u043c\u043e\u0440\u0438\u0458\u0430 \u0443 \u043a\u043e\u0458\u043e\u0458 \u0441\u0435 \u0447\u0443\u0432\u0430\u0458\u0443 \u043f\u043e\u043a\u0440\u0435\u043d\u0443\u0442\u0438 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u0438 \u0438 \u043f\u043e\u0434\u0430\u0446\u0438",
    "\u0425\u0430\u0440\u0434 \u0434\u0438\u0441\u043a": "\u041d\u0435\u0438\u0441\u043f\u0430\u0440\u0459\u0438\u0432\u0438 \u0443\u0440\u0435\u0452\u0430\u0458 \u0437\u0430 \u0441\u043a\u043b\u0430\u0434\u0438\u0448\u0442\u0435\u045a\u0435 \u0432\u0435\u043b\u0438\u043a\u043e\u0433 \u043a\u0430\u043f\u0430\u0446\u0438\u0442\u0435\u0442\u0430 \u043a\u043e\u0458\u0438 \u043a\u043e\u0440\u0438\u0441\u0442\u0438 \u043c\u0430\u0433\u043d\u0435\u0442\u043d\u0435 \u043f\u043b\u043e\u0447\u0435",
    "\u0421\u0421\u0414 \u0443\u0440\u0435\u0452\u0430\u0458": "\u041d\u0435\u0441\u0442\u0430\u043b\u043d\u043e \u0441\u043a\u043b\u0430\u0434\u0438\u0448\u0442\u0435\u045a\u0435 \u043f\u043e\u043c\u043e\u045b\u0443 \u0444\u043b\u0435\u0448 \u043c\u0435\u043c\u043e\u0440\u0438\u0458\u0435, \u0431\u0440\u0436\u0435 \u043e\u0434 \u0425\u0414\u0414-\u0430",
    "\u0431\u0443\u0441": "\u041f\u0443\u0442\u0430\u045a\u0430 \u0437\u0430 \u043f\u043e\u0434\u0430\u0442\u043a\u0435 \u0434\u0430 \u043f\u0443\u0442\u0443\u0458\u0443 \u0438\u0437\u043c\u0435\u0452\u0443 \u043a\u043e\u043c\u043f\u043e\u043d\u0435\u043d\u0442\u0438",
    "\u0443\u043b\u0430\u0437\u043d\u0438 \u0443\u0440\u0435\u0452\u0430\u0458": "\u041e\u043c\u043e\u0433\u0443\u045b\u0430\u0432\u0430 \u0443\u043d\u043e\u0441 \u043f\u043e\u0434\u0430\u0442\u0430\u043a\u0430 \u0443 \u0440\u0430\u0447\u0443\u043d\u0430\u0440 (\u043f\u043e\u043f\u0443\u0442 \u0442\u0430\u0441\u0442\u0430\u0442\u0443\u0440\u0435, \u043c\u0438\u0448\u0430)",
    "\u0438\u0437\u043b\u0430\u0437\u043d\u0438 \u0443\u0440\u0435\u0452\u0430\u0458": "\u041f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u0459\u0430 \u0440\u0435\u0437\u0443\u043b\u0442\u0430\u0442\u0435 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u043a\u0443 (\u043a\u0430\u043e \u043c\u043e\u043d\u0438\u0442\u043e\u0440, \u0448\u0442\u0430\u043c\u043f\u0430\u0447)",
    "\u043c\u0430\u0442\u0438\u0446\u043d\u0430 \u043f\u043b\u043e\u0446\u0430": "\u0413\u043b\u0430\u0432\u043d\u0430 \u043f\u043b\u043e\u0447\u0430 \u043a\u043e\u0458\u0430 \u043f\u043e\u0432\u0435\u0437\u0443\u0458\u0435 \u0441\u0432\u0435 \u043a\u043e\u043c\u043f\u043e\u043d\u0435\u043d\u0442\u0435 \u0440\u0430\u0447\u0443\u043d\u0430\u0440\u0430",
    "\u0440\u0430\u0447\u0443\u043d\u0430\u0440\u0441\u043a\u0430 \u043c\u0440\u0435\u0436\u0430": "\u041a\u043e\u043b\u0435\u043a\u0446\u0438\u0458\u0430 \u043c\u0435\u0452\u0443\u0441\u043e\u0431\u043d\u043e \u043f\u043e\u0432\u0435\u0437\u0430\u043d\u0438\u0445 \u0443\u0440\u0435\u0452\u0430\u0458\u0430 \u043a\u043e\u0458\u0438 \u043c\u043e\u0433\u0443 \u0434\u0430 \u0434\u0435\u043b\u0435 \u0440\u0435\u0441\u0443\u0440\u0441\u0435",
    "\u0418\u041f \u0430\u0434\u0440\u0435\u0441\u0430": "\u0408\u0435\u0434\u0438\u043d\u0441\u0442\u0432\u0435\u043d\u0438 \u043d\u0443\u043c\u0435\u0440\u0438\u0447\u043a\u0438 \u0438\u0434\u0435\u043d\u0442\u0438\u0444\u0438\u043a\u0430\u0442\u043e\u0440 \u0434\u043e\u0434\u0435\u0459\u0435\u043d \u0441\u0432\u0430\u043a\u043e\u043c \u0443\u0440\u0435\u0452\u0430\u0458\u0443 \u043d\u0430 \u043c\u0440\u0435\u0436\u0438",
    "\u043f\u0440\u043e\u0442\u043e\u043a": "\u041c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u043d\u0430 \u0431\u0440\u0437\u0438\u043d\u0430 \u043f\u0440\u0435\u043d\u043e\u0441\u0430 \u043f\u043e\u0434\u0430\u0442\u0430\u043a\u0430 \u043f\u0440\u0435\u043a\u043e \u043c\u0440\u0435\u0436\u043d\u0435 \u043f\u0443\u0442\u0430\u045a\u0435",
    "\u043b\u0430\u0442\u0435\u043d\u0442\u043d\u043e\u0441\u0442": "\u0412\u0440\u0435\u043c\u0435\u043d\u0441\u043a\u043e \u043a\u0430\u0448\u045a\u0435\u045a\u0435 \u0434\u0430 \u043f\u043e\u0434\u0430\u0446\u0438 \u043f\u0443\u0442\u0443\u0458\u0443 \u043e\u0434 \u0438\u0437\u0432\u043e\u0440\u0430 \u0434\u043e \u043e\u0434\u0440\u0435\u0434\u0438\u0448\u0442\u0430"
}